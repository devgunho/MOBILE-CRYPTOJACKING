		try{
			var palDomain = new PalDomainStorage("https://get3.adobe.com", "/util/pal/read/");
			palDomain.requestValue(function(value){
				if(value !== undefined && value.length > 0){
					$("#offer").data("addons").setPalList(value);
				}
				pbDisplayOffer();
			});
		}catch(e){
			pbDisplayOffer();
		}
	});
