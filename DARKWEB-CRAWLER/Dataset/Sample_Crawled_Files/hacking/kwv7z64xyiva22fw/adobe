<script>
// **************************************************
// Mainland China  
// **************************************************
if (location.host == "helpx.dev.adobe.com" || location.host == "helpx.stage.adobe.com" || location.host == "helpx.qa.adobe.com") {
	//console.log("Show Chinese Disclaimer");
}

if (typeof $ == 'function') {
	$.ajax({
		type: "GET",
		dataType: 'jsonp',
		jsonpCallback: 'displayChineseDisclaimer',
		url: 'https://geo2.adobe.com/json/',
		success: function(data) {
		  if (location.host == "helpx.dev.adobe.com" || location.host == "helpx.stage.adobe.com" || location.host == "helpx.qa.adobe.com") {
			//console.log("Country code: " + data.country);
		  }
		  if(data.country == 'CN' || location.search.indexOf('showAlert') > 0) {
			showChineseDisclaimer();
		  }
		}
	});
}


function showChineseDisclaimer(){
    var products = [
        '/anywhere/',
        '/creative-cloud/',
        '/experience-design/',
        '/fuse/',
        '/gaming-sdk/',
        '/mobile-apps/',
        '/muse/',
        '/stock/',
        '/story/',
        '/typekit/',
        '/x-productkb/'
    ];
    var ChineseDisclaimer = 
        '<div class="chinese-disclaimer">' +
        '<span class="warning-icon"><img src="/shared/article-3/alert.svg"/></span>' +
        '<p>å¨ä¸­å½è´­ä¹°ç Creative Cloud äº§åä¸å« CC æ¡é¢åºç¨ç¨åºä¸­ç Creative Cloud ç§»å¨åºç¨ç¨åºåå¨çº¿æå¡åç¸å³åè½ã</p>'+
        '</div>';
        
    for (i=0; i< products.length; i++) {
        if ( location.pathname.indexOf(products[i]) > -1 ) {    
            $('#sidebar').append(ChineseDisclaimer);
        }
    }
}

 
