<!-- Ads refresh -->
<script type="text/javascript">
jQuery(document).ready(function(){
	try {
		function adRefresh() {
			console.log('Refreshing google ads....');
			if (typeof(googletag) !== 'undefined' && googletag) {
				googletag.pubads().refresh();
			}
		}
		function attachAdRefreshEvent() {
			var AD_REFRESH_INTERVAL_SEC = 30;
			var adRefreshInterval = setInterval(adRefresh, AD_REFRESH_INTERVAL_SEC * 1000);
		}
		attachAdRefreshEvent();
	} catch(e) {
		console.log('silently catching error on ads refresh');
	}
});
</script>


