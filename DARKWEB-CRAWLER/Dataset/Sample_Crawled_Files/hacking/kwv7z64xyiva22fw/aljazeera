      var jQueryScriptOutputted = false;
      var jQueryGalleryScriptOutputted = false;
      var galleryViewCalled = false;

      function initJQuery() {

          if (typeof (jQuery) === 'undefined') {

              if (!jQueryScriptOutputted) {
                  //only output the script once..
                  jQueryScriptOutputted = true;

                  var script = document.createElement("script");
                  script.type = "text/javascript";
                  script.src = "http://code.jquery.com/jquery-latest.min.js";
                  document.body.appendChild(script);

                  //output the script (load it from jquery cdn)                             
                  //document.write(unescape("%3Cscript src='http://code.jquery.com/jquery-latest.min.js' type='text/javascript'%3EÂ %3C/script%3E"));
              }

              setTimeout("initJQuery()", 50);
          } else {

              if (typeof ($.fn.owlCarousel) === 'undefined') {
                  if (!jQueryGalleryScriptOutputted) {
                      jQueryGalleryScriptOutputted = true;

                      var link = document.createElement("link");
                      link.type = "text/css";
                      link.rel = "stylesheet";
                      link.href = "/assets/styles/owl-carousal.css";
                      document.body.appendChild(link);

                      var script = document.createElement("script");
                      script.type = "text/javascript";
                      script.src = "/mritems/custom/plugins/owl-carousel/owl.carousel.min.js";
                      document.body.appendChild(script);
                  }

                  setTimeout("initJQuery()", 50);
              } else {
                  if (!galleryViewCalled) {
                      galleryViewCalled = true;

    /* $("div.opinion-wrap div.row-fluid.opinion-flex").owlCarousel({
