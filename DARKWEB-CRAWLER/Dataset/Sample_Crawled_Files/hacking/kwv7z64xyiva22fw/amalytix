    	// NB: using Open Exchange Rates here, but you can use any source!
        '/fxrates.json',
        function(data) {
            // Check money.js has finished loading:
            if ( typeof fx !== "undefined" && fx.rates ) {
                fx.rates = data.rates;
                fx.base = data.base;
            } else {
                // If not, apply to fxSetup global:
                var fxSetup = {
                    rates : data.rates,
                    base : data.base
                }
            }
        }
    );
</script>

<script>
window.amChartsReadyFlag = false;
AmCharts.ready(function() {
    window.amChartsReadyFlag = true;
});
</script>

</body>

</html>



#####EOF#####


