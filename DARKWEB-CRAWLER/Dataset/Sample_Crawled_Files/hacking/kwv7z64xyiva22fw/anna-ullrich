        <script>
        $(document).ready(function () {
		
			var menu = $('.navbar');
			var origOffsetY = menu.offset().top;
		
			function scroll() {
				if ($(window).scrollTop() >= origOffsetY) {
					$('.navbar').css('top', '0');
					$('.navbar').css('position', 'fixed');
				} else {
					$('.navbar').css('top', '60%');
					$('.navbar').css('position', 'absolute');
				}
		
		
			}
		
			document.onscroll = scroll;
		
		});
        </script>
