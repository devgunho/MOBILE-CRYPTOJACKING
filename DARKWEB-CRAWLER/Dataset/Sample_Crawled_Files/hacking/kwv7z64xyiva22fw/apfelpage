<script type="text/javascript">
            var token = "";
            var id = "";
            window.onload = function() {
            	
            	if ('safari' in window && 'pushNotification' in window.safari) {
                    checkPerms();
                }
            };

            function checkPerms() {
                

                var pResult = window.safari.pushNotification.permission('web.de.2peaches.apfelpage');
                if (pResult.permission === 'default') {
                    requestPermissions();
                }
                else if (pResult.permission === 'granted') {
                    token = pResult.deviceToken;
                }
                else if (pResult.permission === 'denied') {
                }
            }

            function requestPermissions() {
                
                window.safari.pushNotification.requestPermission('https://push.apfelpage.de', 'web.de.2peaches.apfelpage', {"id": "1"}, function(c) {
                	
                    if (c.permission === 'granted') {
                        token = c.deviceToken;
                    }
                    else if (c.permission === 'denied') {
                    }
                });
            }
</script>
