function equalHeightsByClassName(classTarget){
  if($(classTarget).length>0){
    $(classTarget).height('auto');
    if(SB.media != 'xs') $(classTarget).equalHeights();
  }
}

function resizeAllLayouts(){
  if(SB.media == 'xs' && $("body.sb-tpl-group-reservations").length==0){
    /*var mt = $(".sb-menu-responsive-top").height()+$(".mean-bar").height()+10;
    $(".sb-contents").css('margin-top', mt+'px');*/
    $(".sb-contents").css('margin-top','108px');
  } else {$(".sb-contents").css('margin-top','0px');}
  /*if($(".menuHolder .sb-hotels-menu > ul > li").length>0) equalHeightsByRowAndClassName(".menuHolder .sb-hotels-menu > ul > li",'5px');*/
    

  if($(".offers-container .cell-offer .sb-package").length>0){
    $(".offers-container .cell-offer .sb-package").height('auto');
    $(".offers-container .cell-offer .sb-package").equalHeightsInset();
  }


  if($(".container.offers .cell-offer").length>0 && $(".offers-slick .cell-offer").length==0) equalHeightsByRowAndClassName(".container.offers .cell-offer");
  if($(".sb-service-banner").length>0 ) equalHeightsByRowAndClassName(".sb-service-banner");
  if($(".top-stories-pkg").length>0 ) equalHeightsByRowAndClassName(".top-stories-pkg");

  

  equalHeightsModuleB();
  fixMenuItemsWidth();
  setTimeout(function(){
    fixSuperBGsize();

    if($(document).height()==$(window).height()){
      $('.sb-arrow-up-down .fa-angle-up').hide();
      $('.sb-arrow-up-down .fa-angle-down').hide();
    }

  }, 5);

}



function equalHeightsModuleB(){
  if($(".top-module-b").length>0){
    var h =  $(".sb-pkg-1").height();
    if(h>100){
      $(".sb-offers-home").height(h);
      $(".sb-offers-envelope").height(h);
    } else {
      $(".sb-offers-home").css('max-height','254px');
      $(".sb-offers-envelope").css('max-height','254px');
      $(".sb-offers-home").height('auto');
      $(".sb-offers-envelope").height('auto');
    }
  }
}

function equalHeightsByRowAndClassName(){
  /* sÃ³lo vÃ¡lido para celdas del mismo ancho */
  var classTarget = arguments[0];
  var marginBotton = (arguments[1])?arguments[1]:'30px';
  var menuChildren = $(classTarget).length;
  if(menuChildren > 0){
    // devLog('equalHeightsByRowAndClassName -> '+classTarget+", "+menuChildren);
    var fixedWidth = $(classTarget).eq(0).width();
    var containerParentWidth = $(classTarget).eq(0).parent().width();
    var numberOfCols = Math.floor(containerParentWidth/fixedWidth);
    var numberOfRows = Math.ceil(menuChildren / numberOfCols);
   /* var mb = (numberOfRows > 1)?marginBotton:'0px';*/
    $(classTarget).css({'height':'auto', 'margin-bottom':marginBotton});
    for (i = 0; i <= numberOfRows - 1; i ++) {
      firstItem = i * numberOfCols;
      lastItem = firstItem + numberOfCols;
      // devLog("slice "+classTarget+" -> "+firstItem+", "+lastItem);
          $(classTarget).slice(firstItem,lastItem).equalHeights();
      };
  }
}

var currentTag;
$(window).scroll( function(){
  if(scr_to) clearTimeout(scr_to);
  scr_to = setTimeout(checkScrollPos,50);
});


var f_top = 0;
function check_pos(){
  var f_top = $('.sb-footer-bottom').offset().top;
  /*var min_height = $(window).outerHeight()-($('body > div.sb-header.container-fluid').height()+$('#sb-bottom > div.container-fluid.sb-background-header').outerHeight()+$('.sb-footer-bottom').outerHeight()+24);

  if ($('body').not('.sb-spbg').find('.sb-container-content').height() < min_height){
    $('body').not('.sb-spbg').not('.sb-tpl-index').find('.sb-arrow-up-down').hide();
    $('body').not('.sb-spbg').not('.sb-tpl-index').find('.sb-footer-bottom').css('padding-bottom','0');
    $('body').not('.sb-spbg').not('.sb-tpl-index').find('.sb-container-content').css('min-height',min_height);
  }

  f_top = $('.sb-footer-bottom').offset().top;
  f_con = $('.sb-container').offset().top;
  d_top = $('#sb-top').offset().top;
  // Sets the current scroll position
  var st = $(this).scrollTop();
  // Determines up-or-down scrolling
  if (f_top-st < $(window).height()-40){
    $('.sb-footer-mini').slideUp('300');
    $('.sb-arrow-up-down').css('bottom','10px');
    $('.media-xs .sb-arrow-up-down').css('bottom','50px');
    $('.sb-arrow-up-down .fa-angle-up').show();
  }
  if (f_top-st >= $(window).height()-40){
    $('.sb-footer-mini').slideDown('300');
    $('.sb-arrow-up-down').css('bottom','250px');
    $('.sb-tpl-index .sb-arrow-up-down').css('bottom','180px');
    $('.media-xs .sb-arrow-up-down').css('bottom','50px');
    $('.sb-arrow-up-down .fa-angle-up').hide();
  }

  if (f_con-st < $(window).height()/2){
    $('.sb-spbg .sb-header').removeClass('sb-big');
  } else {
    $('.sb-spbg .sb-header').addClass('sb-big');
  }

  if (d_top-st < -70){
    $('.sb-arrow-up-down .fa-angle-down').hide();
    // $('.sb-arrow-up-down .fa-angle-up').show();
  } else {
    // $('.sb-arrow-up-down .fa-angle-up').hide();
    $('.sb-spbg .sb-arrow-up-down .fa-angle-down, .sb-tpl-location .sb-arrow-up-down .fa-angle-down').show();
  }
  $('.computer #superbgimage, .computer .sb-map-canvas').css('top',st/2);*/
}






