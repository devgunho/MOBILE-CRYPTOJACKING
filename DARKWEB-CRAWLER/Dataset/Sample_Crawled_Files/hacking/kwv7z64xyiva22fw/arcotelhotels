function scrollDownTo(){
  var h = $(window).height()-10;
  $.scrollTo('+='+h+'px', 800);
}

function checkScrollPos(){
  var st = $(window).scrollTop();
  /*if(SB.media == 'md' || SB.media == 'lg'){*/
  if(SB.media != 'xs'){
    if(st > 66){switchBodyClass(true);} else {switchBodyClass(false);}
  } else {switchBodyClass(false);}
  try{
    var f_top = $('.sb-footer-bottom').offset().top;
   /* var f_con = $('.sb-contents').offset().top;
    var d_top = $('#sb-top').offset().top;*/
    var st = $(this).scrollTop();
    if (f_top-st < $(window).height()-40){
        $('.sb-arrow-up-down .fa-angle-up').show();
        $('.sb-arrow-up-down .fa-angle-down').hide();
    }

    if (f_top-st >= $(window).height()-40){
          
        $('.sb-arrow-up-down .fa-angle-up').hide();
    }


  } catch(e){
      // do nothing
  }

 /*var min_height = $(window).outerHeight()-($('body > div.sb-header.container-fluid').height()+$('#sb-bottom > div.container-fluid.sb-background-header').outerHeight()+$('.sb-footer-bottom').outerHeight()+24);

  if ($('body').not('.sb-spbg').find('.sb-container-content').height() < min_height){
    $('body').not('.sb-spbg').not('.sb-tpl-index').find('.sb-arrow-up-down').hide();
    $('body').not('.sb-spbg').not('.sb-tpl-index').find('.sb-footer-bottom').css('padding-bottom','0');
    $('body').not('.sb-spbg').not('.sb-tpl-index').find('.sb-container-content').css('min-height',min_height);
  }

  f_top = $('.sb-footer-bottom').offset().top;
  f_con = $('.sb-container').offset().top;
  d_top = $('#sb-top').offset().top;
  // Sets the current scroll position
  var st = $(this).scrollTop();
  // Determines up-or-down scrolling
  if (f_top-st < $(window).height()-40){
    $('.sb-footer-mini').slideUp('300');
    $('.sb-arrow-up-down').css('bottom','10px');
    $('.media-xs .sb-arrow-up-down').css('bottom','50px');
    $('.sb-arrow-up-down .fa-angle-up').show();
  }
  if (f_top-st >= $(window).height()-40){
    $('.sb-footer-mini').slideDown('300');
    $('.sb-arrow-up-down').css('bottom','250px');
    $('.sb-tpl-index .sb-arrow-up-down').css('bottom','180px');
    $('.media-xs .sb-arrow-up-down').css('bottom','50px');
    $('.sb-arrow-up-down .fa-angle-up').hide();
  }

  if (f_con-st < $(window).height()/2){
    $('.sb-spbg .sb-header').removeClass('sb-big');
  } else {
    $('.sb-spbg .sb-header').addClass('sb-big');
  }

  if (d_top-st < -70){
    $('.sb-arrow-up-down .fa-angle-down').hide();
    // $('.sb-arrow-up-down .fa-angle-up').show();
  } else {
    // $('.sb-arrow-up-down .fa-angle-up').hide();
    $('.sb-spbg .sb-arrow-up-down .fa-angle-down, .sb-tpl-location .sb-arrow-up-down .fa-angle-down').show();
  }
  $('.computer #superbgimage, .computer .sb-map-canvas').css('top',st/2);*/











}

function switchBodyClass(valor){
  if(!$(".sb-logo ul.dropdown-menu").is(":visible")) {
    if(valor){
      if($(".arcotel-booking-box.sb-vertical").length>0){
        var bottom_of_object = $(".arcotel-booking-box.sb-vertical").offset().top + $(".arcotel-booking-box.sb-vertical").outerHeight();
        var scroll_of_window = $(window).scrollTop();
        //devLog("bottom_of_object: "+bottom_of_object+", bottom_of_window: "+scroll_of_window);
        if(scroll_of_window > bottom_of_object-55){
          if(!$('.sb-header-booking').hasClass("sb-ready")) {$('.sb-header-booking').addClass("sb-ready");}
        } else {
          if($('.sb-header-booking').hasClass("sb-ready")){$('.sb-header-booking').removeClass("sb-ready");}
        }
      } else {
        if(!$('.sb-header-booking').hasClass("sb-ready")) {$('.sb-header-booking').addClass("sb-ready");}
      }
      if(!$('body').hasClass("compact-menu")) {$('body').addClass("compact-menu");}
    } else {

      if($('body').hasClass("compact-menu")){$('body').removeClass("compact-menu");} 
    }
  }
}

function MM_openBrWindow(a,h,n){window.open(a,h,n)}

function moveHomeContainer(){
/*
  setTimeout(function() {
    $(".media-xs .sb-container-home").animate({"bottom": 40-$(".media-xs .sb-container-home").height()}, "slow");
    $(".media-sm .sb-container-home").animate({"bottom": 40-$(".media-sm .sb-container-home").height()}, "slow");
    $(".media-md .sb-container-home").animate({"bottom": 0}, "slow");
    $(".media-lg .sb-container-home").animate({"bottom": 0}, "slow");
  }, 400);
*/

  if(!useNewPinterest){

    $('.media-xs .sb-packages.sb-boxes').pinterest_grid({
      no_columns: 1,
      padding_x: 15,
      padding_y: 15,
      margin_bottom: 50,
      single_column_breakpoint: 700
    });
    $('.media-sm .sb-packages.sb-boxes').pinterest_grid({
      no_columns: 2,
      padding_x: 15,
      padding_y: 15,
      margin_bottom: 50,
      single_column_breakpoint: 700
    });
    $('.media-md .sb-packages.sb-boxes').pinterest_grid({
      no_columns: 3,
      padding_x: 15,
      padding_y: 15,
      margin_bottom: 50,
      single_column_breakpoint: 700
    });
    $('.media-lg .sb-packages.sb-boxes').pinterest_grid({
      no_columns: 3,
      padding_x: 15,
      padding_y: 15,
      margin_bottom: 50,
      single_column_breakpoint: 700
    });
    $('.media-xl .sb-packages.sb-boxes').pinterest_grid({
      no_columns: 3,
      padding_x: 15,
      padding_y: 15,
      margin_bottom: 50,
      single_column_breakpoint: 700
    });
  }

  setTimeout(reArrangeFooter,50);

}
$('html').on("mediaXss", function(){moveHomeContainer();});
$('html').on("mediaXs", function(){moveHomeContainer();});
$('html').on("mediaSm", function(){moveHomeContainer();});
$('html').on("mediaMd", function(){moveHomeContainer();});
$('html').on("mediaLg", function(){moveHomeContainer();});
$('html').on("mediaXl", function(){moveHomeContainer();});

function reArrangeFooter(){
  if(SB.media == 'xs'){
    $('.info-hotel-cell').insertAfter('.newsletter-col');
    $('.bottom-menu').insertBefore('.bottom-copyright');
    $('.bottom-menu').removeClass("text-right");
    $('.footer-links-list > li > ul').hide();
    $(".head-footer .fa-caret-up").parent().next().show();
  } else {
    $('.info-hotel-cell').insertBefore('.social-media-col');
    $('.bottom-menu').insertAfter('.bottom-copyright');
    $('.bottom-menu').addClass("text-right");
    $('.footer-links-list > li > ul').show();
  }
}


//EQUAL HEIGHTS OF OFFERS ON THE OFFERS-BOX PAGE
$.fn.extend({
  equalHeights: function(options){
    var ah=(Math.max.apply(null, $(this).map(function(){ return $(this).outerHeight(); }).get()));

    if (typeof ah=='number') $(this).height(ah);
  }
});
$.fn.extend({
  equalHeightsInset: function(options){
    var ah=(Math.max.apply(null, $(this).map(function(){ return $(this).height(); }).get()));
    //var ah=(Math.max.apply(null, $(this).map(function(){ return $(this).outerHeight(); }).get()));

    if (typeof ah=='number') $(this).height(ah);
  }
});

