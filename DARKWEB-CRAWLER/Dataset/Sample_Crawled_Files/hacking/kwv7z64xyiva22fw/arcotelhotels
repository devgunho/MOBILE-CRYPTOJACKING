            $('.sb-booking select.sb-room option').not('.sb-booking select.sb-room option.' + selectedDestination + '').not('.sb-booking select.sb-room option:first-child').remove();
            $('.sb-booking select.sb-room option:first-child').prop('selected', true);
          //};
        };
      });
    };

    $('.sb-booking select.sb-room').change(function(){
      var selectedRoomClass = $(this).find("option:selected").prop('class');
      var selectedRoomValue = $(this).find("option:selected").prop('value');
      $('.sb-booking select.sb-room option[value="'+selectedRoomValue+'"].'+selectedRoomClass+'').prop('selected', true);
    });
