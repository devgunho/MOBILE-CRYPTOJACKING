});

$(".search-responsive-top > .fa-search").click(function (e){
  if ($(".responsive-search-box").is(':visible')) {$(".responsive-search-box").hide();} else {
    $(".responsive-search-box").show();
    $(".responsive-search-box .search-input input").focus();
  }
  resizeAllLayouts();
});




