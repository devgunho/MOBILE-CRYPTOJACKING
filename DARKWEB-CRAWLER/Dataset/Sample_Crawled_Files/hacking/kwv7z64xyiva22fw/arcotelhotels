var menuClone;
$(document).ready(function() {
		$(".sb-logo-resp").append($('.sb-logo').clone().html());
		$(".sb-full-menu").html('<ul></ul>');
		var lang_str = '<li class="taphover"><a>'+$(".sb-cell-languages > .btn-group > button").html()+'</a>';
		lang_str +='<ul>'+$(".sb-cell-languages > .btn-group > .sb-languages.dropdown-menu").html()+'</ul></li>';
		$(".sb-full-menu > ul").append(lang_str);
		menuClone = $(".sb-menu > ul").clone();
				$(".sb-full-menu > ul").append(menuClone.html());
		$(".sb-cell-header.top-menu-item > .btn-group").each(function(){ 
			var item_str = '';
			if($(this).find("li").length>0){
				item_str = '<li class="taphover"><a>'+$(this).find(".btn-link").html()+'</a>';
				item_str +='<ul>'+$(this).find("ul.dropdown-menu").html()+'</ul>';
				item_str += '</li>';
			} else {
				item_str = '<li>'+$(this).find(".btn-link").html()+'</li>';
			}
			$(".sb-full-menu > ul").append(item_str);
		});
		/*$(".sb-full-menu > ul").append($(".sb-menu-sidebar > .container-sidebar > ul").html());*/

	if ($('.sb-menu').length > 0) {
		$('.sb-header').append('<div class="sb-menu-responsive visible-xs "></div>');
		/*$('.sb-menu-responsive').prepend('<div class="sb-logo"><a href="/de/" title="ARCOTEL Hotels"><img class="img-responsive" src="/files/img/arcotel-hotels.png" height="60" alt="ARCOTEL Hotels" /></a></div>');*/
		$('.sb-full-menu').meanmenu({meanMenuOpen: '<span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>', meanMenuClose: 'Ã', meanScreenWidth: '767', meanMenuContainer: '.sb-menu-responsive'});
		//$('.sb-menu-responsive').prepend('<div class="sb-menu-responsive-top"><div class="sb-logo-resp"><a href="/de/" title="ARCOTEL Hotels"><img class="img-responsive" src="/files/img/arcotel-hotels.png" height="60" alt="ARCOTEL Hotels" /></a></div></div>');
		$(".sb-menu-responsive li.sb-languages ul.sb-languages").removeClass("dropdown-menu");
	}
	$(".sb-menu-responsive .sb-destinations").remove();
});
</script>
