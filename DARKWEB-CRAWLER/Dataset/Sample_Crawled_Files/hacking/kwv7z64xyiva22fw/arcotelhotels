			if(SB.media == 'xs'){
				var cThis = $(this);
				if($(this).next().is(":visible")){
					cThis.next().slideUp('slow', function(){cThis.find("i.fa").removeClass("fa-caret-up").addClass("fa-caret-down");});
				} else {
					if($(".footer-links-list > li > ul:visible").length>0){
						$(".footer-links-list > li > ul:visible").slideUp('slow', function(){
							$(".head-footer i.fa").removeClass("fa-caret-up").addClass("fa-caret-down");
							cThis.next().slideDown('slow', function(){cThis.find("i.fa").addClass("fa-caret-up").removeClass("fa-caret-down");});
						});
					} else {
						cThis.next().slideDown('slow', function(){cThis.find("i.fa").addClass("fa-caret-up").removeClass("fa-caret-down");});
					}
				}
			}
		});

	});
