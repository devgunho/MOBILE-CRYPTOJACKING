  if ($('.sb-booking').length){

    var roomSelection = $('.sb-booking').first().find('select.sb-room option');

    try{

      if ($('.sb-booking select.sb-destination').length){
        selectedDestination = $('.sb-booking select.sb-destination').val();
        if (selectedDestination.match(/^(c_)/) || selectedDestination == ""){
          $('.sb-booking select.sb-room').prop('disabled', 'disabled');
        } else {
          $('.sb-booking select.sb-room').prop('disabled', false);
        };
        if(selectedDestination != ""){
          roomSelection.appendTo('.sb-booking select.sb-room');
          //if ($('.sb-booking select.sb-room option').hasClass(selectedDestination)){
            $('.sb-booking select.sb-room option').not('.sb-booking select.sb-room option.' + selectedDestination + '').not('.sb-booking select.sb-room option:first-child').remove();
            $('.sb-booking select.sb-room option:first-child').prop('selected', true);
          //};
        };
      };
    } catch (e) {
      // do nothing
    }

    if ($('.sb-booking select.sb-destination').length){
      $('.sb-booking select.sb-destination').change(function(){

        var selectedDestination = $(this).val();
        $('.sb-booking select.sb-room option').remove();
        roomSelection.appendTo('.sb-booking select.sb-room');

        $('.sb-booking select.sb-destination').not('.sb-booking.sb-packages select.sb-destination').val(selectedDestination);
        $('.sb-booking select.sb-room option').not('.sb-booking select.sb-room option:first-child').remove();
        $('.sb-booking select.sb-room').val([]);

        if (selectedDestination.match(/^(c_)/) || selectedDestination == "") {
          $('.sb-booking select.sb-room').prop('disabled', 'disabled');
        } else {
          $('.sb-booking select.sb-room').prop('disabled', false);
        }
        if(selectedDestination != ""){
          roomSelection.appendTo('.sb-booking select.sb-room');
