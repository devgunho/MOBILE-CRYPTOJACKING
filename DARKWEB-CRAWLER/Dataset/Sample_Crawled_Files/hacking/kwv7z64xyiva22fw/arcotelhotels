  });



  $(".sb-logo").on('show.bs.dropdown', function () {
    $(".sb-logo button").addClass("dropup");
    if(traslapoTimer) clearTimeout(traslapoTimer);
      traslapoTimer = setTimeout(function(){$(".sb-traslapo").fadeIn('fast');},100);
  });

  $(".sb-logo").on('hide.bs.dropdown', function () {
    $(".sb-logo button").removeClass("dropup");
