  var traslapoTimer;


  



  $(".taphover").mouseenter(function() {
    if(traslapoTimer) clearTimeout(traslapoTimer);
    traslapoTimer = setTimeout(function(){$(".sb-traslapo").fadeIn('fast');},100);
  });
  $(".taphover").mouseleave(function() {
    if(traslapoTimer) clearTimeout(traslapoTimer);
    traslapoTimer = setTimeout(function(){$(".sb-traslapo").fadeOut('fast');},100);
  });



  $(".sb-logo").on('show.bs.dropdown', function () {
    $(".sb-logo button").addClass("dropup");
    if(traslapoTimer) clearTimeout(traslapoTimer);
      traslapoTimer = setTimeout(function(){$(".sb-traslapo").fadeIn('fast');},100);
  });

  $(".sb-logo").on('hide.bs.dropdown', function () {
    $(".sb-logo button").removeClass("dropup");
      if(traslapoTimer) clearTimeout(traslapoTimer);
      traslapoTimer = setTimeout(function(){$(".sb-traslapo").fadeOut('fast');},100);
  });

  $(".sb-logo-resp").on('show.bs.dropdown', function () {
    $(".responsive-search-box").hide();
    $(".sb-logo-resp button").addClass("dropup");
    $(".meanmenu-reveal.meanclose").click();
    resizeAllLayouts();
  });

  $(".sb-logo-resp").on('hide.bs.dropdown', function () {
    $(".sb-logo-resp button").removeClass("dropup");
  });


  $(".sb-cell-header .btn-group").not('.search-icon-btn-group').on('hide.bs.dropdown', function () {
