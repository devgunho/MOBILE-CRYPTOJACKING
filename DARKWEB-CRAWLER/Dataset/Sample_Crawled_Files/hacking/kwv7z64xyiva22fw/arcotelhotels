if($('body').find(".acc_trigger.openable").length>0){
  $(".acc_trigger.openable").next().hide();
  $('.acc_trigger').click(function(){
      if( $(this).next().is(':hidden') ) { //If immediate next container is closed...
        $(this).next().slideDown();
        $(this).removeClass('openable').addClass('closeable').removeClass('fa-plus-circle').addClass('fa-minus-circle');
      } else {
        $(this).next().slideUp();
        $(this).removeClass('closeable').removeClass('fa-minus-circle').addClass('openable').addClass('fa-plus-circle');
      }
      return false; //Prevent the browser jump to the link anchor
  });
  $(".sb-menu-sitemap .sb-hotels-menu > ul > li > ul > li > ul").remove();
  $(".sb-menu-sitemap .sb-hotels-menu > ul > li > ul > li > i.acc_trigger").remove();
}

$(".sb-menu-sitemap li > a[href='#']").click(function (e){
  e.preventDefault();
  if($(this).next().hasClass("fa")) $(this).next().click();
});

