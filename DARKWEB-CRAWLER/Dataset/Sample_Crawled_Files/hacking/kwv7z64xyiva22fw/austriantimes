var ToC =
  "<div role='navigation' class='table-of-contents'>" +
    "<ul>";

var newLine, el, title, link;

$("section h2").each(function() {

  el = $(this);
  title = el.text();
  link = "#" + el.attr("id");

  newLine =
    "<li>" +
      "<a href='" + link + "'>" +
        title +
      "</a>" +
    "</li>";

  ToC += newLine;

});

ToC +=
   "</ul>" +
  "</div>"+
  "</div>";

$(".inhalt").prepend(ToC);
