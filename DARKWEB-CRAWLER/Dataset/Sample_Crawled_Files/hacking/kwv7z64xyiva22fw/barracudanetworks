	<script>
window.CUI_asset_path = 'https://login.barracudanetworks.com/cui';

// show loading indicator after 100ms (if we're taking too long to load stuff)
window.CUI = {
	// default to a production environment
	env: 'production',

	// supports unlimited arguments in everything but IE8 & IE7
	// only show logs when in development
	log: function(arg1, arg2, arg3) {
		var con = window.console;
		CUI.env == 'development' && con && con['log'] && (con['log'].apply ?
			con['log'].apply(con, arguments) :
				con.log((arg1 || '') +' '+ (arg2 || '') +' '+ (arg3 || '')));
	},

	// CUI.ready() for pages to call instead of $.ready()
	readyQueue: [],
	readyPaused: true,
	readyjQuery: null,
	readyPause: function() {
		CUI.log('Pausing readyQueue.');
		this.readyPaused = true;
	},
	readyUnpause: function() {
		CUI.log('Unpausing readyQueue.');
		this.readyPaused = false;
	},
	ready: function(fn, jQuery) {
		this.readyjQuery = jQuery;
		// if (jQuery._cui_extended_ready) {
		if (this.readyPaused)
			this.readyQueue.push(fn);
		else {
			CUI.log('Executing JS immediately (readyQueue is not paused).');
			fn.call(document, jQuery);
		}
		// }
	},
	readyGo: function() {
		CUI.log('Preparing to execute', this.readyQueue.length, 'functions in readyQueue.');
		this.readyUnpause();
		while (this.readyQueue.length) {
			var fn = this.readyQueue.shift();
			CUI.log('Executing JS.');
			fn.call(document, this.readyjQuery);
		}
	},
	unloadQueue: [],
	unload: function(fn, jQuery) {
		this.unloadQueue.push(fn);
	},
	unloadGo: function() {
		CUI.log('Preparing to execute', this.unloadQueue.length, 'functions in unloadQueue.');
		var result = true;

		for (var i = 0; i < this.unloadQueue.length; i++) {
			var fn = this.unloadQueue[i];
			CUI.log('Executing JS.');

			// Set result to false if any of the unloadQueue functions returned a false. This implies the user should not be able to
			// navigate away from their page.
			if (fn.call(document, this.readyjQuery) === false) {
				result = false;
			}
		}

		// If none of the unloadQueue functions returned a false, we clear the queue and continue with the page navigation.
		// We want to keep the functions in the unloadQueue if result is false in order to run them again when the user go to navigate away
		if (result) {
			this.unloadQueue = [];
		}

		return result;
	},
},
	_loaderInterval = setInterval(function() {
		if (document.getElementById('cui-global-loading-indicator')) {
			clearInterval(_loaderInterval);
		}
	}, 50);

less = { env: 'production' };

</script>
