	<script>
		function hideCookieAcceptance() {
			if(!cookie.getCookie('barracuda_notification_hide_2'))
				cookie.setCookie('barracuda_notification_hide_2', true, 1825);
			// Close modal
			document.getElementById("acceptance-cookie-modal").style.display = 'none';
			document.body.style.overflowY = 'visible';
		}

		/* Save visitor id when visitor clicks the accept or close button on the cookie acceptance modal */
		(function() {

			document.getElementById("accept-cookie-btn").addEventListener('click', hideCookieAcceptance);
			document.getElementById("close-cookie-btn").addEventListener('click', hideCookieAcceptance);
		})();
	</script>




