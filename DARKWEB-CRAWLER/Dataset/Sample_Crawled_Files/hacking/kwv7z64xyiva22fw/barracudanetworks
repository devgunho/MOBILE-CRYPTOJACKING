</script>
<script type="text/javascript">
// Initialize variables
var scrollPos                   = 0;
var ticking                     = false;
var section2                    = document.getElementById('section2');
var event1                      = document.querySelector('#event1');
var event2                      = document.querySelector('#event2');
var whatWeDoTitle               = document.querySelectorAll('.what-we-do-title');
var whatWeDoBody                = document.querySelectorAll('.what-we-do-body');
var swiperContents              = document.querySelectorAll('.slider-content');
var newsSection                 = document.querySelector('#newsSwiper');

// Event listener(s)
window.addEventListener('touchmove', handleScroll); // For mobile because mobile cannot detect scroll event
window.addEventListener('scroll', handleScroll);



/*************************************************************************************************************************************************/
// Parallax swiper (jumbotron)
/*************************************************************************************************************************************************/
var parallaxSwiper = new Swiper('#parallaxSwiper', {
    effect: 'fade',
    loop: true,
    slidesPerView: 1,
    pagination: {
        el: '.swiper-pagination',
        clickable: true,
        renderBullet: function (index, className) {
            return '<div class="swiper-pagination-bullet-container ' + className + '"><span class="custom-pagination"></span></div>';
        }
    },
    autoplay: {
        delay: 10000,
    },
    navigation: {
        nextEl: '#parallaxSwiper .swiper-button-next',
        prevEl: '#parallaxSwiper .swiper-button-prev',
    },
    preventClicks: false,
    preventClicksPropagation: false,
    preventInteractionOnTransition: true
});
parallaxSwiper.on('slideNextTransitionEnd', function () {
  document.querySelector("#parallaxSwiper .swiper-button-prev").classList.remove("swiper-button-disabled");
  document.querySelector("#parallaxSwiper .swiper-button-next").classList.remove("swiper-button-disabled");
});
/*************************************************************************************************************************************************/ 
/*************************************************************************************************************************************************/



/*************************************************************************************************************************************************/
// stop autoplay when hover in slider-content, re-start when hover out
/*************************************************************************************************************************************************/
var sliderContent = document.querySelectorAll('.slider-content');
for (var i = 0; i < sliderContent.length; i++) {
    sliderContent[i].addEventListener("mouseover", function() {
        parallaxSwiper.autoplay.stop();
    });

    sliderContent[i].addEventListener("mouseout", function() {
        parallaxSwiper.autoplay.start();
    });
}

/*************************************************************************************************************************************************/ 
/*************************************************************************************************************************************************/


/*************************************************************************************************************************************************/
// News swiper
/*************************************************************************************************************************************************/
var newsSwiper = new Swiper('#newsSwiper', {
    // Show limited amount of slides depending on the window width
    // 1440px is break-point for larger laptop
    // 1024 is break-point for laptop
    // slidesPerView: (document.body.clientWidth < 1440 ? (document.body.clientWidth < 1024 ? 3 : 4) : 5),
    slidesPerView: 'auto',
    // centeredSlides: true,
    // spaceBetween: 50,
    // slidesPerGroup: 3,
    initialSlide: cookie.getCookie('barracuda_news_swiper_active_slide'),
    watchOverflow: true,
    navigation: {
        nextEl: '#newsSwiperContainer .swiper-button-next',
        prevEl: '#newsSwiperContainer .swiper-button-prev',
    },
});
newsSwiper.on('slideChangeTransitionEnd', function(){
    document.cookie = "barracuda_news_swiper_active_slide=" + newsSwiper.activeIndex + "; expires=0; path=/";
});
/*************************************************************************************************************************************************/ 
/*************************************************************************************************************************************************/

/*************************************************************************************************************************************************/
// Scroll-based animations
/*************************************************************************************************************************************************/

// Check if given element is anywhere in viewport
function isElementInView(el) {

    var rect            = el.getBoundingClientRect();
    var windowHeight    = (window.innerHeight || document.documentElement.clientHeight);
    var windowWidth     = (window.innerWidth || document.documentElement.clientWidth);
    var vertInView      = (rect.top <= windowHeight) && ((rect.top + rect.height) >= 0);
    var horInView       = (rect.left <= windowWidth) && ((rect.left + rect.width) >= 0);

    return (vertInView && horInView);
}


// Check if given element is anywhere in viewport
function isViewBelowElement(el) {

    var rect            = el.getBoundingClientRect();

    // console.log('rect', rect);
    return Math.sign(rect.top) == -1 ? true : false;
}


// Handles the fade effect for the slider content upon scroll
function sliderContentAnimation() {
    
    if (scrollPos > 250 && scrollPos < 1000) {

        for (var i = swiperContents.length - 1; i >= 0; i--) {
            swiperContents[i].className = 'slider-content fade-out';
        }
    }
    else if (scrollPos <= 250) {

        for (var i = swiperContents.length - 1; i >= 0; i--) {
            swiperContents[i].className = 'slider-content fade-in';
        }
    }
}

// Slide up effect for the events
function animateEventsSection() {

    // Only apply slide-up effect to existing event div (sometimes there might only be 1 or none)
    if (event1) 
        event1.className = 'slide-up';
    

    if (event2) {
      setInterval(function(){
        event2.className = 'slide-up'; 
      }, 300);
      
    }
}

// Fades in given array of elements
function fadeElementIn(elms) {

    for (var i = elms.length - 1; i >= 0; i--) {

        // We don't want to keep putting in the same class
        if (elms[i].className.indexOf('fade-in-delay') === -1) {
            var originalClassname = elms[i].className;
            elms[i].className = originalClassname + ' fade-in-delay';                
        }
    }
}

// Event listener function, fired everytime user scrolls
function handleScroll(e) {

    scrollPos = window.scrollY;

    if (!ticking) {

        window.requestAnimationFrame(function() {

            sliderContentAnimation();

            if (isElementInView(whatWeDoTitle[0])) {
                fadeElementIn(whatWeDoTitle);
            }

            if (isElementInView(whatWeDoBody[0])) {
                fadeElementIn(whatWeDoBody);
            }

            if (isElementInView(section2)) {
                animateEventsSection();
            }
            
        });
        /*** Note: Comment this out because it's not required ***/
        // ticking = true;
    }
}
// Self-invoke this function in case the user lands midway in the page
handleScroll();

// Check if should animate on the start scrren
if (isElementInView(section2) || isViewBelowElement(section2)) {
    animateEventsSection();
}


/*************************************************************************************************************************************************/ 
/*************************************************************************************************************************************************/    
</script>

