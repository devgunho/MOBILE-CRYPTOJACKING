<!--LHN CODE-->

<script type="text/javascript">
  
// Generate a random number
var rand_num = (Math.floor(Math.random() * 18) + 1);

// Get random livechat person's profile image and change the src of the img in chat bubble
document.getElementById('chat-person-bubble').src = "https://www.barracuda.com/assets/images/common/livechat/img_live-chat-person_" + rand_num + ".jpg";
// change the src of the img in chat invite
document.getElementById('chat-person-invite').src = "https://www.barracuda.com/assets/images/common/livechat/img_live-chat-person_" + rand_num + ".jpg";


// Smarty Variables
var lang_code = "en";


/******************* LHN SDK Initialize before SDK loads *************************/

// Default to Sales LHN config
var lhn_widget_id = "512b62f8-cc54-4b80-b89b-375653fc9e99"; 
var newChatTitleForDept = "Chat with us";
var chatWindowIdForDept = 42465; // Show english chat window

if(lang_code == 'de'){
	var chatWindowIdForDept = 42971; // Show german chat window
}

if(window.location.pathname.split("/")[1] == "support") // Support
{
	lhn_widget_id = "07d9c64d-8a89-4a4d-a209-5cc467e356d5"; 
	newChatTitleForDept = "Chat with Support";
	chatWindowIdForDept = 42466; // Show english chat window

	if(lang_code == 'de'){
		chatWindowIdForDept = 42972; // Show german chat window
	}
}
else if ((window.location.pathname.split("/")[1] == "products" && window.location.pathname.split("/")[2] == "webapplicationfirewall") // WAF
	|| window.location.pathname.split("/")[1] == "wafmq" || window.location.pathname.split("/")[1] == "waf-as-a-service")	
{
	lhn_widget_id = "746648a5-9520-49b5-864f-67d04fc39f34"; 
	newChatTitleForDept = "Chat with Specialist";
	chatWindowIdForDept = 43885; // Show english chat window

	if(lang_code == 'de'){
		chatWindowIdForDept = 43886; // Show german chat window
	}

}
else if (window.location.pathname.split("/")[1] == "products" && window.location.pathname.split("/")[2] == "phishline")
{
	lhn_widget_id = "db3135fd-aee3-46e1-85a5-e422e36efa74"; 
	newChatTitleForDept = "Chat with Specialist"; // same as WAF
	chatWindowIdForDept = 44378; // Show english chat window

	if(lang_code == 'de'){
		chatWindowIdForDept = 44379; // Show german chat window
	}
}

/******************* END LHN SDK Initialize before SDK loads *************************/

//Added opening chat window functionality here becoz it gets widely used in all tpl. Change in place.
window.lhnJsSdkInit = function () {

	lhnJsSdk.setup = {
		application_id: "9ffebe61-5df4-4648-b008-eb3f0cf596a5",
		application_secret: "7ee82a29986541e38632c176beb70936e36dba37bec947dd95"
	};
	
	lhnJsSdk.dictionary = {
		agentConnecting: "Connecting to agent",
		cancel: "Cancel",
		chatBoxPlaceholder: "Your message",
		chatTitle: "Conversation", // After chat session starts
		newChatTitle: newChatTitleForDept, // Before chat session starts
		email: "Email",
		endChat: "End Chat",
		endChatConfirm: "Are you sure you want to end the current chat?",
		inviteCancel: 'DISMISS',
		inviteStart: 'CHAT NOW',
		offlineTitle: "Leave a message",
		send: "Send",
		submit: "Submit",
		startChat: "Start Chat"
	};
	
	lhnJsSdk.options = {
		custom1: "", //custom1 value -- 350 character limit
		custom2: "2484a8d79c51c2c88c6b8570f08195fa", //custom2 value -- 350 character limit
		custom3: "https://www.barracuda.com/livechat/create_sf_lead?sesd=2484a8d79c51c2c88c6b8570f08195fa", //custom3 value -- 350 character limit
		chatWindowId: chatWindowIdForDept,
		hideButtonWhenOpen: true
	};
	
	lhnJsSdk.preChatData = {
	};
	
	lhnJsSdk.controls = [{
		type: "hoc",
		id: lhn_widget_id
	}];
};

// This function will be called when sdk is done loading and ready
function lhnJsSdkReady()
{
	// We need to hide the chat bubble on support page as its confusing but show everywhere else
    if(window.location.pathname.split("/")[1] == "support" && (window.location.pathname.split("/")[2] == null || window.location.pathname.split("/")[2] == "index" ))
    {}
    else
    {
		document.getElementById('chat-container').style.display = 'block'; // Show chat container only when everything is loaded
	
		document.getElementById('lhnHocButton').addEventListener('click', function(){
      openLHNEmbeddedChat();
      this.classList.remove('animateUp')
      this.classList.add('animateDown');

    });


	}
	
	// Need to show the invite 15 seconds after lhn load
	setTimeout(check_if_chat_loaded, 15000);
}

(function (d, s) {
  var newjs, lhnjs = d.getElementsByTagName(s)[0];
  newjs = d.createElement(s);
  newjs.src = "https://developer.livehelpnow.net/js/sdk/lhn-jssdk-current.min.js";
  lhnjs.parentNode.insertBefore(newjs, lhnjs);
}(document, "script"));


// This opens up the embedded chat for Support
function openLHNEmbeddedChat()
{    
	//console.log(lhnJsSdk);
	if (typeof(lhnJsSdk) == 'undefined' || (typeof(lhnJsSdk) !== 'undefined' && typeof(lhnJsSdk.isOnline) == 'undefined'))
	{
		lhnIssue();
	}
	else
	{
		lhnJsSdk.refresh();
		lhnJsSdk.openHOC();

    document.getElementById('lhnWindowMinimizer').addEventListener('click', function() {
      document.getElementById('lhnHocButton').classList.remove('animateDown');
      document.getElementById('lhnHocButton').classList.add('animateUp');

    });

	}
	
	return false;
}

// This function is added to show sorry inconvenience message when livechat isnot functioning
function lhnIssue()
{
	var processingModal = modalFactory.build({
		modal : document.getElementById('livechat-issue'),
		obj : {
			onModalHide : function(){},
			onModalHidden: function(){}
		}
	});
	
	// Lauching the modal with spinner
	processingModal.launch();
}

/******************* Event Listners for pages *************************/

// get the chats elems for header/footer and add popup events
if (document.querySelector('[data-chat]') !== null)
{
	document.querySelector('[data-chat]').addEventListener('click', function(){ openLHNEmbeddedChat() });
}

// get the chats elems for glossary page and add popup events
if (document.querySelector('[data-chat-glossary]') !== null)
{
	document.querySelector('[data-chat-glossary]').addEventListener('click', function(){ openLHNEmbeddedChat() });
}

// get the chats elem for waf page and add popup events
if (document.querySelector('[data-chat-waf]') !== null)
{

	this.dataChatWaf = document.querySelectorAll('[data-chat-waf]');

	for (var i = 0; i < this.dataChatWaf.length; i++)
	{
		this.dataChatWaf[i].addEventListener('click', function(){ openLHNEmbeddedChat() });
	}
}

// get the chats elems for support page and add popup events 
if (document.querySelector('[data-chat-support]') !== null)
{
	this.dataChatSupport = document.querySelectorAll('[data-chat-support]');
	// add popup events twice as there are 2 elements on the page with same attribute
	for (var i = 0; i < this.dataChatSupport.length; i++)
	{
		this.dataChatSupport[i].addEventListener('click', function(){ openLHNEmbeddedChat() });
	}
}



/******************* Invite window *************************/

// Default hide the invite
document.getElementById('lhnHocInvite').style.display = "none";

// Show the chat invite
function show_invite()
{
	document.getElementById('lhnHocInvite').style.display = "block";
	
	document.getElementById('invite_chat_now').addEventListener('click', openLHNEmbeddedChat);
}

// set the cookie which stops invite until the end of that session
function setcookie_to_notshow_invite()
{
	cookie.setCookie('lhnAutoInviteDismissed', true);
}

// Check if the lhn chat is loaded fully before checking chat progress.
function check_if_chat_loaded()
{
	var chat_in_progress = (typeof(document.getElementsByClassName('lhnChat')[0]) != "undefined");
	//console.log('chat in progress: ' + chat_in_progress);

	// show the invite only if the chat is not in progress and user hasnot dismissed it 
	if(!chat_in_progress && cookie.getCookie('lhnAutoInviteDismissed') == "")
	{
		show_invite();
	}
}
  
  
    
</script>
</body>
</html>


