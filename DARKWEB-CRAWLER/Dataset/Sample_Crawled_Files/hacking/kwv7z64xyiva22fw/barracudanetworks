	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

	// set property ID based on domain
	ga('create', (ga_keys[subdomain] || login_ga_key), 'auto');
	ga('send', 'pageview');

	// Event notifies components waiting on GA readiness
	var gaEvent;
	if (typeof Event === 'function') {
		// Standard events:
		gaEvent = new Event('gaReady');
	} else {
		// IE11-style events:
		gaEvent = document.createEvent('CustomEvent');
		gaEvent.initEvent('gaReady', true, true);
	}
	document.dispatchEvent(gaEvent);
</script>
