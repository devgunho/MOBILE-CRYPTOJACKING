				<script type="text/javascript" language="JavaScript">

				function readCookie(name) {
				     var nameEQ = name + "=";
				     var ca = document.cookie.split(';');
				     for(var i=0;i < ca.length;i++) {
				           var c = ca[i];
				           while (c.charAt(0)==' ') c = c.substring(1,c.length);
				           if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
				     }
				     return "direct";
				}
				</script>

				<script type="text/javascript">

				function setform(){

				var webchannel_val = readCookie('webchannel');
				var land_url = window.location.href;

				document.getElementById('iframeform').innerHTML = ('<iframe src="https://go.blackducksoftware.com/LP=84?ar_id=428&lang=en-us&sfdc_id=70134000001OFqd&sfdc_name=19_01_Global_CU_BlackDuck&land_url=' + land_url + '&webchannel_val=' + webchannel_val + '" width="100%" height="1140px" frameborder="0"></iframe>');

				}

				window.addEventListener ?
				window.addEventListener("load",setform,false) :
				window.attachEvent && window.attachEvent("onload",setform);
				</script>

  			</section>

