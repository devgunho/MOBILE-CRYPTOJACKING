  window.drift.on("emailCapture", function(e) {
    // Heap Analytics  tracking
    if (window['heap']) {
      heap.track('Chat - email entered');
    }

    // Google Analytics tracking
    gtag('config', 'UA-1434058-1', { 'page_path': '/goal/saleschat/' });

