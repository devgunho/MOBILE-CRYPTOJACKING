     $(document).ready(function(){
         $('#collapseSeeMore')
                 .on('hidden.bs.collapse', function () {
                     $('#employer-see-more-button').text("See More");
                 })
                 .on('shown.bs.collapse', function () {
                     $('#employer-see-more-button').text("See Less");
                 });
     });
     $( window ).load(function(){
         $('.alert.top-alert.alert-dismissable').show('fast');
         setTimeout(function(){
             $('.alert.top-alert.alert-dismissable').fadeOut("slow");
         }, 5000);
     });

     //this is where we apply opacity to the arrow
     $(window).scroll( function(){
         //get scroll position
         var topWindow = $(window).scrollTop();
         var windowHeight = $(window).height();

         var miniRegisterActionButton = $('#miniRegisterActionButton');
         if (topWindow > windowHeight) {
             miniRegisterActionButton.show();
         } else {
             miniRegisterActionButton.hide();
         }
     });
 </script>

