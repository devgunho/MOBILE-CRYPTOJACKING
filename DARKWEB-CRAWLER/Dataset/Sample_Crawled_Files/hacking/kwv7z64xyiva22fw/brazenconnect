        var params = getQueryParams() ? getQueryParams() + "&" : "?" + getQueryParams();

        var url = "/events/43dzv/login" + params + "force_redirect=true&expand_login=true";
            url += ";wasUserInvitedAsRep=true";

        window.open(url, "_self");
    }

    function register() {
        var url;
               url = "https://communitybrands.wufoo.com/forms/z1o0yvpi0wk2p4i/";

        if (url) {
            window.open(url, "_self");
        }
    }

    function getQueryParams() {
        var url = window.location.href;
        var queryParams = url.indexOf("?");
        if (queryParams !== -1) {
            return url.substring(queryParams);
        }
        else {
            return "";
        }
    }

    function goToExhibitorPage() {
        var url = window.location.href;
        var hasQueryParams = url.indexOf("?") !== -1;

        var exhibitorUrl;
        if (hasQueryParams) {
            exhibitorUrl = url.substring(0, url.indexOf("?"));
        } else {
            exhibitorUrl = url;
        }

        exhibitorUrl += "/exhibitors" + getQueryParams();

        window.open(exhibitorUrl, "_self");
    }

    function goToAttendeePage() {
        var url = window.location.href;
        window.open(url.replace("/exhibitors", ""), "_self");
    }
</script>

<script>
    function showModal(modalTitle, modalBody, showOk) {
        $('#modalTitle').text(modalTitle);
        $('#modalBodyText').text(modalBody);

        var modalFooter = $('#modalFooter');
        if (showOk) {
            modalFooter.show();
        } else {
            modalFooter.hide();
        }

        $("body").addClass("modal-open");
        var modal = $('#modal');
        modal.attr("tabIndex", "0");
        modal.attr("aria-hidden", "false");
        modal.modal('show');
    }

    function hideModal()
    {
        var modal = $('#modal');
        modal.attr("tabIndex", "-1");
        modal.attr("aria-hidden", "true");
        modal.modal('hide');
    }

    $(document).ready(function(){
        $('.close-modal').on("click", function ()
        {
            $("body").removeClass("modal-open");
            hideModal();
        });
    });

</script>

<script>
    $(document).ready(function(){
            $('.are-you-attendee').on("click", function ()
            {
                goToAttendeePage();
            });

            $('.are-you-exhibitor').on("click", function ()
            {
                goToExhibitorPage();
            });

            $('.login-button').on("click", function()
            {
                login();
            })

        $('.register-button').on("click", function ()
        {
            register();
        });
    });
</script>

 <script>
     $(document).ready(function(){
         $('#collapseSeeMore')
                 .on('hidden.bs.collapse', function () {
                     $('#employer-see-more-button').text("See More");
                 })
                 .on('shown.bs.collapse', function () {
                     $('#employer-see-more-button').text("See Less");
                 });
     });
     $( window ).load(function(){
         $('.alert.top-alert.alert-dismissable').show('fast');
         setTimeout(function(){
             $('.alert.top-alert.alert-dismissable').fadeOut("slow");
         }, 5000);
     });

     //this is where we apply opacity to the arrow
     $(window).scroll( function(){
         //get scroll position
         var topWindow = $(window).scrollTop();
         var windowHeight = $(window).height();

         var miniRegisterActionButton = $('#miniRegisterActionButton');
         if (topWindow > windowHeight) {
             miniRegisterActionButton.show();
         } else {
             miniRegisterActionButton.hide();
         }
     });
 </script>

</body>
</html>



