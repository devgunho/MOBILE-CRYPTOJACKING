function checkOptions()
{
	optionValueIKT = getOptionValue("IKT");

	if(document.forms[0].ADI_BIB)
	{
		if(document.forms[0].ADI_BIB.options[0].selected != true) // wenn erstes Element "Alle Bibliotheken"
		{													// nicht selektiert ist
		   setMatFilter();
		   document.forms[0].NOSCAN.value="Y";
		}
		else if(document.forms[0].ADI_BIB.options[0].selected == true) // wenn erstes Element "Alle Bibliotheken"
		{																															 // selektiert ist
			if (document.getElementById("ADI_MAT_Id").checked == true)
			{
			   setMatFilter();
			}
		}
	}
	if(document.getElementById("ADI_MAT_Id"))
	{
		if(document.getElementById("ADI_MAT_Id").checked == true)
		{
		   setMatFilter();
		}
	}
	if(document.getElementById("ADI_EXCLUDE_Id"))
	{
		if(document.getElementById("ADI_EXCLUDE_Id").checked == true)
		{
		   setMatFilter();
		}
	}
	if(document.getElementById("ADI_DAT_Id"))
	{
		if(document.getElementById("ADI_DAT_Id").checked == true)
		{
		   setMatFilter();
		}
	}
	if(document.getElementById("ADI_BIB_Id"))
	{
		if(document.getElementById("ADI_BIB_Id").checked == true)
		{
		   setMatFilter();
		}
	}
	return true;
}

function sortOptions()
{
	var Bibs = new Array();
	var selectedValue = 0;

	if(document.forms[0].ADI_BIB)
		var listenObj = document.forms[0].ADI_BIB; // Listen Object

	var tmpPair = new Array(listenObj.length);

	var tmpTextAll  = listenObj.options[0].text; // Text "Alle Bibliotheken" merken
	//var tmpValueAll = listenObj.options[0].value;// (darf nicht mitsortiert werden)

	// Listenwerte holen und in Array speichern
	for (i = 1; i < listenObj.length; ++i) // ab i=1, zweites Element der Liste
	{
		var tmpText  = listenObj.options[i].text;
		var tmpValue = listenObj.options[i].value;

		if(listenObj.options[i].selected)
			selectedValue = tmpValue; // selektierten Wert merken

		tmpPair[i-1] = new Array(2);
		tmpPair[i-1][0] = tmpText; // text und value als paar speichern, fuer die Wiederherstellung nach der Sortierung
		tmpPair[i-1][1] = tmpValue;

		Bibs.push(tmpText);
	}

	//Array sortieren
    Bibs.sort();

	listenObj.options[0].text = tmpTextAll ; // Text "Alle Bibliotheken" an erster Stelle  setzen
	listenObj.options[0].value = ""; // und value leer setzen

	//Listenwerte mit den sortierten Werten ueberschreiben
	for (i = 0; i < Bibs.length; ++i)
	{
		var tmpText = Bibs[i];

		listenObj.options[i+1].text = tmpText;

		// fuer den sortierten Text den richtigen value suchen
		for (j = 0; j < Bibs.length; ++j)
		{
			if (tmpPair[j][0] == tmpText) //passendes value finden
			{
				tmpValue = tmpPair[j][1];
				break;
			}
		}

		listenObj.options[i+1].value = tmpValue.toLowerCase();

		if(tmpValue == selectedValue) // selection wiederherstellen
			listenObj.options[i+1].selected = true;
	}
}

function helpIKTlist()
{
	var optionIndex = "";
        var optionValue = "";
	var DB = "2.104";
	var LNG = "DU";

//	if (document.forms[0].IKT.selectedIndex)
//	{
	  optionIndex = document.forms[0].IKT.selectedIndex; // selectierter index
	  optionValue = document.forms[0].IKT.options[optionIndex].value; // zum selectierenden index index gehoerender wert
	 // alert ("optionIndex: " + optionIndex + ", optionValue: " + optionValue);
//	}


	if (DB == "2.104" || DB == "2.114")
	{if (optionValue == "2072" || optionValue == "2073" || optionValue == "3023")
	 {
                var url_help = "http://icoswb.bsz-bw.de/psi/img_psi/2.0/help_ikt/2.104" + "/" + LNG + "/" + optionValue + ".htm";
	 }
	 else
         var url_help = "http://icoswb.bsz-bw.de/psi/img_psi/2.0/help_ikt/2.104" + "/" + LNG + "/ognd_iktlist" + ".htm#" + optionValue;
	}
        else
	var url_help = "http://icoswb.bsz-bw.de/psi/img_psi/2.0/help_ikt/2.104" + "/" + LNG  + "/" + optionValue + ".htm";

	return url_help;
}

var popupWindow = null;

function popUp(url)
{
   popupWindow = window.open(url, "Popupfenster", "width=600,height=550,resizable=yes, scrollbars=yes");
   popupWindow.focus();
}


function setMousePointer(obj, status)
{
	if (status == 1)
	{
		if (document.all) // wenn IE
			obj.style.cursor='hand';
		else
			obj.style.cursor='pointer';
	}
	else
		obj.style.cursor='default';
}

function resetForm()  // Alle Eingaben auf loeschen bzw. auf default setzen
{
	if (document.forms[0].ACT)
		document.forms[0].ACT.selectedIndex = 1;

	if (document.forms[0].IKT)
		document.forms[0].IKT.selectedIndex = 0;

        if (document.forms[0].TRM)
		document.getElementById("TRM").value = "";

                var x=document.getElementById("TRM");
                if(x) x.focus();

	if (document.forms[0].SRT)
		document.forms[0].SRT.selectedIndex = 0;

	var DB = "2.104";

	if (DB != "2.312" && document.forms[0].ADI_BIB)
		document.forms[0].ADI_BIB.selectedIndex = 0;

	if(document.getElementById("ADI_MAT_Id"))
	{
		if(document.getElementById("ADI_MAT_Id").checked)
			document.getElementById("ADI_MAT_Id").checked = false;
	}
	if(document.getElementById("ADI_EXCLUDE_Id"))
	{
		if(document.getElementById("ADI_EXCLUDE_Id").checked)
			document.getElementById("ADI_EXCLUDE_Id").checked = false;
	}
	if(document.getElementById("ADI_DAT_Id"))
	{
		if(document.getElementById("ADI_DAT_Id").checked)
			document.getElementById("ADI_DAT_Id").checked = false;
	}
	if(document.getElementById("ADI_BIB_Id"))
	{
		if(document.getElementById("ADI_BIB_Id").checked)
			document.getElementById("ADI_BIB_Id").checked = false;
	}
	if(document.getElementById("fuzzy"))
	{
		if(document.getElementById("fuzzy").checked)
			document.getElementById("fuzzy").checked = false;
	}
}

