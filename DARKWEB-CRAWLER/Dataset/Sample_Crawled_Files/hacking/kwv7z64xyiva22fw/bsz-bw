function getOptionValue(IKT)
{
	var optionIndex= "";
	var optionValue= "";

	if (document.getElementById("IKT").selectedIndex)
	{
		optionIndex = document.getElementById("IKT").selectedIndex; // selectierter index
		optionValue = document.getElementById("IKT").options[optionIndex].value; // zum selectierenden index gehoerender wert
	}
    return optionValue;
}

function setMatFilter()
{
	document.forms[0].MATCFILTER.value="Y";
	document.forms[0].MATCSET.value="Y";
}

function setActBrowse()
{
	document.forms[0].ACT.value="BRWS";
}

function checkSubmit()
{
	var LIB_SELECT = "";
//	var IMPLICITTRUNC = "N";
	var LIB_ID_SELECT = "";
	var ADI_MAT = "";
	var DB = "2.104";
	var bRet = true;

	if (LIB_SELECT != "" || LIB_ID_SELECT != "" || ADI_MAT != "")
		bRet = checkOptions();

	var optionValueIKT = getOptionValue("IKT");

	if (optionValueIKT)
	{
	if (DB == "2.369")
           {
		if(optionValueIKT == "2130" || optionValueIKT == "2135" || optionValueIKT == "2144" || optionValueIKT == "3020" || optionValueIKT == "4000" || optionValueIKT == "4002" || optionValueIKT == "2220") // SIG, SGE, SXS, LNO, SKT, BKT, SGL
		{
			checkTRM();
		}
           }
        else
           {
		if(optionValueIKT == "2130" || optionValueIKT == "2135" || optionValueIKT == "3020" || optionValueIKT == "4000" || optionValueIKT == "4002" || optionValueIKT == "2220") // SIG, SGE, LNO, SKT, BKT, SGL
		{
			checkTRM();
		}
           }
	}
//	if (IMPLICITTRUNC == "Y")
//	{
//		if(optionValueIKT == "4070")// TST
//		{
//			implicitTRUNC();
//		}
//	}
	if (document.getElementById("ADI_MAT_Id"))
	{
		if (document.getElementById("ADI_MAT_Id").checked == true)
		{
			setMatParam();
		}
	}
	if (document.getElementById("ADI_EXCLUDE_Id"))
	{
		if (document.getElementById("ADI_EXCLUDE_Id").checked == true)
		{
			setMatParam();
		}
	}

	if (document.getElementById("ADI_DAT_Id"))
	{
		if (document.getElementById("ADI_DAT_Id").checked == true)
		{
			setMatParam();
		}
	}
	if (document.getElementById("ADI_BIB_Id"))
	{
		if (document.getElementById("ADI_BIB_Id").checked == true)
		{
			setMatParam();
		}
	}
	if(optionValueIKT == "4000" || optionValueIKT == "4002")
	{
		{
		   setActBrowse();
		}
	}

	if (DB == "2.103" || DB == "2.104")
	{
		document.getElementById("retrace").value = "0";
		document.getElementById("trm_old").value = "";
	}

	return bRet;
}

function checkTRM()
{
	var TRMvalue = document.getElementById("TRM").value;
	var length = TRMvalue.length;
	var accent = "`";
	var liste = new Array("?", "!", "*", "#");

	if (length == 0)
		return;

	var TRMvalueNew = TRMvalue.replace(/`/g, "");

	length = TRMvalueNew.length;

	for (var i=0; i<liste.length; i++)
	{
		var z = liste[i];

		while(TRMvalueNew[length-1] == z) // letzte Zeichen
		{
			if (length > 1 && TRMvalueNew[length-2] == z) // vorletzte Zeichen (doppelt -> muss weg)
			{
				TRMvalueNew = TRMvalueNew.substring(0, length-1);
				length = length-1;
			}
			else
				break;
		}
	}

	var lastChar = TRMvalueNew.charAt(length-1);

	var TRMvalueNew2 = TRMvalueNew.substring(0, length-1);

	if (lastChar == "?" || lastChar == "*" || lastChar == "!" || lastChar == "#")
	{
		TRMvalueNew = accent + TRMvalueNew2 +  accent + lastChar;
	}
	else
		TRMvalueNew = accent + TRMvalueNew + accent;

	document.getElementById("TRM").value = TRMvalueNew;

}

