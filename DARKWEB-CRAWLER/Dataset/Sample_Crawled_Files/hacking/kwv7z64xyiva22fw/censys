    <script>
      $(document).ready(function(){

        /* affix the navbar after scroll below header */
        $(".navbar__container").affix({
          offset: {top: parseInt($(".navbar__container").css('padding-top'))}
        });

        $('.mobile-nav__dismiss, .mobile-nav__overlay').on('click', function () {

            $('.mobile-nav--conatiner').removeClass('active');
            $('.mobile-nav__overlay').fadeOut();
        });

        $('#sidebarCollapse').on('click', function () {
            $('.mobile-nav--conatiner').addClass('active');
            $('.mobile-nav__overlay').fadeIn();
            $('.collapse.in').toggleClass('in');
            $('a[aria-expanded=true]').attr('aria-expanded', 'false');
        });

        $('.pricing-page__table-controls li').on('click', function () {
          event.preventDefault();

          if (!$(this).hasClass('active')){
            console.log(this);
            $('.price-card__price--yearly, .price-card__price--monthly').toggleClass('active');
          }
          $('.pricing-page__table-controls li').removeClass('active');
          $(this).addClass('active');

        });

      });

      // Bind functionality for making pre-canned search queries in our
      // search-blocks simply click targets to execute the search query.
      $('[data-controller=search-block] [data-action="click->search-block#query-clicked"]').on('click', function () {
        if ($('[data-target="search-block.query-input"]').is(':disabled')) {
          event.preventDefault();
          $('[data-target="search-block.form"]').submit();
        }
      });
    </script>
    <script>
      function certsearch() {
        value = document.getElementById('cert-q').value;
        query = 'tags: trusted and parsed.names: '+value;
        window.location = '/certificates?q=' + encodeURIComponent(query);
        return false;
      }
      function scadasearch() {
        value = document.getElementById('scada-q').value;
        query = 'tags: scada and (location.country_code: "'+value+ '" or location.country: "'+value+'")';
        window.location = '/ipv4?q=' + encodeURIComponent(query);
        return false;
      }
    </script>

    
  <script>
      function ipv4() {
          document.getElementById("home-search").action="/ipv4";
          document.getElementById("home-search").submit();
      }

      function websites() {
          document.getElementById("home-search").action="/domain";
          document.getElementById("home-search").submit();
      }

      function certificates() {
          document.getElementById("home-search").action="/certificates";
          document.getElementById("home-search").submit();
      }

      function indexSelectDidChange(event, searchIndexConstant) {
        event.preventDefault();
        switch (searchIndexConstant) {
          case 'ipv4':
            ipv4();
            break;
          case 'websites':
            websites();
            break;
          case 'certificates':
            certificates();
            break;
          default:
            console.error('Unable to change search index based on event: ' + event);
        }
      }
  </script>
  <script charset="utf-8" src="https://censys.io/static/js/jquery.autocomplete.min.js"></script>
  <script>
      var baseRoute = window.location.pathname.split('/').filter(function(s) { return !!s; })[0];
      if (window.location.pathname === "/") { baseRoute = "ipv4"; }
      if (['ipv4', 'certificates', 'domain'].indexOf(baseRoute) !== -1) {
          var autocompleteRoute = "/static/data/autocomplete-" + baseRoute + ".json";
          $.ajax({
              url: autocompleteRoute,
              success: function (data) {
                  $('#q').autocomplete({
                      lookup: data["data"],
                      lookupLimit: 250,
                      groupBy: 'category',
                      delimiter: ' ',
                      maxHeight: 600,
                      tabDisabled: true,
                      minChars: 0,
                      beforeRender: function (_, suggestions) {
                          if (suggestions) {
                              $("#q").css("border-bottom-left-radius", "0pt");
                          }
                      },
                      onHide: function (_) {
                          $("#q").css("border-bottom-left-radius", "4pt");
                      },
                      onSelect: function (suggestion) {
                          this.focus();
                          this.value = this.value;
                      }
                  });
              }
          });
      }
  </script>

