<script>
var sortVar = 'title';
</script>
<script>
$(document).ready(function(){
registerMoreLess('.filter',7);
var listCount = 0;
var pageCount = 10;
var paginationBottomOptions = {
name: "paginationBottom",
paginationClass: "paginationBottom",
outerWindow: 1,
innerWindow: 1,
next: true,
prev: true
};
var listOptions = {
valueNames: ["title", "description", "yearFilter", "dateSortFilter", "categoryFilter", "divisionFilter", "lastNameFilter", "typeFilter", "publisherFilter", "sponsoredFilter", "areaFilter", "renewalFilter", "locationFilter", "durationFilter", "deliveryFilter", "examFilter", "productSuiteFilter", "serviceFilter", "certificationTypeFilter", "technicalAreaFilter","careerLocationFilter","accessFilter"],
page: pageCount,
fuzzySearch: {
searchClass: "search",
location: 0,
distance: 200,
threshold: 0.4,
multiSearch: true
},
pagination: paginationBottomOptions,
};
var assetList = new List('assets',listOptions);
assetList.on('updated', function(){
setListCount();
});
if (sortVar === 'yearFilter' || sortVar === 'dateSortFilter'){
assetList.sort(sortVar, { order: "desc" });
} else {
assetList.sort(sortVar, { order: "asc" });
}
function setListCount(){
listCount = assetList.matchingItems.length;
$(".resultCount").html(listCount + ' Results');
if(listCount == 0){
var noResults = document.createElement("li");
noResults.setAttribute("id","noResultsMessage");
noResults.innerHTML = "No results available for the selected filters.";
document.querySelector("ul.list").appendChild(noResults);
}
}
function filterList() {
var multiFilter = [];
function filterCheck(el) {
return function(value) {
return value === $(el).data('type');
}
}
$("input[type='checkbox'][class*='filterCheckbox']:checked").each(function (item) {
if (["sponsoredFilter",
"renewalFilter",
"durationFilter",
"examFilter",
"deliveryFilter",
"productSuiteFilter",
"careerLocationFilter",
"certificationTypeFilter",
"accessFilter"].some(filterCheck($(this)))
) {
multiFilter.push({type: $(this).data('type'), value: $.trim($(this).val())});
} else {
multiFilter.push({type: $(this).data('type'), value: $.trim($(this).parent().text())});
}
});
$(".lastNameFilter > .filter-selected").each(function (item) {
multiFilter.push({type: $(this).data('type'), value: $.trim($(this).text())});
});
assetList.filter( function(item) {
return (
checkCustomFilter(new CustomFilter(multiFilter, 'divisionFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'categoryFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'publisherFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'yearFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'paperTypeFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'lastNameFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'typeFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'areaFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'renewalFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'durationFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'examFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'deliveryFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'serviceFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'productSuiteFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'sponsoredFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'technicalAreaFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'locationFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'careerLocationFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'certificationTypeFilter'), item) &&
checkCustomFilter(new CustomFilter(multiFilter, 'accessFilter'), item)
);
});
}
/* Decided to not do this. leaving for reference
function addFirstLast(){
var lastStartMod = assetList.size() % pageCount;
var lastStartIndex = 1;
var lastItem = assetList.size() - 1;
if (lastStartMod > 0){
lastStartIndex = assetList.size() - lastStartMod;
} else {
lastStartIndex = assetList.size() - pageCount;
}
$('.btn-last').html('');
$('.btn-first').html('');
lastStartIndex = lastStartIndex + 1;
if(! assetList.items[0].visible()){
$('.btn-first').html(' << ');
$('.btn-first').on('click', function(){
assetList.show(1,pageCount);
});
}
if(! assetList.items[lastItem].visible())
$('.btn-last').html(' >> ');
$('.btn-last').on('click', function(){
assetList.show(lastStartIndex,pageCount);
});
/* assetList.on('updated',function(){
addFirstLast();
})*/
$(".lastNameFilter > .filter-name").click(function() {
var sel = $(this).attr('data-selected');
if (sel === "true") {
$(this).attr('data-selected', false);
$(this).removeClass('filter-selected');
} else {
$(this).attr('data-selected', true);
$(this).addClass('filter-selected');
}
filterList();
});
$("input[type='checkbox'][class*='filterCheckbox']").change(function() {
filterList();
});
$('#resultsPerPage').change(function () {
var selection = this.value;
assetList.show(1,selection);
});
function getResultsPerPage(){
var list = document.getElementById("resultsPerPage");
var itemsPerPage = list.options[list.selectedIndex].value;
return itemsPerPage;
}
var url = window.location.href;
var searchRE = new RegExp("searchstring=([^\&\#]*)(\&|$|#)");
var result = searchRE.exec(url.toLowerCase());
if(result!=null && result.length>=1){
var keyWord = decodeURI(result[1].replace("%26","&"));
$("#filter-keyword").val(keyWord);
assetList.fuzzySearch(keyWord);
}
var catRE = new RegExp("categories=([^\&\#]*)(\&|$|#)");
var catsmatch = catRE.exec(url.toLowerCase());
if(catsmatch!=null&&catsmatch.length>=1){
cats = decodeURI(catsmatch[1]).toLowerCase().replace("%26","&");
catsList = cats.split(",");
categoryFilters = $( "input[data-type='categoryFilter']" );
areaFilters = $( "input[data-type='areaFilter']" );
techAreaFilters =  $( "input[data-type='technicalAreaFilter']" );
serviceFilters = $( "input[data-type='serviceFilter']" );
productSuiteFilters = $( "input[data-type='productSuiteFilter']" );
accessFilters = $( "input[data-type='accessFilter']" );
if(accessFilters.length>0){
for(var i=0;i<accessFilters.length;i++){
cat = accessFilters[i];
if(catsList.indexOf(cat.value.toLowerCase())>=0){
cat.checked = true;
}
}
}
if(categoryFilters.length>0){
for(var i=0;i<categoryFilters.length;i++){
cat = categoryFilters[i];
if(catsList.indexOf(cat.value.toLowerCase())>=0){
cat.checked = true;
}
}
}
if(areaFilters.length > 0){
for(var i=0;i<areaFilters.length;i++){
cat = areaFilters[i];
if(catsList.indexOf(cat.value.toLowerCase())>=0){
cat.checked = true;
}
}
}
if(techAreaFilters.length>0){
for(var i=0;i<techAreaFilters.length;i++){
cat = techAreaFilters[i];
if(catsList.indexOf(cat.value.toLowerCase())>=0){
cat.checked = true;
}
}
}
if(serviceFilters.length>0){
for(var i=0;i<serviceFilters.length;i++){
cat = serviceFilters[i];
if(catsList.indexOf(cat.value.toLowerCase())>=0){
cat.checked = true;
}
}
}
if(productSuiteFilters.length>0){
for(var i=0;i<productSuiteFilters.length;i++){
cat = productSuiteFilters[i];
if(catsList.indexOf(cat.value.toLowerCase())>=0){
cat.checked = true;
}
}
}
filterList();
}
});
</script>
