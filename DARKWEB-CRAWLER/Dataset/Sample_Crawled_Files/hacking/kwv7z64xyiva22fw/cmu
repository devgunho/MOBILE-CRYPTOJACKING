jQuery('.assetBrowseList').hide(100);
jQuery('#' + divID).show(200);
}
jQuery(document).ready(function(){
toggleBrowseList('assetBrowseListShort'); // Show short list by default on page load
var banners = new BannerMessages({
messages: messages,
target: 'site-messages'
});
});
</script>
