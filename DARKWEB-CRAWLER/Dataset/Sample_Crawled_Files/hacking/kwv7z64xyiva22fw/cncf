<script>
(function ($) {
    var nav = $('.navbar-inverse');
    $(window).scroll(function (event) {
      var scroll = $(window).scrollTop();
      if(scroll > 20) {
        nav.addClass('scrolled');
      } else {
        nav.removeClass('scrolled');
      }
    });

    $("#search_toggle").on("click", function(){
			$("header").toggleClass("search-container");
      $(".search-wrap").toggleClass("search-opened").promise().done(function(){
        if($(".search-wrap").hasClass("search-opened")) {
          $("#search_form input[type='text']").focus();
        }
      });
    });
    $("#search_form .close").on("click", function(){
      $(".search-wrap").removeClass("search-opened");
      $("header").removeClass("search-container");
    });
    $(document).mouseup(function(e)    {
      var subject = $("#search_form");
      if(e.target.id != subject.attr('id') && !subject.has(e.target).length && $(".search-wrap").hasClass("search-opened") && e.target.id != "search_toggle")
      {
        $(".search-wrap").toggleClass("search-opened");
			$("header").toggleClass("search-container");
      }
    });

 /*===========*/
$( ".navbar-toggle" ).click(function() {
  $( ".navbar-collapse" ).stop().slideToggle("fast").toggleClass('in');
  $(this).toggleClass("toggle_active");
});

/*$("header .navbar-nav > li.menu-item-has-children").append( '<span class="sub_toggle"> </span>' );*/

function mobile_nav() {
	if ($(window).width() < 991) {
		$("header .navbar-nav > li").on( "click", "a", function(e) {
			var $parent_li = $(this).parent('li');
			var $sibling_ul = $(this).siblings('ul');
			$sibling_ul.stop().toggleClass('sub_nav_toggle');
			$parent_li.stop().toggleClass('active').siblings('li').removeClass('active').children('ul').removeClass('sub_nav_toggle');
		});
	}
}
function headerPusher() {
	$('header+.header-pusher').each(function() {
		height_header = $('header .navbar').outerHeight();
		$(this).height(height_header);
	});
}
function footerPusher() {
		var header_height = $('.navbar-inverse');
		var footer_height = $('footer');
		$('.container-wrap').css('min-height', $(window).height() - (header_height.outerHeight(true) + footer_height.outerHeight(true)));
}
mobile_nav();
$( window ).load(function() {
	mobile_nav();
	footerPusher();
	headerPusher();
});
$(window).resize(function() {
	mobile_nav();
	footerPusher();
	headerPusher();
}).trigger("resize");
 /*===========*/

 $('#newsletter_form').submit(function(e) {
    e.preventDefault(); // don't submit multiple times
		$('#newsletter_form').hide(); // blank the input
    $('#newsletter_form').parent().append('<h3>Thank You</h3><p>You are now subscribed to our newsletter.</p>');
