  function resize(){
    var style = document.createElement("style");
    document.head.appendChild( style );
    var sheet = style.sheet;
    var ww = $(window).width();
    var rww = 0;

    if ($('#mainStyle').attr("href").match(/tablet/)){
      rww = ww*0.30;
      ww = ww*0.70;
      sheet.insertRule( "#super-banner div {-moz-transform: scale("+rww/300+","+rww/300+");-moz-transform-origin: 0 0;-webkit-transform: scale("+rww/300+","+rww/300+");-webkit-transform-origin: 0 0;-o-transform: scale("+rww/300+","+rww/300+");-o-transform-origin: 0 0;-ms-transform: scale("+rww/300+","+rww/300+");-ms-transform-origin: 0 0;}", 0 );
      sheet.insertRule( "#super-banner div { height:"+(90*rww/300)+"px; width:"+728+"px;}", 0 );
    } else {
      sheet.insertRule( "#super-banner div {-moz-transform: scale("+ww/728+","+ww/728+");-moz-transform-origin: 0 0;-webkit-transform: scale("+ww/728+","+ww/728+");-webkit-transform-origin: 0 0;-o-transform: scale("+ww/728+","+ww/728+");-o-transform-origin: 0 0;-ms-transform: scale("+ww/728+","+ww/728+");-ms-transform-origin: 0 0;}", 0 );
