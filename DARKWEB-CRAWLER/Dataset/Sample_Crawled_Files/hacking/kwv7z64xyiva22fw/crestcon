        "featureType": "water",
        "elementType": "all",
        "stylers": [
            {
                "hue": "#a1cdfc"
            },
            {
                "saturation": 30
            },
            {
                "lightness": 49
            }
        ]
    }
]
                };

var mapElement = document.getElementById('map');
        var map = new google.maps.Map(mapElement, mapOptions);
        var marker = new google.maps.Marker({
            position: myLatlng,
            map: map,
            icon: 'http://www.crestandiisp.com/wp-content/themes/crestiisp/img/mapicon.png'
        });        

            }

        </script>  <div id="map"></div>
