<div class='widget_item'>
<!-- ID: 2338 -->
<!-- TPL: common/mod/mod_widget_tags_portal_slider.tpl -->
	

<div id="inpage_nav_2338" class="widgetItem widget_pagebuilder_portal_slider widget_pagebuilder_portal_slider_2338 "
style="
					">

	
	

		
	
								
	
	<script language="JavaScript" type="text/JavaScript">
	$(document).ready(function() { 

		// breakpoint where swiper will be destroyed and rebuilt
		var breakpoint = window.matchMedia("(max-width: 768px)");
		
		// swiper instances
		var mySwiperMobile_2338;
		var mySwiperDesktop_2338;
		
		var breakpointChecker = function() {
			if ( breakpoint.matches === true ) { // create mobile slider

                    if ( mySwiperDesktop_2338 !== undefined ) mySwiperDesktop_2338.destroy(true, true);
                    return enableSwiperMobile();
                    

			} else { // create desktop slider
					
                    if ( mySwiperMobile_2338 !== undefined ) mySwiperMobile_2338.destroy(true, true);
                    return enableSwiperDesktop();
                    

			}
		};
		
		// ********* MOBILE SLIDER *********
		var enableSwiperMobile = function() {
				mySwiperMobile_2338 = $('#widgetSlide_2338 .swiper-container').swiper({
                    speed: 1000,
                                            loop: true,
                    										slidesPerView: 1,
					autoHeight: true,
					// watchSlidesVisibility: true, ---> enable if lazyLoading "true" and slidesPerView "auto" or slidesPerView > 1
					nextButton: "#widgetSlide_2338 .sliderArrow .next",
					prevButton: "#widgetSlide_2338 .sliderArrow .prev",
					pagination: '#widgetSlide_2338 .sliderPagination',
					paginationClickable: true,
					preloadImages: false, // disable preloading of all images
					lazyLoading: true, // enable lazy loading
                    lazyLoadingInPrevNext: true, //lazy loading for the closest slides images
                                            onSlideChangeStart: function(swiper) {
                            
                            var prevIndex = swiper.previousIndex,
                                prevVideoElm = $(swiper.slides[prevIndex]).find("video"),
                                prevVideoObj = prevVideoElm.get(0);

                            var curIndex = swiper.activeIndex,
                                curVideoElm = $(swiper.slides[curIndex]).find("video"),
                                curVideoObj = curVideoElm.get(0);

                            if (typeof prevVideoObj !== "undefined") {
                                prevVideoObj.pause();
                            }

                            if (typeof curVideoObj !== "undefined") {
                                curVideoObj.play();
                            }
                            
                        }
                                    });
                
                
				
								/**** max viewport height / else see css for height ****/
					
					
					function set_mobileWidgetSlide_2338_height() {
						
						var minHeight = 300;
                        var windowHeight = $(window).height();
						
						$("#widgetSlide_2338 .item").each(function(){
							var $elm = $(this);
							var $imageBox = $(this).find(".contentImage");
							var contentHeight = $(this).find(".contentTableWrapper").height();
							var imageHeight = windowHeight - contentHeight;
							
							if (imageHeight < minHeight) imageHeight = minHeight;
							$elm.height(imageHeight);
							$imageBox.height(imageHeight);
						});

						mySwiperMobile_2338.update();
							
					}

					$(window).on("resize", function(){ set_mobileWidgetSlide_2338_height(); });
					$(document).ready(function(){ set_mobileWidgetSlide_2338_height(); });

						};

		// ********* DESKTOP SLIDER *********
		var enableSwiperDesktop = function() {
			mySwiperDesktop_2338 = $('#widgetSlide_2338 .swiper-container').swiper({
					speed: 1000,
                                            loop: true,
                    					effect: "fade",
										slidesPerView: 1,
					// watchSlidesVisibility: true, ---> enable if lazyLoading "true" and slidesPerView "auto" or slidesPerView > 1
					nextButton: "#widgetSlide_2338 .sliderArrow .next",
					prevButton: "#widgetSlide_2338 .sliderArrow .prev",
					pagination: '#widgetSlide_2338 .sliderPagination',
					paginationClickable: true,
					preloadImages: false, // disable preloading of all images
					lazyLoading: true, // enable lazy loading
                    lazyLoadingInPrevNext: true, //lazy loading for the closest slides images
                                            onSlideChangeStart: function(swiper) {
                            
                            var prevIndex = swiper.previousIndex,
                                prevVideoElm = $(swiper.slides[prevIndex]).find("video"),
                                prevVideoObj = prevVideoElm.get(0);

                            var curIndex = swiper.activeIndex,
                                curVideoElm = $(swiper.slides[curIndex]).find("video"),
                                curVideoObj = curVideoElm.get(0);

                            if (typeof prevVideoObj !== "undefined") {
                                prevVideoObj.pause();
                            }

                            if (typeof curVideoObj !== "undefined") {
                                curVideoObj.play();
                            }
                            
                        }
                                        
				});

				

				
								/**** max viewport height / else see css for height ****/
					
					function set_widgetSlide_2338_height() {
						var minHeight = 300;
						var itemHeight = $(window).height();
					
						if (itemHeight < minHeight) itemHeight = minHeight;
					
						$("#widgetSlide_2338 .item").height(itemHeight);
					}
					$(window).on("resize", function(){ set_widgetSlide_2338_height(); });
					$(document).ready(function(){ set_widgetSlide_2338_height(); });
						};
		
		
        var vidcount = $("#widgetSlide_2338 video").length;
        if (vidcount > 0) objectFitVideos();
        
		// listen on viewport size changes
		breakpoint.addListener(breakpointChecker);
		// init
        breakpointChecker();

	});
	</script>
	
					
	
