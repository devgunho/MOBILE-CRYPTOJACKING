form.radiusr.value = perRound(radius);
form.diameterr.value = perRound(diameter);
form.circumferencer.value = perRound(circumference);
form.strutar.value = perRound(struta);
form.strutbr.value = perRound(strutb);
form.strutcr.value = perRound(strutc);
form.strutdr.value = perRound(strutd);
form.struter.value = perRound(strute);
form.strutfr.value = perRound(strutf);
form.ff.value = perRound(ff);
form.asteel.value = perRound(asteel);
form.bsteel.value = perRound(bsteel);
form.csteel.value = perRound(csteel);
form.dsteel.value = perRound(dsteel);
form.esteel.value = perRound(esteel);
form.fsteel.value = perRound(fsteel);
form.aemtsteel.value = perRound(aemtsteel);
form.bemtsteel.value = perRound(bemtsteel);
form.cemtsteel.value = perRound(cemtsteel);
form.demtsteel.value = perRound(demtsteel);
form.eemtsteel.value = perRound(eemtsteel);
form.femtsteel.value = perRound(femtsteel);
form.aaluminum.value = perRound(aaluminum);
form.baluminum.value = perRound(baluminum);
form.caluminum.value = perRound(caluminum);
form.daluminum.value = perRound(daluminum);
form.ealuminum.value = perRound(ealuminum);
form.faluminum.value = perRound(faluminum);
form.masteel.value = perRound(masteel);
form.mbsteel.value = perRound(mbsteel);
form.mcsteel.value = perRound(mcsteel);
form.mdsteel.value = perRound(mdsteel);
form.mesteel.value = perRound(mesteel);
form.mfsteel.value = perRound(mfsteel);
form.maemtsteel.value = perRound(maemtsteel);
form.mbemtsteel.value = perRound(mbemtsteel);
form.mcemtsteel.value = perRound(mcemtsteel);
form.mdemtsteel.value = perRound(mdemtsteel);
form.meemtsteel.value = perRound(meemtsteel);
form.mfemtsteel.value = perRound(mfemtsteel);
form.maaluminum.value = perRound(maaluminum);
form.mbaluminum.value = perRound(mbaluminum);
form.mcaluminum.value = perRound(mcaluminum);
form.mdaluminum.value = perRound(mdaluminum);
form.mealuminum.value = perRound(mealuminum);
form.mfaluminum.value = perRound(mfaluminum);
form.plywood.value =  perRound(plywood);
form.slabfour.value =  perRound(slabfour);
form.slabsix.value =  perRound(slabsix);
form.slabeight.value =  perRound(slabeight);
form.cyslabfour.value =  perRound(cyslabfour);
form.cyslabsix.value =  perRound(cyslabsix);
form.cyslabeight.value =  perRound(cyslabeight);
form.diamweight.value =  perRound(diamweight);
form.surfarea.value =  perRound(surfarea);
form.costmshrinkwrap.value =  perRound(costmshrinkwrap);
form.costishrinkwrap.value =  perRound(costishrinkwrap);
form.costaluminet.value =  perRound(costaluminet);
form.costpo1ya.value =  perRound(costpo1ya);
form.costpo1yb.value =  perRound(costpo1yb);
form.costcanvasa.value =  perRound(costcanvasa);
form.costcanvasb.value =  perRound(costcanvasb);
form.costtyvek.value =  perRound(costtyvek);
form.costaluminet.value = perRound(costaluminet);
form.costcottona.value =  perRound(costcottona);
form.costplywood.value =  perRound(costplywood);
form.costplycover.value =  perRound(costplycover);
form.costosb.value =  perRound(costosb);
form.costwstruta.value =  perRound(costwstruta);
form.costwstrutb.value =  perRound(costwstrutb);
form.costwstrutc.value =  perRound(costwstrutc);
form.sradius.value = radius;
form.sradiusr.value = perRound(radius);
form.sdiameter.value = diameter;
form.sdiameterr.value = perRound(diameter);
form.ssarea.value = (2 * PI * (radius * radius));
form.ssarear.value = perRound((2 * PI * (radius * radius)));
form.svolume.value = (4/3 * PI * (radius * radius * radius));
form.svolumer.value = perRound((4/3 * PI * (radius * radius * radius)));
form.sstruta.value = radius * .19814743;
form.sstrutar.value = perRound(.19814743);
form.sstrutb.value = radius * .23179025;
form.sstrutbr.value = perRound(.23179025);
form.sstrutc.value = radius * .22568578;
form.sstrutcr.value = perRound(.22568578);
form.sstrutd.value = radius * .24724291;
form.sstrutdr.value = perRound(.24724291);
form.sstrute.value = radius * .25516701;
form.sstruter.value = perRound(radius * .25516701);
form.sstrutf.value = radius * .24508578;
form.sstrutfr.value = perRound(radius * .24508578);
form.ff.value = radius * .24534642;
form.ff.value = perRound(radius * .24534642);
  }
}

var toDegrees = 360 / (Math.PI * 2);
var toRadians = (Math.PI * 2) / 360;
</script>





</head>



