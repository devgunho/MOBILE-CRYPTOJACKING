if (area > 0) {
var radius = sqrt(area / 3.0902);
diameter = 2 * radius;
circumference = PI * diameter;
struta = radius * .25318;
strutb = radius * .29524;
strutc = radius * .29453;
strutd = radius * .31287;
strute = radius * .32492;
strutf = radius * .29859;
