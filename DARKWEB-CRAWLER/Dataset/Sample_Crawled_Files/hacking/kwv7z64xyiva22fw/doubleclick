    var PREBID_TIMEOUT = 700;
    var FAILSAFE_TIMEOUT = 700;

    var pbjs = pbjs || {};
    pbjs.que = pbjs.que || [];

    pbjs.que.push(function() {
        window.pbjs.setConfig({
            currency: {
                adServerCurrency: 'EUR'
            },
            priceGranularity: 'high'
        });
        pbjs.addAdUnits(adUnits);
        pbjs.requestBids({
            bidsBackHandler: initAdserver,
            timeout: PREBID_TIMEOUT
        });
    });

    function initAdserver() {
        if (pbjs.initAdserverSet) return;
        pbjs.initAdserverSet = true;
        googletag.cmd.push(function() {
            pbjs.que.push(function() {
                pbjs.setTargetingForGPTAsync();
                googletag.pubads().refresh();
            });
        });
    }
    // in case PBJS doesn't load
    setTimeout(function() {
        initAdserver();
    }, FAILSAFE_TIMEOUT);

    function shuffle(array) {
      var idx = Math.floor(Math.random() * array.length);
      return array[idx];
    }

    function consentFeatureIsEnabled() {
        return document.documentElement.hasAttribute('data-consentmanagement');
    }

    function getConsentStatus(consentName) {
        var consents = JSON.parse(localStorage.usercentrics || window.usercentrics).consents || [];
        var consentStatus = consents
            .filter(function(consent) {
                return consent.dataProcessors
                    .filter(function(processor) {
                        return processor === consentName;
                }).length > 0;
            })[0]
            .consentStatus;
        return consentStatus;
    }

    function displayAds(personalized) {

        googletag.cmd.push(function() {

            if (personalized) {
                googletag.pubads().setRequestNonPersonalizedAds(0);
            } else {
                googletag.pubads().setRequestNonPersonalizedAds(1);
            }
                googletag.defineSlot("/6514/www.heise.de/extras/partnerzone", [[468,60],[728,90],[800,250],[960,250],[970,250],[1104,90]], "div-gpt-ad-2")
                    
                        .addService(googletag.pubads())
                            .setTargeting('ad_group', Adomik.randomAdGroup())
                            .setTargeting('ad_h', (new Date).getUTCHours()).setTargeting("pos", ["top"]);
            
                googletag.defineSlot("/6514/www.heise.de/extras/partnerzone", [[120,600],[120,800],[160,600],[160,800],[200,600],[200,800]], "div-gpt-ad-3")
                    
                        .addService(googletag.pubads())
                            .setTargeting('ad_group', Adomik.randomAdGroup())
                            .setTargeting('ad_h', (new Date).getUTCHours()).setTargeting("pos", ["top"]);
            
                googletag.defineSlot("/6514/www.heise.de/extras/partnerzone", [[300,250],[300,600]], "div-gpt-ad-4")
                    
                        .addService(googletag.pubads())
                            .setTargeting('ad_group', Adomik.randomAdGroup())
                            .setTargeting('ad_h', (new Date).getUTCHours()).setTargeting("pos", ["top"]);
            
                googletag.defineSlot("/6514/www.heise.de/extras/partnerzone", [[300,250],[300,600],[500,500],[695,110],"fluid"], "div-gpt-ad-1")
                    
                    .setForceSafeFrame(true)
                    
                        .addService(googletag.pubads())
                            .setTargeting('ad_group', Adomik.randomAdGroup())
                            .setTargeting('ad_h', (new Date).getUTCHours()).setTargeting("textad", ["3"]).setTargeting("pos", ["bottom"]).setTargeting("mpos", ["bottom"]);
            

            googletag.pubads().setTargeting("themenhub", "yes");
            googletag.pubads().setTargeting(
                "test", "test_" + shuffle(["a", "b", "c", "d", "e", "f", "g", "h", "i", "j"])
            );
            googletag.pubads().collapseEmptyDivs(true);

            googletag.pubads().disableInitialLoad();
            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
        });

    }

    if (consentFeatureIsEnabled()) {
        if (localStorage.usercentrics || window.usercentrics) {
            try {
                var personalizationConsent = getConsentStatus('Google Ad Manager');
                displayAds(personalizationConsent);
            } catch (error) {
                displayAds(false, false);
            }
        } else {
            displayAds(false, false);
        }
    } else {
        displayAds(true, true);
    }

    var gallery_adUnit = "/6514/www.heise.de/extras/partnerzone";
