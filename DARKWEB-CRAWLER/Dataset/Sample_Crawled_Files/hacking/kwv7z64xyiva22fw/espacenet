<script type="text/javascript">
    $(document).ready(function () {
        updateParamsFromHash();
    	dataloader.loadDataPageForUrlWithHost(dataFeedRequestHost, window.location.pathname + window.location.search, dataFeedRequestExtraParams, onDataLoaded);
    });

    function onDataLoaded(data){
        updateContent(data);
        espacenetReady();
        mainReady();
        documentDetailsReady(onDataLoaded);

        contentReady();
        initpage();
        addHashToURL(data);
        updateLinksInLanguageNavigator(data);
    }

    var baseOriginalDocumentPageUrl;
    var firstPageClippingUrl;
    var firstPageClippingNotAvailable;

    function contentReady() {
        baseOriginalDocumentPageUrl = $('input[name=sourcePdfLink]').val();
        if ($('input[name=firstPageClippingUrl]')){
            firstPageClippingUrl = $('input[name=firstPageClippingUrl]').val();
        }
        firstPageClippingNotAvailable = $('input[name=firstPageClippingNotAvailable]').val()=='true'?true:false;
        //alert("baseOriginalDocumentPageUrl: "+baseOriginalDocumentPageUrl);
        loadPageByIndex($('input[name=pageIndex]')?parseInt($('input[name=pageIndex]').val(),10):0);
    }


</script>
