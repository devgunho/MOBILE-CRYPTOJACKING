<script>
var h, w, needresize;

jQuery(document).ready(checkContainer);

function checkContainer () {
  if(!jQuery('div.fb-video.fb_iframe_widget.fb_iframe_widget_fluid_desktop iframe').contents().find('body').is(':visible')) {
    window.requestAnimationFrame(checkContainer);
   } else {
   createResize();
  }
}

function createResize() {	
jQuery('div.fb-video.fb_iframe_widget.fb_iframe_widget_fluid_desktop span iframe').each(function(i, obj) {
	var checkExist = setInterval(function() {
		h = jQuery('div.fb-video.fb_iframe_widget.fb_iframe_widget_fluid_desktop span iframe')[i].style.height;
		w = jQuery('div.fb-video.fb_iframe_widget.fb_iframe_widget_fluid_desktop span iframe')[i].style.width;
		h = h.replace("px", "");
		w = w.replace("px", "");
		w = parseInt(w);
		h = parseInt(h);

			if (h > 0) {
		      if (h > 478) {
				needresize = true;
				jQuery('div.fb-video.fb_iframe_widget.fb_iframe_widget_fluid_desktop')[i].style.margin= 'auto';
				jQuery('div.fb-video.fb_iframe_widget.fb_iframe_widget_fluid_desktop')[i].style.width = '350px';
				var cnfrm = jQuery('div.fb-video.fb_iframe_widget.fb_iframe_widget_fluid_desktop')[i].style.margin;
				} else {
				needresize = false;
				}
			clearInterval(checkExist);
			}
	}, 100); // check every 100ms
});
}
</script>	</div>

