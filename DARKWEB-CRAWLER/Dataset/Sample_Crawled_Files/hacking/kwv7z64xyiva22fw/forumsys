		jQuery(document).ready(function() {
			jQuery(".sentry.tab_content").hide(); 
			jQuery("#sentry.tabs li:first").addClass("active").show(); 
			jQuery(".sentry.tab_content:first").show(); 
			jQuery("#sentry.tabs li").click(function() {
				jQuery("#sentry.tabs li").removeClass("active");
				jQuery(this).addClass("active"); 
				jQuery(".sentry.tab_content").hide(); 
				var activeTab = jQuery(this).find("a").attr("href"); 
				jQuery(activeTab).fadeIn(); 
				return false;
			});	
		});
