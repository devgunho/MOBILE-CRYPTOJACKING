<!-- pagetitle start here -->
<section id="pagetitle-container" style="background-repeat: no-repeat; background:url(https://www.forumsys.com/wp-content/uploads/2014/02/Page-Header-default1.jpg)">
	<div class="row"> 
				<div class="twelve column pagetitle">
		<h1>Blog</h1>		</div>
	</div>	      
