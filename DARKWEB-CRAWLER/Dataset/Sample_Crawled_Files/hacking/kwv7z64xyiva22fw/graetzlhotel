                    if(originalBookingFormRight == 0) {
                        originalBookingFormRight = $('#bookingform').css('right');
                        console.log(originalBookingFormRight);
                    }
                    
				    if (parseInt($('#bookingform').css('right'), 10) < 0) {
				      $('#bookingform').css('right', 0);
				      $('a.openbook i').removeClass("fa-caret-up").addClass("fa-caret-down");
				    } else {
				      $('#bookingform').css('right', originalBookingFormRight);
				      $('a.openbook i').removeClass("fa-caret-down").addClass("fa-caret-up");
