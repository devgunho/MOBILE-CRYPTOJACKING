            var currentImage = 0;
            var totalImages = 4;
            var timeout = 5000;
            setTimeout(setNextHeroImage, timeout);
            
            function setNextHeroImage() {
                currentImage++;
                if(currentImage >= totalImages) {
                    currentImage = 0;
                }
                //console.log(heroElements[currentImage]);
                if(heroElements[currentImage]['backgroundImage']) {
                    $('#headerSlider').css('background-image', "url('" + heroElements[currentImage]['backgroundImage'] + "')");
                    if(heroElements[currentImage]['backgroundColor']) {
                        $('#headerSlider').css('background-color', heroElements[currentImage]['backgroundColor']);
                    }
                }
                else {
                    $('#headerSlider').css('background-image', "none");
                    console.log(heroElements[currentImage]);
                    if(heroElements[currentImage]['backgroundColor']) {
                        $('#headerSlider').css('background-color', heroElements[currentImage]['backgroundColor']);
                    }
                }
                
                if(heroElements[currentImage]['teaserText']) {
                    $('#headerSlider .teasertext').html('<p class="large">' + heroElements[currentImage]['teaserText'] + '</p>');                }
                else {
                    $('#headerSlider .teasertext').html('');
                }
            
                if(heroElements[currentImage]['color']) {
                    $('#headerSlider').removeClass('menu-dark').removeClass('menu-light').removeClass('menu-weiss').removeClass('menu-schwarz');
                    $('#headerSlider').addClass(heroElements[currentImage]['color']);
                    
                    if(heroElements[currentImage]['color'] == 'menu-light') {
                        $('#urbanauts-logo').attr('src','https://www.graetzlhotel.com/wp-content/themes/urbanauts/images/Urbanauts_Logo_white.svg');
                        $('#theme-logo').attr('src','/wp-content/themes/urbanauts/images/graetzlhotel_white.svg');
                    }
                    else if(heroElements[currentImage]['color'] == 'menu-weiss') {
                        $('#urbanauts-logo').attr('src','https://www.graetzlhotel.com/wp-content/themes/urbanauts/images/Urbanauts_Logo_white.svg');
                        $('#theme-logo').attr('src','/wp-content/themes/urbanauts/images/graetzlhotel_white.svg');
                    }
                    else if(heroElements[currentImage]['color'] == 'menu-schwarz') {
                        $('#urbanauts-logo').attr('src','https://www.graetzlhotel.com/wp-content/themes/urbanauts/images/Urbanauts_Logo_black.svg');
                        $('#theme-logo').attr('src','/wp-content/themes/urbanauts/images/graetzlhotel_schwarz.svg');
                    }
                    else {
                        $('#urbanauts-logo').attr('src','https://www.graetzlhotel.com/wp-content/themes/urbanauts/images/Urbanauts_Logo_black.svg');
                        $('#theme-logo').attr('src','/wp-content/themes/urbanauts/images/graetzlhotel_schwarz.svg');
                    }
                }

                setTimeout(setNextHeroImage, timeout);
            }
            
                        
