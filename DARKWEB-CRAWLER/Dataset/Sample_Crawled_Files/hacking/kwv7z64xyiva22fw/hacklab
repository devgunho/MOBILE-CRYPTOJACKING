document.addEventListener("DOMContentLoaded", function() {
    function demangle(s) { return s.replace(/\.POISTATAMA/g, ""); }

    Array.prototype.forEach.call(
        document.querySelectorAll("a[href^='mailto:'], span.roskaposti"),
        function(el) {
            el.innerText = demangle(el.innerText)

            var href = el.getAttribute("href");
            if (href)
                el.setAttribute("href", demangle(href));
        });
});
</script>

