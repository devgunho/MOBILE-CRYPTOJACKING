        jQuery(window).resize(function() {
          wHeight = window.innerHeight;
          $('#sb-searchform').css('top', wHeight / 2);
        });
        // Search
        $('#search-button').click(function() {
          console.log("Open Search, Search Centered");
          $("div.sb-search-overlay").addClass("sb-search-overlay-show");
        });
        $("a.sb-close").click(function() {
          console.log("Closed Search");
          $("div.sb-search-overlay").removeClass("sb-search-overlay-show");
        });

	if (localStorage.sb_calendar_promo_code == 'undefined') localStorage.sb_calendar_promo_code = '';
	   

  
	// ****************************************** //
