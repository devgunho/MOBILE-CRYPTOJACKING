function vh_room_des() {
	$(".media-md .sb-tpl-content-rooms .sb-packages .sb-package, .media-lg .sb-tpl-content-rooms .sb-packages .sb-package, .media-xl .sb-tpl-content-rooms .sb-packages .sb-package").each(function(){
 			var vh = ($(this).find(".sb-photo-gallery").height()-$(this).find(".sb-hea").height()-125);
			$(this).find(".sb-des").css("height", vh);
	});
}
