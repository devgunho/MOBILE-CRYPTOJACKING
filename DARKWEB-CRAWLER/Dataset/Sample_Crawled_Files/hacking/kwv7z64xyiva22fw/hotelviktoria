    color: #333;
}

.btn-success {
    font-family: georgia;
    text-transform: uppercase;
    font-weight: bold;
    border-color: #8d7243;
}

.btn-primary {
    background-color: transparent;
    border-color: transparent;
}

.btn-success {
    background: #a79757;
    /* Old browsers */
    background: -moz-linear-gradient(top, #a79757 0%, #a79757 46%, #a79757 89%);
    /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #a79757), color-stop(46%, #a79757), color-stop(89%, #a79757));
    /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #a79757 0%, #a79757 46%, #a79757 89%);
    /* Chrome10+,Safari5.1+ */
