    <script type="text/javascript">
        function LoadPageData() {
            $(".btn-more-botom").hide();
            var url = "";
            var ret = /\?sc_itemid/;
            var path = window.location.href;
            var Count = 0
            if (ret.exec(path) == "?sc_itemid") {
                url = path + "&d=ws";
            } else {
                url = "?d=ws";
            }

            //url = "?d=ws";
            //å·²å è½½çæ°é
            if ($(".row.publications .col-sm-2.col-xs-6").length >= 18) {
                Count = $(".row.publications .col-sm-2.col-xs-6").length;
                url = url + "&hasCount=" + Count;
                $.ajax({
                    url: url,
                    type: "POST",
                    beforeSend: function () {
                        $("#waiting").css("display", "block");
                    },
                    success: function (html) {
                        $(".btn-more-botom").show();
                        if ($($.trim(html)).find(".col-sm-2.col-xs-6").length < 18) {
                            $(".btn-more-botom").hide();
                        }
                        $(".row.solCategory .col-sm-12").append(html);
                        if ($(".col-sm-2.col-xs-6").length >= $("#CountId").val()) {
                            $(".btn-more-botom").hide();
                        }
                        $("#waiting").css("display", "none");
                    },
                    error: function (responseText) {

                    }
                });
            } else {
                Count = $(".segment.previous .row .col-sm-6").length
                url = url + "&hasCount=" + Count;
                $.ajax({
                    url: url,
                    type: "POST",
                    beforeSend: function () {
                        $("#waiting").css("display", "block");
                    },
                    success: function (html) {
                        $(".btn-more-botom").show();
                        if ($($.trim(html)).find(".item.col-sm-6").length < 10) {
                            $(".btn-more-botom").hide();
                        }
                        $(".segment.previous").append(html);
                        if ($(".segment.previous .row .col-sm-6").length >= $("#CountId").val()) {
                            $(".btn-more-botom").hide();
                        }
                        $("#waiting").css("display", "none");
                    },
                    error: function (responseText) {

                    }
                });
            }
        }

        $(document).ready(function () {

            if ($(".row.publications .col-sm-2.col-xs-6").length < 18 && $(".segment.previous .row .col-sm-6").length < 8) {
                $(".btn-more-botom").hide();
            }

            //ç¹å»âå è½½æ´å¤â
            $(".btn-more-botom").click(function () {
                LoadPageData();
            });
        });
