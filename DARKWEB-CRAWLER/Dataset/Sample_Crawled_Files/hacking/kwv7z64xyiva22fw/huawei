        $(function () {
            $(".removeWH").removeAttr("width").removeAttr("height");
            $(".removeImgWH").removeAttr("width").removeAttr("height");
            var listImg = $(".removeImgWH");
            listImg.each(function (i) {
                var strSrc = listImg[i].src;
                var path = strSrc.split('?');
                listImg[i].src = path[0];
            });
        });
        function IsPageEditor() {
            if (typeof Sitecore == "undefined") {
                return false;
            }
            if (typeof Sitecore.PageModes == "undefined" || Sitecore.PageModes == null) {
                return false;
            }
            return Sitecore.PageModes.PageEditor != null;
        }
        $(".row .col-xs-7 h3 a").each(function (i) {
            //debugger;
            if ($.trim($(this).attr("href")) == "" && IsPageEditor() == false) {
                var xx = $(this).html();
                $(this).replaceWith(xx);
            }

        })
        $(".row .col-sm-3 h3 a").each(function (i) {
            //debugger;
            if ($.trim($(this).attr("href")) == "" && IsPageEditor() == false) {
                var xx = $(this).html();
