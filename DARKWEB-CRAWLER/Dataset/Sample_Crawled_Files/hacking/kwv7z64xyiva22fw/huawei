            }

        })
        window.onload = function () {
            if ($.trim($(".moreinfo").html()).length == 0) {
                $("#js_read_more_product").hide();
            }
            $(".lt-img-rt-tx .container .more a").each(function (i) {
                if ($.trim($(this).attr("href")) == "" && IsPageEditor() == false) {
                    $(".lt-img-rt-tx .container .more").hide();
                }
                else {
                    $(".lt-img-rt-tx .container .more").show();
                }
            })
        };
