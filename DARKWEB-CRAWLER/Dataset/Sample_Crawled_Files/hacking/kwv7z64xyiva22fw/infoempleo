        if (typeof result.responseJSON != 'undefined'
            && typeof result.responseJSON.lightbox != 'undefined') {
            //App.Lightbox.hide('Login');
            App.Lightbox.show(result.responseJSON.lightbox);
        }
    }
    function LoginFailure(xhr, status, error) {
        window.location.replace("/infoempleo9/500.html");
    }
