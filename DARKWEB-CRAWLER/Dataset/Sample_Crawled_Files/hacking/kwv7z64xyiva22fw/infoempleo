            var app = Voonto.getInstance();

            // consent information is empty, set non personalized ads
            app.when('consent:empty').then(function () {
                googletag
                    .pubads()
                    .setRequestNonPersonalizedAds(1)
                    .setTargeting('vfunclog', '')
                    .setTargeting('vedadlog', '')
                    .setTargeting('area', 'empleo')
                    .setTargeting('provbus', '')
                    
                    .collapseEmptyDivs();
            });

            // advertising consent information was found
            app.when('consent:advertising').then(function (result) {
                googletag
                    .pubads()
                    .setRequestNonPersonalizedAds(result ? 0 : 1)
                    .setTargeting('vfunclog', '')
                    .setTargeting('vedadlog', '')
                    .setTargeting('area', 'empleo')
                    .setTargeting('provbus', '')
                    
                    .collapseEmptyDivs();
            });

            // Start ad fetching
            googletag.enableServices();
        });

