                .sizeMapping()
                .addSize([1260, 480], [300, 250]) // robapaginas
                .build()
            ;

            gptAdSlots[0] = googletag
                .defineSlot('/4900/vocento.infoempleo/portada', [[320, 50], [300, 600], [300, 250]], 'megabanner')
                .defineSizeMapping(mapping_megabanner)
                .setCollapseEmptyDiv(true)
                .addService(googletag.pubads())
