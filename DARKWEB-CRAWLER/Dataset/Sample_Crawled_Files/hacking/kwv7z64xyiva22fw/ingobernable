<script>
function laingo_open(url) {

if (window.innerWidth <= 640) {
// if width is smaller then 640px, create a temporary a elm that will open the link in new tab
var a = document.createElement('a');
a.setAttribute("href", url);
a.setAttribute("target", "_blank");

var dispatch = document.createEvent("HTMLEvents");
dispatch.initEvent("click", true, true);

a.dispatchEvent(dispatch);
}
else {
var width = window.innerWidth * 0.66 ;
// define the height in
var height = width * window.innerHeight / window.innerWidth ;
// Ratio the hight to the width as the user screen ratio
window.open(url , 'newwindow', 'width=' + width + ', height=' + height + ', top=' + ((window.innerHeight - height) / 2) + ', left=' + ((window.innerWidth - width) / 2));
}
return false;
}

jQuery(document).ready(function(){

	function resize_items(){
	var ctop = 0, cur = jQuery();
	jQuery('.item:not(.item-type-colectivo)').css({height: 'auto'}).each(function(){
		var top = jQuery(this).offset().top;
		if (ctop && Math.abs(top - ctop) > 15){
		// new line
		apply_same_height(cur);
		cur = jQuery();
		}
		ctop = top;
		   cur = cur.add(jQuery(this));
		   
	});
	apply_same_height(cur);
	function apply_same_height(items){
		if (items.length < 2)
		   return;
		   var maxh = 0;
		 items.each(function(){
		 var h = jQuery(this).outerHeight();
		   if (!maxh || h > maxh)
		      maxh = h;
		 });
		 items.css({height: maxh});
	}
	}
	resize_items();
	jQuery(window).on('resize', function(){ resize_items(); });

	jQuery('.okulec-sblocks-props-1-2').each(function(){
		var bl = jQuery('.okulec-sblock');
		var minh = 0;
		bl.each(function(){
			var h = jQuery(this).height();
			if (!minh || h < minh)
			   minh = h;
		});
		
//		bl.css({height: minh, overflow: 'hidden'});
		bl.find('.item-next-events > div').each(function(){
			if (jQuery(this).position(jQuery(this).closest('.item-next-events')).top > minh)
			   jQuery(this).hide();
		});
	});

	jQuery('.okulec-featured-slider').each(function(){
	var items = jQuery(this).children('.okulec-tweet');

var item = items.first().show();
var ready = true;
var last_i = 0;

setInterval(function(){
if (!ready)
   return;
   
ready = false;
last_i++;
if (last_i >= items.length)
   last_i = 0;

	var nitem = items.eq(last_i);

//var h = nitem.show().height();
//nitem.hide();
//console.log('step '+1);
item.stop().animate({opacity: 0}, 1000, function(){
//console.log('step '+2);
item.hide();
nitem.css({opacity: 0}).show().stop().animate({opacity: 1}, 1000, function(){

//console.log('step '+3);
  item = nitem;
  
  ready = true;
});

});


}, 10000);

});
});
</script>

