        <!-- ANFANG Link Bestellung Heimatbundle HCS -->


        <!-- ENDE Link Bestellung Heimatbundle HCS -->







        <script type="text/javascript">

          // Wird per PHP gesetzt, wenn ein Tab geöffnet sein soll
          var openTab = '';

          // Schließt alle Tabs bis auf den gewünschten
          var tabs = $$('.payment.tab');
          tabs.each(function(el) {
            if (openTab == '' || (openTab != '' && el.hasClassName(openTab) === false)) {
              el.select('.container').first().hide();
            }
          });

          if (openTab != '') {
            setOpenTabValue(openTab);
          }

        </script>

      </div><!-- content-container oeffnendes div direkt nach erstem skript -->
