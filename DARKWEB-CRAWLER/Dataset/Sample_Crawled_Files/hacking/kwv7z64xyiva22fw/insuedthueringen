    /*
     * Beispielhafte Verwendung von Google-Analytics
     * unter Verwendung des Ajaxlogins / Varnish
     *
     * controller.onLoad(function(userData) {
     *
     *   // Kunde eingeloggt => dimension1 setzen
     *   if (userData && userData.permissions) {
     *     ga('set', 'dimension1', 'eingeloggt');
