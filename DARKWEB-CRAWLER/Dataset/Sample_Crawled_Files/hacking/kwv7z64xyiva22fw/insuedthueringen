	<!-- hier war Aufruf von srp.js -->
    
    <script type='text/javascript'>

      // ermittle Endgeräte-Typ
      var adtnRes_screen_width = typeof document.documentElement != 'undefined' ? document.documentElement.clientWidth : window.innerWidth;
      var dt = 'desktop';
      if (/Mobile/i.test(navigator.userAgent)) { dt = 'phone'; }
      if (/iPad|RIM Tablet|Silk/i.test(navigator.userAgent) ) { dt = 'tablet'; }
      if (/Android/i.test(navigator.userAgent)) {
        if( /Mobile/i.test(navigator.userAgent)) { dt = 'phone'; }
        else { dt = 'tablet'; }
      }

	  var renderedAditionSlots = renderedAditionSlots || [];
      var emptyAditionSlots = [];

      // Liste Profilparameter und AdSlots
      var mws = { };

      /* Liste der an den AdServer zu übergebenen Key-Values */
      mws.pvars = {
        "vpaid": "0",  // Bezahlartikel [Mögliche Werte: 0 oder 1] 0-> freier Artikel; 1 -> bezahlter Artikel
        "device": dt // benutztes Endgerät [mögliche Werte: 'desktop', 'phone', 'tablet']
      };

      /* Zuordnungsliste AdContainer und WerbeplatzIDs
       Diese Liste enthält alle Werbeplätze, die auf der jeweiligen Seite geladen werden sollen */
      mws.adslots = {
        "adition_CA3-1_werb6": "3713318",
        "adition_CA3-1_werb7": "3713319",
        "adition_CA6-1_werb8": "3713320",
        "adition_CA6-1_werb9": "3713321",
        "adition_CA4-1_werb10": "3713324",
        "adition_CA4-1_werb11": "3713325",
        "adition_CA6-1_werb12": "3713322",
        "adition_CA6-1_werb13": "3713323",
        "adition_CA1-1_werb15": "3714786",
      };

      mws.inlineCampaigns = [];

      //Render Event-Callbacks
      function onPreRenderCallback(renderSlot, bannerDescriptionAPI, renderControlAPI) {
        // Aktionen vor dem Rendern eines AdSlots
        var options;

        options = bannerDescriptionAPI.getOptions();
        if (options.kennung && options.kennung == "Anzeige") {
          var e = document.createElement("div");
          e.className = "adWrapper";
          e.innerHTML = '<div class="kennung lightgrey-small">'+options.kennung+'</div>';

          renderControlAPI.addInnerWrapper(e);
        }
      }

      function onPostRenderCallback(renderSlot, bannerDescriptionAPI, renderControlAPI) {
        // Aktionen nach dem Rendern des AdSlots (kann bei bedarf weiter bearbeitet werden)
        renderedAditionSlots.push(renderSlot.id);
      }
      
      function onFinishLoadingCallback(renderSlot, bannerDescriptionAPI, renderControlAPI) {
      	emptyAditionSlots = fcmsJs._.difference(Object.keys(mws.adslots), Object.values(renderedAditionSlots));
        
        for (var i=0, l=emptyAditionSlots.length; i < l; i++) {
        	var sl = document.getElementById(emptyAditionSlots[i]);
        	if (sl) {
            	sl.classList.add('adtnCollapsed');
            }
        }
      }

      // Setup callback queue
      var adition = adition || { };
      adition.srq = adition.srq || [];

      adition.srq.push(function(api) {
        // Configure single request ...
        api.registerAdfarm("adfarm1.adition.com");

        //Setzen der Profilparameter
        var pv = mws.pvars;
        for (var v in pv) {
          api.setProfile(v, pv[v]);
        }

        api.events.onPreRender(onPreRenderCallback);						
        api.events.onPostRender(onPostRenderCallback);
        
        
        // ... render slots ...
        var cs = mws.adslots,
            ic = mws.inlineCampaigns;
        for (var pos in cs) {
          api.configureRenderSlot(pos).setContentunitId(parseInt(cs[pos])).setRenderer(ic.indexOf(pos) > -1 ? 'inline' : 'iframe');
        }

		
        if (adtnRes_screen_width < 801) {
          var aditionSlots = Object.keys(mws.adslots);

          var aditionSlot10 = aditionSlots.indexOf('adition_CA4-1_werb10');
          if (aditionSlot10 > -1) {
              aditionSlots.splice(aditionSlot10, 1);	
          }
          var aditionSlot6 = aditionSlots.indexOf('adition_CA3-1_werb6');
          if (aditionSlot6 > -1) {
              aditionSlots.splice(aditionSlot6, 1);	
          }
        }
		
        if (typeof aditionSlots != 'undefined') {
            api.load(aditionSlots).completeRendering();
        }
        else {        
        	api.load().completeRendering();
        }
        
        api.events.onFinishLoading(onFinishLoadingCallback);

      });
      

     	
    </script>
