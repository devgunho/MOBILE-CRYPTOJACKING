      <script>            
var rsCtrl = false;

function stickySB(rsCtrl) {   
    if (currScrW() > 830) {
      if ( ( ! navigator.userAgent.match(/Android|BlackBerry|iPhone|iPad|iPod|Opera Mini|IEMobile/i) && currScrH() > 700 ) || ( navigator.userAgent.match(/Android|BlackBerry|iPhone|iPad|iPod|Opera Mini|IEMobile/i) && window.matchMedia("(orientation: portrait)").matches ) ) {	
      
        var stickySBsetAttr = function() {
          if ( currSbH() < 120 ) {
            if (document.querySelector('.desktopTop').style.position === 'fixed') {
            	document.querySelector('.desktopTop').style.marginTop = currSbH() + 'px';
            }
            else {
            	document.querySelector('.desktopTop').style.marginTop = '0px';
            }
            
            document.querySelector('.desktopTop').style.top = "0px";
            document.getElementById('header').style.marginTop = currSbH() + 'px'; // "90px";
            document.getElementById('superbanner').style.position = 'fixed';

            if (navigator.userAgent.match(/Trident.*rv\:11\./) != null || navigator.appName === 'Microsoft Internet Explorer') {
              document.getElementById('mainNavigation').style.top = currSbH() + 'px'; // "90px";

              if (document.querySelector('.desktopTop').style.position === 'static') {
                  document.getElementById('mainNavigation').style.top = "0px";
              }
            } 
            return;
          }
        }

        var obs_navbar = new MutationObserver(function(mutations) {
            mutations.forEach(function(mutation) {
                if (mutation.target.style.position === "fixed") {
                    if ( currSbH() < 120 ) {
                        stickySBsetAttr();
                    }
                }
                else if (mutation.target.style.position === "relative") {
                	document.querySelector('.desktopTop').style.marginTop = "0px";
                }
            });
        });
        obs_navbar.observe(document.getElementById('mainNavigation'), { attributes: true, subtree: true } );

        var superbannertarget = document.querySelector('#superbanner div');
        var superbannerobserver = new MutationObserver(function(mutations) {
            mutations.forEach(function(mutation) {
                if (mutation.type === 'childList' && currSbH() < 120) {
                    document.getElementById('superbanner').style.position = 'fixed';
                    document.getElementById('header').style.marginTop = currSbH() + 'px';
                }
            });
        });    
        superbannerobserver.observe(superbannertarget, { childList: true, subtree: true } );
        
        if (rsCtrl) {
            stickySBsetAttr();
        }
      }
      else {
      	  document.querySelector('.desktopTop').style.top = "0px";
          document.querySelector('.desktopTop').style.marginTop = "0px";
          document.getElementById('header').style.marginTop = '0px';
          document.getElementById('superbanner').style.position = 'static';
          if (navigator.userAgent.match(/Trident.*rv\:11\./) != null || navigator.appName === 'Microsoft Internet Explorer') {
            document.getElementById('mainNavigation').style.top = "0px";
          }

      }
    } // /if >830 desktop oder tablet portrait

	else if (currScrW() > 744) {
		if ( (navigator.userAgent.match(/Android|BlackBerry|iPhone|iPad|iPod|Opera Mini|IEMobile/i) && window.matchMedia("(orientation: portrait)").matches) || currScrH() > 700 ) {

          var stickySBsetAttr = function() {
          	if ( currSbH() < 120 ) {
              document.getElementById('superbanner').style.position = 'fixed';
              document.querySelector('.desktopTop').style.marginTop = currSbH() + 'px';
              document.querySelector('.mm-menu > .mm-panel.mm-opened').style.top = document.querySelector('.desktopTop').offsetTop + "px";
              return;
           	}
          }
			
          var obs_navbar = new MutationObserver(function(mutations) {
              mutations.forEach(function(mutation) {
                  if (mutation.target.style.position === "fixed") {
                    document.getElementById('header').style.marginTop = currSbH() + 'px';
                    document.querySelector('.desktopTop').style.top = "0px";       
                  }
              });
          });
          obs_navbar.observe(document.getElementById('mobileNavi'), { attributes: true } );  
            
          var superbannertarget = document.querySelector('#gpt-1-1554363103');
          var superbannerobserver = new MutationObserver(function(mutations) {
              mutations.forEach(function(mutation) {
                  if (mutation.type === 'childList') {
                  	stickySBsetAttr();
                  }
              });
          });    
          superbannerobserver.observe(superbannertarget, { childList: true, subtree: true } );
          
          if (rsCtrl) {
            stickySBsetAttr();
          }
    	}
        else {
        	document.getElementById('superbanner').style.position = 'static';
            document.querySelector('.desktopTop').style.marginTop = "0px";
            document.querySelector('.mm-menu > .mm-panel.mm-opened').style.top = document.querySelector('.desktopTop').offsetTop + "px";
        }
    } // /else if >744 tablet portrait
   
	else if (currScrW() > 640 && currScrW() < 745 ) {
		if ( (navigator.userAgent.match(/Android|BlackBerry|iPhone|iPad|iPod|Opera Mini|IEMobile/i) && window.matchMedia("(orientation: portrait)").matches) || currScrH() > 700 ) { 
          
          var stickySBsetAttr = function() {
            if ( currSbH() < 120 ) {
              document.getElementById('superbanner').style.position = 'fixed';
              document.querySelector('.desktopTop').style.marginTop = currSbH() + 'px';
              document.querySelector('.mm-menu > .mm-panel.mm-opened').style.top = document.querySelector('.desktopTop').offsetTop + "px";
              return;
            }
          }
          
          var obs_navbar = new MutationObserver(function(mutations) {
              mutations.forEach(function(mutation) {
                  if (mutation.target.style.position === "fixed") {
                    document.getElementById('header').style.marginTop = currSbH() + 'px';
                    document.querySelector('.desktopTop').style.top = "0px";       
                  }
              });
          });
          obs_navbar.observe(document.getElementById('mobileNavi'), { attributes: true } );
          
          var superbannertarget = document.querySelector('#pos1');
          var superbannerobserver = new MutationObserver(function(mutations) {
              mutations.forEach(function(mutation) {
                  if (mutation.type === 'attributes' && mutation.attributeName === 'data-google-query-id') {
                      stickySBsetAttr();
                  }
              });
          });    
          superbannerobserver.observe(superbannertarget, { attributes: true } );
          
          if (rsCtrl) {
            stickySBsetAttr();
          }
  		}
        else {
        	document.getElementById('superbanner').style.position = 'static';
            document.querySelector('.desktopTop').style.marginTop = '0px';
            document.querySelector('.mm-menu > .mm-panel.mm-opened').style.top = document.querySelector('.desktopTop').offsetTop + "px";
        }
    } // /else if >640 <745 tablet portrait
    
    return;
}    




document.getElementById('superbanner').addEventListener('metaTagSlotDone', function() {
	console.log('sb loaded');
  	stickySB(true);
})

	window.addEventListener("resize", function(){stickySB(true)}, true);
    window.addEventListener("scroll", function(){stickySB(true)}, true);

