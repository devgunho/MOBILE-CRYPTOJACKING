<script type="text/javascript">
  fcmsJs.load('extlib_jquerymmenu').onLoad(function() {
    jQuery('#mymenu').mmenu({
      "slidingSubmenus": false,
      "iconPanels": true,
    }, {
      offCanvas : {
        pageSelector: "#mypage"
      }
    });
    var API = jQuery("#my-menu").data( "mmenu" );

    jQuery("#my-button").click(function() {
      API.close();
    });
  });

</script>


