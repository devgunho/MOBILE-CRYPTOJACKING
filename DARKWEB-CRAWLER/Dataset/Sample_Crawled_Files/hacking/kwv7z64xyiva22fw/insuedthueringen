  fcmsJs.load('fcmsUser_Customer').onLoad(function () {

    // Parameter: Name des aktuellen Templates, Eindeutige Id für das Div
    var controller = new fcmsUser_Customer_LoginLogout_Controller('kunde-login-logout-ajax.tpl', '9f0d122b7ed16f14fa51c1724852e1c7');
    controller.onLoad(function(userData, ssoPermaCookie) {
        /*
         * externer Auto-Login über SSO, kann wahlweise als zweiten Parameter den Response von ssoPermaCookie oder den Cookie-Namen entgegennehmen
         * ssoPermaCookie = PHP-Repsonse über die Existenz des Cookies aus Provider permaCookieName
         * Cookie-Name = JS-Auslesen des Cookies
         */
        // fcmsUser_Customer_Sso.tryExtLogin(userData, ssoPermaCookie, '');
    });

    /*
     * Beispielhafte Verwendung von Google-Analytics
     * unter Verwendung des Ajaxlogins / Varnish
     *
     * controller.onLoad(function(userData) {
     *
     *   // Kunde eingeloggt => dimension1 setzen
     *   if (userData && userData.permissions) {
     *     ga('set', 'dimension1', 'eingeloggt');
     *
     *     // Kunde mit Permission X => dimension2 setzen
     *     if (userData.permissions.indexOf('X') !== -1) {
     *       ga('set', 'dimension2', 'permission X');
     *     }
     *   }
     *
     *   // Seitenaufruf tracken
     *   ga('send','pageview');
     * });
     */

    if (typeof fcmsJs.ajaxLoginControllers === 'undefined') {
      fcmsJs.ajaxLoginControllers = [];
    }

    fcmsJs.ajaxLoginControllers.push(controller);
