        box-sizing:border-box;
        width:100%;
        max-width:1000px;        
    }
    #cookiehinweis a.button { cursor: pointer; margin-left:5px; margin-bottom:5px; float: right; }
    #cookiehinweis p.cookiemessage {display: block; padding: 0; margin: 0;}
    </style>
<div id="cookiehinweis-wrapper">
    <div id="cookiehinweis">
    <a href="/mehr/recht/datenschutz/" class="button" style="margin:0 4px 0 4px;">Ändern</a>

    <a onClick="var d = new Date(); d = new Date(d.getTime() +1000*60*60*24*730); document.cookie = 'cookiehinweis=1; expires='+ d.toGMTString() + ';path=/'; document.getElementById('cookiehinweis-wrapper').style.display = 'none';" class="button">Einverstanden</a>

<p class="cookiemessage">Diese Webseite nutzt Cookies für Funktions-, Statistik- und Werbezwecke. In unserer <a class="more" href="/mehr/recht/datenschutz/"><span class="raq">&raquo;</span> Datenschutzerklärung</a> können Sie die Cookie-Einstellungen ändern. Wenn Sie der Verwendung von Cookies zustimmen, klicken Sie bitte &quot;Einverstanden&quot;.</p>
    </div>
</div>

    <script>
function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

(function() {
	var cookiehinweis = getCookie("cookiehinweis");
    if (cookiehinweis != "") {
        document.getElementById('cookiehinweis-wrapper').style.display = 'none';
    }
})(); 

if ( navigator.userAgent.match(/Android|BlackBerry|iPhone|iPad|iPod|Opera Mini|IEMobile/i) ) {
    document.getElementById('cookiehinweis').style.zIndex = '121';
    document.getElementById('cookiehinweis').style.padding = '6px 6px 4px 6px';
}
    </script>
    <!-- Cookie-Bar End -->

  </body>
</html>



