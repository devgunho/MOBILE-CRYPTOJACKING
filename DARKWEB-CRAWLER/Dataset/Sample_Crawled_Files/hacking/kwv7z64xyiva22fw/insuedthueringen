<script type="text/javascript">

  function setOpenTabValue(tab) {
    // entsprechende Inputs mit dem tab befüllen, damit dieser Tab wieder angezeigt werden kann
    var openTabInputs = $$('input[name="payment[opentab]"]');
    openTabInputs.each(function(el) {
      el.setValue(tab);
    })
  }

  // Schaltet einen Tab sichtbar/unsichtbar
  function toggleTab(tab) {

    var tabs = $$('.payment.tab');
    tabs.each(function(el) {

      // den tab-Tab aufklappen
      if (el.hasClassName(tab) === true) {
        el.select('.container').first().toggle();

        // die restlichen Tabs zuklappen
      } else {
        el.select('.container').first().hide();
      }
    });

    // setzt den Namen des offenen Tabs als input-value
    setOpenTabValue(tab);
  }


  function switchLoginRegisterTab() {

    var buttons   = $$('.payment.tab .container input.button');
    var tabs      = $$('.anmeldung-registrierung .tabs div');
    var bs        = $$('.anmeldung-registrierung .tabs b');
    var container = $$('.anmeldung-registrierung div.container');

    buttons.each(switchActiveInactive);
    tabs.each(switchActiveInactive);
    bs.each(switchActiveInactive);
    container.each(switchActiveInactive);

  }

  function switchActiveInactive(el) {

    if (el.hasClassName('aktiv') || el.hasClassName('inaktiv')) {
      el.toggleClassName('aktiv');
      el.toggleClassName('inaktiv');
    }
  }



  function calculatePrice(id) {
    var priceOne      = parseFloat($(id).select('.preis-erste_ausgabe')[0].getValue());
    var priceMore     = parseFloat($(id).select('.preis-weitere_ausgaben')[0].getValue());
    var discountPrice = parseFloat($(id).select('.preis-erste_ausgabe_mit_rabatt')[0].getValue());

    var price = 0.00;
    $(id).select('.checkbox-ausgabe').each(function(issue) {
      if (issue.checked == true) {
        if (price == 0.00) {
          if (isNaN(discountPrice) == false) {
            price = discountPrice;
          } else {
            price = priceOne;
          }
        } else {
          price = price + priceMore;
        }
      }
    });

    price = Math.round((price+0.00001)*100)/100;

    $(id).select('.ausgabe-gesamtpreis')[0].innerHTML = returnMoney(price);
  }

  function setDiscountPrice(id, price, checkbox) {
    var container = $(id);

    if (container == null) {
      return false;
    }
    if (checkbox != undefined && $(checkbox).checked === false ) {
      container.select('.preis-erste_ausgabe_mit_rabatt')[0].setValue('');
    } else {
      container.select('.preis-erste_ausgabe_mit_rabatt')[0].setValue(price);
    }
    calculatePrice(id);
  }


  function returnMoney(number) {
    var nStr = '' + parseFloat(number);
    var x = nStr.split('.');
    var x1 = x[0];

    var x2 = ',00';
    if (typeof x[1] !== 'undefined') {
      if (x[1].length == 1) {
        x2 = ','+x[1]+'0';
      } else {
        x2 = ','+x[1];
      }
    }

    return x1 + x2;
  };

</script>

