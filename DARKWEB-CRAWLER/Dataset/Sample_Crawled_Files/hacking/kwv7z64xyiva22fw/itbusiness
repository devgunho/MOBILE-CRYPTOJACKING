<script>
	$(document).scroll(function(){
    // If has not activated (has no attribute "data-top"
    if (!$('.top-nav').attr('data-top')) {
        // If already fixed, then do nothing
        if ($('.top-nav').hasClass('navbar-fixed-top')) return;
        // Remember top position
        var offset = $('.top-nav').offset()
        $('.top-nav').attr('data-top', offset.top);
    }

    if ($('.top-nav').attr('data-top') - $('.top-nav').outerHeight() <= $(this).scrollTop())
        $('.top-nav').addClass('navbar-fixed-top');
    else
        $('.top-nav').removeClass('navbar-fixed-top');
});
</script>

