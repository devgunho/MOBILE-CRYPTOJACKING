    <script type="text/javascript"
            src="https://maps.googleapis.com/maps/api/js?v=3.exp&key=AIzaSyCnJUFnp7aIXpa8I-msbawB_GF-uxSeSyw&libraries=places"></script>

    <script type="text/javascript">
        var $searchForm = $('[data-google-autocomplete]');
        var $searchInput = $searchForm.find('input');
        var searchDestination = $searchForm.attr('action');

        var autocomplete = new google.maps.places.Autocomplete(
            $searchInput[0],
            {
                componentRestrictions: {
                    country: "de"
                }
            }
        );

        google.maps.event.addListener(autocomplete, 'place_changed', function () {
            top.location.href = searchDestination + '?q=' + encodeURIComponent($searchInput.val());
        });
    </script>
