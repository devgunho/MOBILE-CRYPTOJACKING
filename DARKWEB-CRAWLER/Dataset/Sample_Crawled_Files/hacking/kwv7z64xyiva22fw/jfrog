        var CaptchaCallback = function () {
            jQuery('.g-recaptcha').each(function (index, el) {

                var id = jQuery(this).parents('.component_form');
                var has_server = jQuery(this).parents('.has-server');
                if (id.length) {
                    // buy now has server
                    if (jQuery(this).parents('.has-server').length) {

                        var widgetId = grecaptcha.render(el,
                            {
                                'sitekey': jfrog_general.jf_recaptcha,
                                'size': 'invisible',
                                'callback': function () {
                                    has_server.find('#saas_server_exist_btn').trigger('click');
                                }
                            });

                        jQuery(this).attr('data-widget-id', widgetId);

                    } else {
                        // component forms
                        var widgetId = grecaptcha.render(el,
                            {
                                'sitekey': jfrog_general.jf_recaptcha,
                                'size': 'invisible',
                                'callback': function () {
                                    id.find('.jpc-form .jpc-next-btn').trigger('click');
                                }
                            });

                        jQuery(this).attr('data-widget-id', widgetId);
                    }

                } else {
                    // hosted forms
                    var id = jQuery(this).parents('form').find('button[type="submit"]');

                    var widgetId = grecaptcha.render(el,
                        {
                            'sitekey': jfrog_general.jf_recaptcha,
                            'size': 'invisible',
                            'callback': function () {
                                id.trigger('click');
                            }
                        });
                    jQuery(this).attr('data-widget-id', widgetId);

                }

            });
        };
    </script>
