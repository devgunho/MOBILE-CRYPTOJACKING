    <script>
        (function(window, document) {
            // cookie reader by Lea Verou: http://lea.verou.me/2009/12/reading-cookies-the-regular-expression-way/
            var cookieManager = {};
            var cookieDomain = '.keysight.com';

            cookieManager.getCookie = function getCookie(name) {
                // Escape regexp special characters
                name = name.replace(/([.*+?^=!:()|[\]\/\\])/g, '\\$1');

                const regex = new RegExp('(?:^|;)\\s?' + name + '=(.*?)(?:;|$)', 'i'),
                    match = document.cookie.match(regex);

                return match && unescape(match[1]);
            };

            cookieManager.setCookie = function setCookie(name, value, expireTime) {
                document.cookie = name + '=' + value + '; path=/' + '; domain=' + cookieDomain + '; expires=' + expireTime;
            };

            var userCredentialsCookie = cookieManager.getCookie('USER_CREDENTIALS');
			var chkPassive = cookieManager.getCookie('chkPassive');
            var loggedInCookie = cookieManager.getCookie('IS_LOGGED_IN');
            var returnCookie = cookieManager.getCookie('AEMReturnURL');

            // Check if the USER_CREDENTIALS cookie is set to valid and we don't have an AEM login token.
            if (userCredentialsCookie && userCredentialsCookie.toLowerCase() === 'valid' && !loggedInCookie && !returnCookie) {
                // Create return URL cookie.
                var milsToMin = 1000 * 60;
                var expireTime = new Date((5 * milsToMin) + Date.now()).toUTCString()
                cookieManager.setCookie('AEMReturnURL', window.location.href, expireTime);

                // Redirect to passive SAML page.
                window.location = 'https:\/\/www.keysight.com\/us\/en\/passive\u002Dsaml.login.html';
            } else if (chkPassive && chkPassive === 'true' && !loggedInCookie && !returnCookie) {
				var milsToMin = 1000 * 60;
                var expireTime = new Date((5 * milsToMin) + Date.now()).toUTCString()
                cookieManager.setCookie('AEMReturnURL', window.location.href, expireTime);

                // Redirect to passive SAML page.
                window.location = 'https:\/\/www.keysight.com\/main\/acomauth?env=https:\/\/www.keysight.com\/us\/en\/passive\u002Dsaml.login.html';
			}
        })(window, document);
    </script>




<script type="text/javascript">
