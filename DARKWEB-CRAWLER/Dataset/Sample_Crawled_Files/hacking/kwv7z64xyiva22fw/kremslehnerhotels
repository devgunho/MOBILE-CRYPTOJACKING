<script>

  var traslapoTimer;

    $(".taphover").mouseenter(function() {
    if(traslapoTimer) clearTimeout(traslapoTimer);
    traslapoTimer = setTimeout(function(){$(".compact-menu .sb-traslapo").fadeIn('fast');},100);
  });
  $(".taphover").mouseleave(function() {
    if(traslapoTimer) clearTimeout(traslapoTimer);
    traslapoTimer = setTimeout(function(){$(".sb-traslapo").fadeOut('fast');},100);
  });


// ****************************************** //
//  GET IE VERSION                          * //
// ****************************************** //
function GetIEVersion() {
	var sAgent = window.navigator.userAgent;
	var Idx = sAgent.indexOf("MSIE");

	// If IE, return version number.
	if (Idx > 0){
		return parseInt(sAgent.substring(Idx+ 5, sAgent.indexOf(".", Idx)));
	}

	// If IE 11 then look for Updated user agent string.
	else if (!!navigator.userAgent.match(/Trident\/7\./)){
		return 11;
	} else {
		return 0; //It is not IE
	}
}


// ****************************************** //
// EQUAL HEIGHTS							* //
// ****************************************** //
$.fn.equalHeights = function(){
	return this.height( Math.max.apply(this, $.map( this , function(e){ return $(e).height() }) ) );
}
// ****************************************** //
// CHECK POSITIONS							* //
// ****************************************** //
var lastScrollTop = 0;
$(window).scroll(function(event){
	st = $(this).scrollTop();
	if (st > 150){
		if (st > lastScrollTop){
			$( '.media-xs .sb-footer-buttons').removeClass('sb-show');
		} else {
			$( '.media-xs .sb-footer-buttons').addClass('sb-show');
		}
		lastScrollTop = st;
	}
});
function check_pos() {
	if(!$('body').hasClass("sb-tpl-location")){
		var st = $(this).scrollTop();
		var mc = ($('body').hasClass("sb-spbg-menu"))?$('.sb-main-content').offset().top:350;
		if(st > mc - 40) {$('body').addClass("compact-menu");} else {$('body').removeClass("compact-menu");}
		if($('body').hasClass("sb-spbg")){
			var mc2 = $('.sb-main-content').offset().top;
			//console.log("mc2:"+mc2+", st:"+st);
			if(st > mc2 - 188) {$('body').addClass("fixed-be");} else {$('body').removeClass("fixed-be");}
		}
		if($('body').hasClass("sb-tpl-reviews")){
			var bottom_of_window = $(window).scrollTop() + $(window).height();
			var footerTOP = $(".sb-footer.sb-footer-bottom").offset().top;
			if(bottom_of_window < footerTOP){$('body').addClass("show-up-arrow");} else {$('body').removeClass("show-up-arrow");}
		}
		
	}



	/*if (mc-st < $(window).height()/2){
	
		$('.sb-spbg .sb-header').removeClass('sb-big');
	} else {
		
		$('.sb-spbg .sb-header').addClass('sb-big');
	}*/
}
$(window).load(function() {
  fixSuperBGMenu();
  check_pos();
  fixRowBannersHeights();
  fixReviews();
  fixFirstBlock();
  fixOffersCarouselHeader();
  fixGalleryTitles();
});


function fixFirstBlock(){
	if($(".col-first-block .sb-box-shadow .sb-summary-first-block").length>0 && $(".col-reviews .sb-box-shadow").length>0){
		if(SB.media != 'xs' && SB.media != 'sm'){
			var h = $(".col-reviews .sb-box-shadow").height();
			$(".col-first-block .sb-box-shadow .sb-summary-first-block").height(h);
		} else {
			$(".col-first-block .sb-box-shadow .sb-summary-first-block").height(300);
		}
		
	}
}

function fixGalleryTitles(){
	if($(".sb-tpl-gallery .sb-img-title").length>0){
		if(SB.media == 'xs'){
			$(".sb-tpl-gallery .sb-img-title").height('auto');
		} else {
			$(".sb-tpl-gallery .sb-img-title").height('auto');
			$(".sb-tpl-gallery .sb-img-title").equalHeights();
		}
	}
}

function fixOffersCarouselHeader(){
	if($(".sb-offers-block-top").length>0){
		var t = $(".sb-offers-block-top").width();
		var s = $(".sb-hea-offers > span").width();
		var m = 35;
		var w = (t-s)*0.5-m;
		if(w>0){
			$(".sb-hea-offers-right").width(w);
			$(".sb-hea-offers-left").width(w);
		}
	}
}

function fixRowBannersHeights(){
	if($(".row.sb-container-banners").length>0){
		$(".row.sb-container-banners").each(function(){
			$(this).find(".sb-hea").height('auto');
			$(this).find(".sb-bod").height('auto');
			$(this).find(".sb-hea").equalHeights();
			$(this).find(".sb-bod").equalHeights();
		});
	}
	if($(".sb-first-package-block").length>0){
		if(SB.media == 'xs'){
			$(".sb-first-package-block .sb-col-img").height('auto');
		} else {
			var h1 = $(".sb-first-package-block .sb-col-img").height();
			var h2 = $(".sb-first-package-block .sb-col-txt").height();
			if(h2 > h1)  $(".sb-first-package-block .sb-col-img").height(h2);
		}
	}
}

if($("html").hasClass("sb-banners-voucher")){
	$(".sb-featured-blocks").appendTo(".sb-text-center-headings ");
}

function BrowserDetection() {

	$("html").addClass("noie");
	//Check if browser is IE or not
	if (navigator.userAgent.search("MSIE") >= 0) {
		$("html").addClass("ie");
		$("html").removeClass("noie");
	}
	//Check if browser is IE 11
	else if (!!navigator.userAgent.match(/Trident\/7\./)) {
		$("html").addClass("ie11");
		$("html").addClass("ie");
		$("html").removeClass("noie");
	}
	//Check if browser is Chrome or not
	else if (navigator.userAgent.search("Chrome") >= 0) {$("html").addClass("chrome");}
	//Check if browser is Firefox or not
	else if (navigator.userAgent.search("Firefox") >= 0) {$("html").addClass("firefox");}
	//Check if browser is Safari or not
	else if (navigator.userAgent.search("Safari") >= 0 && navigator.userAgent.search("Chrome") < 0) {$("html").addClass("safari");}
	//Check if browser is Opera or not
	else if (navigator.userAgent.search("Opera") >= 0) {$("html").addClass("opera");}
	else {$("html").addClass("other");}


}

