<script type="text/javascript">
    (function(){
        // hasProperty :: a -> String -> Bool
        const hasProperty = obj => key => Object.prototype.hasOwnProperty.call(obj, key);

        // isObject :: a -> Bool
        const isObject = val => typeof val === 'object';

        // merge :: a -> b -> c
        const merge = obj1 => (obj2) => {
            const obj = Object.assign({}, obj1);
            let key;
            for (key in obj2) {
                if (key === '__proto__' && !hasProperty(obj1)(key)) {
                    continue;
                }

                let oldVal = obj1[key];
                let newVal = obj2[key];

                if (Array.isArray(oldVal) && Array.isArray(newVal)) {
                    obj[key] = oldVal.concat(newVal);
                } else if (isObject(oldVal) && isObject(newVal)) {
                    obj[key] = merge(oldVal)(newVal);
                } else {
                    obj[key] = newVal;
                }
            }

            return obj;
        };


        var config = merge({
            html_content: {
                subTitle : 'Your membership helps us deliver great news and content.',
                formButton: 'Join Now',
            },
            ga: { location: '7671' },
        })({html_content:{ title: 'Join LiveScience'}});

        var gtmdl = window.gtmDataLayer ||  window.dataLayer || [];
        Purch.EMAIL_MODAL_CONF = config;

        (function(url) { var s=document.createElement('script');s.type='text/javascript';s.defer=true;document.getElementsByTagName('head')[0].appendChild(s);s.src = url;
        })('//assets.purch.com/creative-templates/2.0.1-15/emailModal.js?t='+(new Date()).getTime());
    })();
</script>
