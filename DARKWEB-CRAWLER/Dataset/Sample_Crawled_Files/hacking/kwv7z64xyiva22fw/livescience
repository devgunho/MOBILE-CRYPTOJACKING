    if (slot && soConf.productsCond === true) {
      try {
        slot.insertAdjacentHTML(soConf.insertMethod, styles + "<div class='fct-text'>" + soConf.sentence + "</div>");
      } catch (e) {}
    }
  }

  document.addEventListener('readystatechange', function() {
    if (document.readyState === "interactive") {
      init();
    }
  });
})();
