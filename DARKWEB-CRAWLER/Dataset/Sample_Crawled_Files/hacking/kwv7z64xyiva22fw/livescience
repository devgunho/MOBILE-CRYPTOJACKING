        <script type="text/javascript">
        var omnVars = {};
        for (var key in s) {
            if (!s.hasOwnProperty(key)) continue;
            var obj = s[key];
            if (key.indexOf('prop') == 0 || key.indexOf('eVar') == 0 || key.indexOf('pageName') == 0 || key.indexOf('channel') == 0 || key.indexOf('pageType') == 0 || key.indexOf('contentID') == 0) {
                omnVars[key] = typeof s[key] !== 'undefined' ? s[key] : '';
            }
        }
    </script>

