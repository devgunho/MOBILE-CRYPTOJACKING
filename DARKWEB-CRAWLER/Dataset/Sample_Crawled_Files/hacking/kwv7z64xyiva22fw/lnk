    function levelIsImpliedConsent() {
        return window.linkfire.consent.minLevel.toLowerCase() === 'implied';
    }

    function loadAlreadyConsented() {
        Object.keys(window.linkfire.trackingPixels).forEach(function(key) {
            if (checkConsent('performance', key)) {
                loadScript('performance', key);
            }
        });
        Object.keys(window.linkfire.retargetingPixels).forEach(function(key) {
            if (checkConsent('advertiser', key)) {
                loadScript('advertiser', key);
            }
        });
    }

        function checkConsent(area, provider) {
        switch (area) {
            case 'performance':
                                if (Object.keys(window.linkfire.consent.performance).length === 0) {
                                        if (levelIsImpliedConsent()) {
                        return true;
                    }
                                } else {
                                        if (provider in window.linkfire.consent.performance) {
                        return window.linkfire.consent.performance[provider];
                    } else {
                                                if (levelIsImpliedConsent()) {
                            return window.linkfire.consent.performance[Object.keys(window.linkfire.consent.performance)[0]];
                        }
                    }
                }
                break;
            case 'advertiser':
                                if (Object.keys(window.linkfire.consent.advertiser).length === 0) {
                    if (levelIsImpliedConsent()) {
                        return true;
                    }
                } else {
                    if (provider in window.linkfire.consent.advertiser) {
                        return window.linkfire.consent.advertiser[provider];
                    } else {
                        if (levelIsImpliedConsent()) {
                            return window.linkfire.consent.advertiser[Object.keys(window.linkfire.consent.advertiser)[0]];
                        }
                    }
                }
                break;
        }

        return false;
    }

    function checkScriptLoaded(area, provider) {
        switch (area) {
            case 'performance':
                if (typeof window.linkfire.trackingPixels !== 'undefined') {
                    if (provider in window.linkfire.trackingPixels && window.linkfire.trackingPixels[provider].isLoaded == true)Â {
                        return true;
                    }
                }
                break;
            case 'advertiser':
                if (typeof window.linkfire.retargetingPixels !== 'undefined') {
                    if (provider in window.linkfire.retargetingPixels && window.linkfire.retargetingPixels[provider].isLoaded == true)Â {
                        return true;
                    }
                }
                break;
        }
        return false;
    }

    function setScriptLoaded(area, provider) {
        switch (area) {
            case 'performance':
                window.linkfire.trackingPixels[provider].isLoaded = true;
                break;
            case 'advertiser':
                window.linkfire.retargetingPixels[provider].isLoaded = true;
                break;
        }
    }

    function loadScript(area, provider) {
        try {
            if (!checkScriptLoaded(area, provider)) {
                switch (area) {
                    case 'performance':
                        if (typeof window.linkfire.trackingPixels !== 'undefined') {
                            if (provider in window.linkfire.trackingPixels && typeof window.linkfire.trackingPixels[provider].loader === 'function' && !window.linkfire.trackingPixels[provider].isLoaded)Â {
                                window.linkfire.trackingPixels[provider].loader();
                            }
                        }
                        break;
                    case 'advertiser':
                        if (typeof window.linkfire.retargetingPixels !== 'undefined') {
                            if (provider in window.linkfire.retargetingPixels && typeof window.linkfire.retargetingPixels[provider].loader === 'function' && !window.linkfire.retargetingPixels[provider].isLoaded)Â {
                                window.linkfire.retargetingPixels[provider].loader();
                            }
                        }
                        break;
                }
                setScriptLoaded(area, provider);
            } else {
    //            console.log(area + ':' + provider + ' already loaded');
            }
        } catch (err) {
        }
    }

   /**
    * Set cookie
    * @param {String} name (required)
    * @param {String} value (required)
    * @param {Date} expires
    * @param {String} path
    * @param {String} domain
    * @param {Boolean} secure
    */
    function setCookieExBase(name, value, expires, path, domain, secure) {
        value = escape(value);
        var theCookie = name + '=' + value +
        ((expires) ? '; expires=' + expires.toUTCString() : '') +
        ((path) ? '; path=' + path : '') +
                // Domain won't be added on IE
        ((domain) ? '; domain=' + domain : '') +
                ((secure) ? '; secure' : '');
        document.cookie = theCookie;
    }

    function setcookieEx(cname, cvalue, days) {
        var d = new Date((new Date()).getTime() + (days * 1000 * 60 * 60 * 24)); // days added as ms
        setCookieExBase(cname, cvalue, d, '/', window.linkfire.consent.domain);
    }

    function setcookieExM(cname, cvalue, minutes) {
        var d = new Date((new Date()).getTime() + (minutes * 1000 * 60)); // minutes added as ms
        setCookieExBase(cname, cvalue, d, '/', window.linkfire.consent.domain);
    }

    function setLFCookies() {
        // Set linkfire VT & UID cookies
        Object.keys(window.linkfire.settings).forEach(function(key) {
            if (key.indexOf('LF_visitor') !== -1 || key.indexOf('LF_session') !== -1) {
                setcookieExM(key, window.linkfire.settings[key], 10);
            } else {
                setcookieEx(key, window.linkfire.settings[key], 180);
            }
        });
    }

    if (window.linkfire.trackingPixels.linkfire != undefined) {
        window.linkfire.trackingPixels.linkfire.loader = function() {
            setLFCookies();
        }
    }

    function addTrackingImage(imageUrl) {
        // Add image to tracking container
        $('#trackingcontainer').append($('<img>', {
            src : imageUrl,
            width : 1,
            height : 1,
        }));
    }
    function addTrackingIframe(iframeUrl) {
        // Add image to tracking container
        $('#trackingcontainer').append($('<iframe>', {
            src : iframeUrl,
            width : 1,
            height : 1,
        }));
    }
    function addTrackingScript(scriptSrc) {
        // Add image to tracking container
        $('#trackingcontainer').append($('<script>', {
            src : scriptSrc,
            width : 1,
            height : 1,
        }));
    }

    function trackConsent(baseUrl) {
        var trackingUrl = baseUrl;
        trackingUrl += '&consentLevel=' + window.linkfire.consent.level;
        trackingUrl += '&consentDialogType=' + window.linkfire.consent.dialogType;
        trackingUrl += '&consentPerformance=' + encodeURI(JSON.stringify(window.linkfire.consent.performance));
        trackingUrl += '&consentAdvertiser=' + encodeURI(JSON.stringify(window.linkfire.consent.advertiser));

        addTrackingImage(trackingUrl);
    }
</script>
<script>
            (function(w,d,e,n,s,c,a,f){w['LnkConsentObject']=n;w[n]=w[n]||function(){
            (w[n].q=w[n].q||[]).push(arguments)},w[n].c=c;a=d.createElement(e),
            f=d.getElementsByTagName(e)[0];a.async=1;a.src=s;f.parentNode.insertBefore(a,f)
            })(window,document,'script','lnk_consent','https://static.assetlab.io/consent/1.1.4/consent.js','https://static.assetlab.io/consent/1.1.4/consent.css');

            lnk_consent('init', {
                level: window.linkfire.consent.minLevel,
                consentedLevel: window.linkfire.consent.level,
                dialogType: window.linkfire.consent.dialogType,
                consentedRequired: window.linkfire.consent.required,
                consentedPerformance: window.linkfire.consent.isConsentedToLevel ? window.linkfire.consent.performance : {},
                consentedAdvertising: window.linkfire.consent.isConsentedToLevel ? window.linkfire.consent.advertiser : {},
                providerListRequired: window.linkfire.requiredCookies,
                providerListPerformance: ["google","linkfire"],
                providerListAdvertising: ["facebook","google","twitter","mediarithmics"],
                controller: window.linkfire.controller
            });

           /**
            * Callback for when the Explicit Consent dialog is displayed on load
            */
            lnk_consent('on', 'dialog_view', function() {
                trackConsent(window.linkfire.consent.tracking.dialogView);
            });

            /**
             * Callback when user opts in
             * @param {String[]} consentedPerformance list of performance cookies the user is consenting to
             * @param {String[]} consentedAdvertising list of advertising cookies the user is consenting to
            */
            lnk_consent('on', 'opt_in', function(consentedPerformance, consentedAdvertising) {
                window.linkfire.consent.level = window.linkfire.consent.minLevel;
                setcookieEx('LF_level', window.linkfire.consent.minLevel, 180)
                Object.keys(consentedPerformance).forEach(function(key) {
                    // if value changed from false to true, fire pageview pixels if not already

                    if (consentedPerformance[key] == true) {
                        if (key in window.linkfire.consent.performance && window.linkfire.consent.performance[key] == false || !(key in window.linkfire.consent.performance) && consentedPerformance[key] == true) {
                            loadScript('performance', key);
                        }
                    }

                    if (key == "linkfire" && consentedPerformance[key] == true) {
                        setLFCookies();
                    }
                });

                window.linkfire.consent.performance = consentedPerformance;
                setcookieEx('LF_performance', JSON.stringify(window.linkfire.consent.performance), 180);

                Object.keys(consentedAdvertising).forEach(function(key) {
                    if (consentedAdvertising[key] == true) {
                        if (key in window.linkfire.consent.advertiser && window.linkfire.consent.advertiser[key] == false || !(key in window.linkfire.consent.advertiser) && consentedAdvertising[key] == true) {
                            loadScript('advertiser', key);
                        }
                    }
                });

                window.linkfire.consent.advertiser = consentedAdvertising;
                setcookieEx('LF_advertiser', JSON.stringify(window.linkfire.consent.advertiser), 180);

                trackConsent(window.linkfire.consent.tracking.optin);
            });

            /**
             * Callback when user updated their cookie preferences
             * @param {String[]} consentedPerformance list of performance cookies the user is consenting to
             * @param {String[]} consentedAdvertising list of advertising cookies the user is consenting to
            */
            lnk_consent('on', 'update_consent', function(consentedPerformance, consentedAdvertising) {
                setcookieEx('LF_level', window.linkfire.consent.minLevel, 180)

                Object.keys(consentedPerformance).forEach(function(key) {
                    if (consentedPerformance[key] == true) {
                        if (key in window.linkfire.consent.performance && window.linkfire.consent.performance[key] == false || !(key in window.linkfire.consent.performance) && consentedPerformance[key] == true) {
                            loadScript('performance', key);
                        }
                    }
                    if (key == "linkfire") {
                        if (consentedPerformance[key] == true) {
                            setLFCookies();
                        } else {
                            var cookies = getCookies();
                            Object.keys(cookies).forEach(function(key) {
                                if (key.indexOf('LF_prefPlayer') === -1 &&Â key.indexOf('LF_session') === -1 &&Â key.indexOf('LF_performance') === -1 && key.indexOf('LF_advertiser') === -1 && key.indexOf('LF_level') === -1) {
                                    setcookieEx(key, '', -1);
                                }
                            });
                        }
                    }
                });

                window.linkfire.consent.performance = consentedPerformance;
                setcookieEx('LF_performance', JSON.stringify(window.linkfire.consent.performance), 180)

                Object.keys(consentedAdvertising).forEach(function(key) {
                    if (consentedAdvertising[key] == true) {
                        if (key in window.linkfire.consent.advertiser && window.linkfire.consent.advertiser[key] == false || !(key in window.linkfire.consent.advertiser) && consentedAdvertising[key] == true) {
                            loadScript('advertiser', key);
                        }
                    }
                });

                window.linkfire.consent.advertiser = consentedAdvertising;
                setcookieEx('LF_advertiser', JSON.stringify(window.linkfire.consent.advertiser), 180)

                if (
                    Object.keys(window.linkfire.consent.performance).every(function(name) { return !window.linkfire.consent.performance[name]; }) &&
                    Object.keys(window.linkfire.consent.advertiser).every(function(name) { return !window.linkfire.consent.advertiser[name]; })
                ) {
                    trackConsent(window.linkfire.consent.tracking.optout);
                } else {
                    trackConsent(window.linkfire.consent.tracking.advanced);
                }
            });

            /**
             * Callback when user opens cookie preferences/info modal
             */
            lnk_consent('on', 'open_preferences', function() {
                // trackConsent(window.linkfire.consent.tracking.openPref);
            });

            /**
             * Callback when user closes cookie preferences/info modal
             */
            lnk_consent('on', 'close_preferences', function() {
                // trackConsent(window.linkfire.consent.tracking.closePref);
            });

            /**
             * Callback when user dismiss the initial dialog
             */
            lnk_consent('on', 'dismiss', function() {
                trackConsent(window.linkfire.consent.tracking.dismiss);
            });
        </script>
