function handleVisibilityChange(){
    if(document.visibilityState == "visible" && !tracked){
        $('#trackingcontainer').append($('<img>', {
                    src : "https://st.linkfire.com/Api/Click/click/4350cdad6dadc80ab4932cdd5aa7c80b/415640?boardUuid=476519a6-6ef0-4295-93f8-dcd09e24f749&linkUuid=5a50bfe1-014d-48cd-aa81-7e1b49bc3176&ouuid=93babeae-9602-409a-8936-13b6e8cfedd1&tlouuid=93babeae-9602-409a-8936-13b6e8cfedd1&country=Unknown&visitorToken=4350cdad6dadc80ab4932cdd5aa7c80b&boardId=17277&uid=5ca595b34b0698.66878737&shortlink=colors.lnk.to%2FSpotify&referrer=&linkChannel=0&url=https%3A%2F%2Fopen.spotify.com%2Fuser%2Fcolorsxstudios%3Fsi%3DlqbNb7_aRkWRzpsKkb-YXw&artists=&tags=&orgId=11968&browser=Python-urllib&device=crawler&os=unknown&querystring=&city=Unknown&activity=&channelType=Original&channelId=&organisation=COLORS+Media&artist=&album=&track=&label=&board=COLORS&genre=&countryName=Unknown&puuid=850ed79a-df27-46a2-b629-c281d2bab6bb&sourcetype=landingpage&linkId=415640&sourcetype=landingpage&newSession=" + session + "",
