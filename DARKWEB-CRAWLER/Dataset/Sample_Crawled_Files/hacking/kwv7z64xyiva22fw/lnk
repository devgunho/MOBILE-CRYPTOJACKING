  <script type="text/javascript">
    // Avoid namespace polution
    var linkfireDeeplink = {
      clicked: false,
      iframeTarget: document.getElementById('detectURI'),
      isDesktop: false,
      services: document.getElementsByClassName('redirect'),
      visited: 0
    };

    // Attach event listeners to all anchors
    for (var i = 0; i < linkfireDeeplink.services.length; i++) {
      linkfireDeeplink.services[i].addEventListener('click', redirectHandler, false);
    }

    function redirectHandler(e) {
      e.preventDefault();
      if(linkfireDeeplink.clicked) return false;

      // set user has clicked a service
      linkfireDeeplink.clicked = true;
      // Reset after 3sec
      setTimeout(function() {
          linkfireDeeplink.clicked = false;
      },3000);
      linkfireDeeplink.visited = 0;

      if(e.target.hasAttribute('href')){
        var link = e.target;
      }else{
        var link = this;
      }

      var uri = link.getAttribute('data-uri');

            var url = link.getAttribute('href');
      var player = link.getAttribute('data-player');
      var appType = link.getAttribute('data-apptype');
      var serviceType = link.getAttribute('data-serviceType');
      var eventShowId = link.hasAttribute('data-showid') ? link.getAttribute('data-showid') : null;
      var eventlabel = 'unknown';
      if(link.children[1]){
        eventlabel = link.children[1].textContent;
      }

      if(!appType || appType.length == 0){
          appType = 'manual';
      }
      if(!serviceType || serviceType.length == 0){
          serviceType = 'goto';
      }
      // Only set preferred service if not ticket link
      if (!eventShowId) {
        setCookie('LF_prefPlayer', player, 90);
      }
      
		var trackingUrl = "https://st.linkfire.com/Api/Click/app/0a64d010df147bb4a58c7b1e6092a79f/507629?boardUuid=476519a6-6ef0-4295-93f8-dcd09e24f749&linkUuid=833d9c18-28c8-4ac5-8ccb-467475048f3b&ouuid=93babeae-9602-409a-8936-13b6e8cfedd1&tlouuid=93babeae-9602-409a-8936-13b6e8cfedd1&country=Unknown&visitorToken=0a64d010df147bb4a58c7b1e6092a79f&boardId=17277&uid=5ca59eb58bfaf2.54120646&shortlink=colors.lnk.to%2Fapplemusic&referrer=&linkChannel=0&url=https%3A%2F%2Fgeo.itunes.apple.com%2Fde%2Fcurator%2Fcolorsxstudios%2F1434327878%3Fmt%3D1%26app%3Dmusic%26at%3D1001lNZj&artists=&tags=Curator%3Bapplemusic&orgId=11968&browser=Python-urllib&device=crawler&os=unknown&querystring=&city=Unknown&activity=&channelType=Original&channelId=&organisation=COLORS+Media&artist=&album=&track=&label=&board=COLORS&genre=&countryName=Unknown&puuid=850ed79a-df27-46a2-b629-c281d2bab6bb&sourcetype=landingpage&linkId=507629&channel=" + player + "&apptype=" + appType + "&eventCategoryType=" + serviceType;
