    <script>
        var google_custom_params = window.linkfire.retargetingPixels.google.tags;
        var conv_handler = null;
        var gaData = window.linkfire.retargetingPixels.google.gaData;
        function TriggerGooglePixel(){
            for(var i = 0; i < gaData.length; i++){
                window.google_conversion_id = gaData[i].id;
                window.google_conversion_label = gaData[i].label;
                window.google_remarketing_only = true;
                if (typeof(conv_handler) == 'function') {
                    conv_handler();
                }
            }
        }
        window.linkfire.retargetingPixels['google'].loader = function() {
            if (!checkScriptLoaded('advertiser','google')) {
                var googlescript = document.createElement('script');
                googlescript.src='//www.googleadservices.com/pagead/conversion_async.js';
                googlescript.type='text/javascript';
                googlescript.onload = function() {
                    conv_handler = window['google_trackConversion'];
                    TriggerGooglePixel();
                }
                document.getElementsByTagName('Body').item(0).appendChild(googlescript);
            }
        }
