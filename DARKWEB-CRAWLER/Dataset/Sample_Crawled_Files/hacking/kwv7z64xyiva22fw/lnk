    window.linkfire.retargetingPixels['facebook'].loader = function() {
        if (!checkScriptLoaded('advertiser','facebook')) {
            !function(f,b,e,v,n,t,s){if(f.fbq)return;n=f.fbq=function(){n.callMethod?
            n.callMethod.apply(n,arguments):n.queue.push(arguments)};if(!f._fbq)f._fbq=n;
            n.push=n;n.loaded=!0;n.version='2.0';n.queue=[];t=b.createElement(e);t.async=!0;
            t.src=v;s=b.getElementsByTagName(e)[0];s.parentNode.insertBefore(t,s)}(window,
            document,'script','//connect.facebook.net/en_US/fbevents.js');

            window.linkfire.retargetingPixels.facebook.ids.forEach(function(id) {
                if (id === "399736106876066") {
                    fbq('init', id , {external_id: window.linkfire.settings.visitorToken});
                } else {
                    fbq('init', id);
                }
            });
            window._fbdata = window.linkfire.retargetingPixels.facebook.tags;
            if(window.linkfire.player != null) {
                window._fbdata.action = 'service';
                window._fbdata.musicservice = window.linkfire.player;
            }
            if(window.linkfire.appClickPlayer != null) {
                window._fbdata.eventlabel = window.linkfire.appClickPlayer;
            }
            fbq('trackCustom', 'LF', window._fbdata);
        }
