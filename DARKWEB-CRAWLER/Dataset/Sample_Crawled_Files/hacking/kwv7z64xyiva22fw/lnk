<iframe id="detectURI" src="" style="display:none"></iframe>

  <script type="text/javascript">
    // Avoid namespace polution
    var linkfireDeeplink = {
      clicked: false,
      iframeTarget: document.getElementById('detectURI'),
      isDesktop: false,
      services: document.getElementsByClassName('redirect'),
      visited: 0
    };

    // Attach event listeners to all anchors
    for (var i = 0; i < linkfireDeeplink.services.length; i++) {
      linkfireDeeplink.services[i].addEventListener('click', redirectHandler, false);
    }

    function redirectHandler(e) {
      e.preventDefault();
      if(linkfireDeeplink.clicked) return false;

      // set user has clicked a service
      linkfireDeeplink.clicked = true;
      // Reset after 3sec
      setTimeout(function() {
          linkfireDeeplink.clicked = false;
      },3000);
      linkfireDeeplink.visited = 0;

      if(e.target.hasAttribute('href')){
        var link = e.target;
      }else{
        var link = this;
      }

      var uri = link.getAttribute('data-uri');

            var url = link.getAttribute('href');
      var player = link.getAttribute('data-player');
      var appType = link.getAttribute('data-apptype');
      var serviceType = link.getAttribute('data-serviceType');
      var eventShowId = link.hasAttribute('data-showid') ? link.getAttribute('data-showid') : null;
      var eventlabel = 'unknown';
      if(link.children[1]){
        eventlabel = link.children[1].textContent;
      }

      if(!appType || appType.length == 0){
          appType = 'manual';
      }
      if(!serviceType || serviceType.length == 0){
          serviceType = 'goto';
      }
      // Only set preferred service if not ticket link
      if (!eventShowId) {
        setCookie('LF_prefPlayer', player, 90);
      }
      
		var trackingUrl = "https://st.linkfire.com/Api/Click/app/63f97c774845a50080bdf7d06733f37b/410539?boardUuid=79d83a14-b4b2-4c79-a70a-59efc95da13e&linkUuid=7dac4963-aab8-4c13-9795-9d90980ea68c&ouuid=6f0f6537-8414-4b6c-9fcd-414f59ceb44f&tlouuid=6ee95251-4c47-11e6-9fd0-066c3e7a8751&country=Unknown&visitorToken=63f97c774845a50080bdf7d06733f37b&boardId=12367&uid=5ca6399ba20dd9.90295267&shortlink=lnk.to%2FTheCureHits&referrer=&linkChannel=0&url=https%3A%2F%2Fopen.spotify.com%2Fuser%2Fthecure%2Fplaylist%2F6b1JrDE4vFKRywrCKmxgSJ&artists=&tags=&orgId=1&browser=Python-urllib&device=crawler&os=unknown&querystring=&city=Unknown&activity=&channelType=Original&channelId=&organisation=Universal+Music+International&artist=&album=&track=&label=&board=UMGi+Commercial+Affairs+-+VEVO%2FYouTube&genre=&countryName=Unknown&puuid=c5af5c00-4013-4675-945f-f41c55b4f415&sourcetype=landingpage&linkId=410539&channel=" + player + "&apptype=" + appType + "&eventCategoryType=" + serviceType;
