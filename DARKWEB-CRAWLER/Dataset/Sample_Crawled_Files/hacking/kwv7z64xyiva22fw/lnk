var tracked = false;
var session = (document.cookie.indexOf("LF_session_f46d400f64bcf00f3c08348373ed7173=0") !== -1 ? 'True' : 'False');
function handleVisibilityChange(){
    if(document.visibilityState == "visible" && !tracked){
        $('#trackingcontainer').append($('<img>', {
                    src : "https://st.linkfire.com/Api/Click/click/2dd4f64249322f273a4a4a524983a3d5/602818?boardUuid=476519a6-6ef0-4295-93f8-dcd09e24f749&linkUuid=bcc47845-9622-458b-8d32-65b51f8268c7&ouuid=93babeae-9602-409a-8936-13b6e8cfedd1&tlouuid=93babeae-9602-409a-8936-13b6e8cfedd1&country=Unknown&visitorToken=2dd4f64249322f273a4a4a524983a3d5&boardId=17277&uid=5ca5a7e7c21157.51788811&shortlink=colors.lnk.to%2Fnone&referrer=&linkChannel=0&url=https%3A%2F%2Fopen.spotify.com%2Ftrack%2F0qVmnY53N0Qkb9YdOy4owI&artists=Crush&tags=&orgId=11968&browser=Python-urllib&device=crawler&os=unknown&querystring=&city=Unknown&activity=&channelType=Original&channelId=&organisation=COLORS+Media&artist=Crush&album=none&track=none&label=&board=COLORS&genre=&countryName=Unknown&puuid=850ed79a-df27-46a2-b629-c281d2bab6bb&sourcetype=landingpage&linkId=602818&sourcetype=landingpage&newSession=" + session + "",
                    width : 1,
                    height : 1,
                }));                var d = new Date();
        d.setTime(d.getTime() + 600000);
        var expires = "expires=" + d.toGMTString();
        document.cookie = "LF_session_f46d400f64bcf00f3c08348373ed7173=1; "+expires + "; path=/; domain=" + window.linkfire.consent.domain + ";";
        tracked = true;
    }
}

if (typeof document.addEventListener === "undefined" || typeof document.visibilityState === "undefined") {
  document.visibilityState = 'visible';
} else {
  document.addEventListener("visibilitychange", handleVisibilityChange, false);
}
