<script>
var tracked = false;
var session = (document.cookie.indexOf("LF_session_6f7e4c56d8085ec11edddcfac014e4a9=0") !== -1 ? 'True' : 'False');
function handleVisibilityChange(){
    if(document.visibilityState == "visible" && !tracked){
        $('#trackingcontainer').append($('<img>', {
