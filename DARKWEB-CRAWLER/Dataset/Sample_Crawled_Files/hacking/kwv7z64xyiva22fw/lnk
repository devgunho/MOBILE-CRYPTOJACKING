            insert_pos.parentNode.insertBefore(new_script_section, insert_pos);
            })(window, document, 'https://sc-static.net/scevent.min.js');

            snapIds.forEach(function(id) {
                snaptr('init', id);
            });

            triggerSnap('pageview');
        }
    }

    function triggerSnap(eventType) {
        if (typeof(snaptr) === 'undefined' || typeof(snaptr) !== 'function') {
            return;
        }

        var snapEvents = {
          'player': 'VIEW_CONTENT',
          'service': 'START_CHECKOUT',
          'pageview': 'PAGE_VIEW'
        };

        snaptr('track', snapEvents[eventType]);
    }
