        $('#trackingcontainer').append($('<img>', {
                    src : "https://st.linkfire.com/Api/Click/click/6345ffa106528855127aeffabe5ee119/618740?boardUuid=476519a6-6ef0-4295-93f8-dcd09e24f749&linkUuid=e30532d4-92c6-4442-a941-810432cecad3&ouuid=93babeae-9602-409a-8936-13b6e8cfedd1&tlouuid=93babeae-9602-409a-8936-13b6e8cfedd1&country=Unknown&visitorToken=6345ffa106528855127aeffabe5ee119&boardId=17277&uid=5ca655179f0f56.42963670&shortlink=colors.lnk.to%2Fnotenough&referrer=&linkChannel=0&url=https%3A%2F%2Fopen.spotify.com%2Falbum%2F2ET3XAgn94YgMKynJWXVBN&artists=Benny+Sings&tags=&orgId=11968&browser=Python-urllib&device=crawler&os=unknown&querystring=&city=Unknown&activity=&channelType=Original&channelId=&organisation=COLORS+Media&artist=Benny+Sings&album=Not+Enough&track=&label=&board=COLORS&genre=Alternative&countryName=Unknown&puuid=850ed79a-df27-46a2-b629-c281d2bab6bb&sourcetype=landingpage&linkId=618740&sourcetype=landingpage&newSession=" + session + "",
