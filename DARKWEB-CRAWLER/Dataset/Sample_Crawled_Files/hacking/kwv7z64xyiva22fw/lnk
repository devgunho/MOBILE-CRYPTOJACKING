            (function(w,d,e,n,s,c,a,f){w['LnkConsentObject']=n;w[n]=w[n]||function(){
            (w[n].q=w[n].q||[]).push(arguments)},w[n].c=c;a=d.createElement(e),
            f=d.getElementsByTagName(e)[0];a.async=1;a.src=s;f.parentNode.insertBefore(a,f)
            })(window,document,'script','lnk_consent','https://static.assetlab.io/consent/1.1.4/consent.js','https://static.assetlab.io/consent/1.1.4/consent.css');

            lnk_consent('init', {
                level: window.linkfire.consent.minLevel,
                consentedLevel: window.linkfire.consent.level,
                dialogType: window.linkfire.consent.dialogType,
                consentedRequired: window.linkfire.consent.required,
                consentedPerformance: window.linkfire.consent.isConsentedToLevel ? window.linkfire.consent.performance : {},
                consentedAdvertising: window.linkfire.consent.isConsentedToLevel ? window.linkfire.consent.advertiser : {},
                providerListRequired: window.linkfire.requiredCookies,
                providerListPerformance: ["google","linkfire"],
                providerListAdvertising: ["facebook","google","twitter"],
                controller: window.linkfire.controller
            });

           /**
            * Callback for when the Explicit Consent dialog is displayed on load
            */
            lnk_consent('on', 'dialog_view', function() {
                trackConsent(window.linkfire.consent.tracking.dialogView);
            });

            /**
             * Callback when user opts in
             * @param {String[]} consentedPerformance list of performance cookies the user is consenting to
             * @param {String[]} consentedAdvertising list of advertising cookies the user is consenting to
            */
            lnk_consent('on', 'opt_in', function(consentedPerformance, consentedAdvertising) {
                window.linkfire.consent.level = window.linkfire.consent.minLevel;
                setcookieEx('LF_level', window.linkfire.consent.minLevel, 180)
                Object.keys(consentedPerformance).forEach(function(key) {
                    // if value changed from false to true, fire pageview pixels if not already

                    if (consentedPerformance[key] == true) {
                        if (key in window.linkfire.consent.performance && window.linkfire.consent.performance[key] == false || !(key in window.linkfire.consent.performance) && consentedPerformance[key] == true) {
                            loadScript('performance', key);
                        }
                    }

                    if (key == "linkfire" && consentedPerformance[key] == true) {
                        setLFCookies();
                    }
                });

                window.linkfire.consent.performance = consentedPerformance;
                setcookieEx('LF_performance', JSON.stringify(window.linkfire.consent.performance), 180);

                Object.keys(consentedAdvertising).forEach(function(key) {
                    if (consentedAdvertising[key] == true) {
                        if (key in window.linkfire.consent.advertiser && window.linkfire.consent.advertiser[key] == false || !(key in window.linkfire.consent.advertiser) && consentedAdvertising[key] == true) {
                            loadScript('advertiser', key);
                        }
                    }
                });

                window.linkfire.consent.advertiser = consentedAdvertising;
                setcookieEx('LF_advertiser', JSON.stringify(window.linkfire.consent.advertiser), 180);

                trackConsent(window.linkfire.consent.tracking.optin);
            });

            /**
             * Callback when user updated their cookie preferences
             * @param {String[]} consentedPerformance list of performance cookies the user is consenting to
             * @param {String[]} consentedAdvertising list of advertising cookies the user is consenting to
            */
            lnk_consent('on', 'update_consent', function(consentedPerformance, consentedAdvertising) {
                setcookieEx('LF_level', window.linkfire.consent.minLevel, 180)

                Object.keys(consentedPerformance).forEach(function(key) {
                    if (consentedPerformance[key] == true) {
                        if (key in window.linkfire.consent.performance && window.linkfire.consent.performance[key] == false || !(key in window.linkfire.consent.performance) && consentedPerformance[key] == true) {
                            loadScript('performance', key);
                        }
                    }
                    if (key == "linkfire") {
                        if (consentedPerformance[key] == true) {
                            setLFCookies();
                        } else {
                            var cookies = getCookies();
                            Object.keys(cookies).forEach(function(key) {
                                if (key.indexOf('LF_prefPlayer') === -1 &&Â key.indexOf('LF_session') === -1 &&Â key.indexOf('LF_performance') === -1 && key.indexOf('LF_advertiser') === -1 && key.indexOf('LF_level') === -1) {
                                    setcookieEx(key, '', -1);
                                }
                            });
                        }
                    }
                });

                window.linkfire.consent.performance = consentedPerformance;
                setcookieEx('LF_performance', JSON.stringify(window.linkfire.consent.performance), 180)

                Object.keys(consentedAdvertising).forEach(function(key) {
                    if (consentedAdvertising[key] == true) {
                        if (key in window.linkfire.consent.advertiser && window.linkfire.consent.advertiser[key] == false || !(key in window.linkfire.consent.advertiser) && consentedAdvertising[key] == true) {
                            loadScript('advertiser', key);
                        }
                    }
                });

                window.linkfire.consent.advertiser = consentedAdvertising;
                setcookieEx('LF_advertiser', JSON.stringify(window.linkfire.consent.advertiser), 180)

                if (
                    Object.keys(window.linkfire.consent.performance).every(function(name) { return !window.linkfire.consent.performance[name]; }) &&
                    Object.keys(window.linkfire.consent.advertiser).every(function(name) { return !window.linkfire.consent.advertiser[name]; })
                ) {
                    trackConsent(window.linkfire.consent.tracking.optout);
                } else {
                    trackConsent(window.linkfire.consent.tracking.advanced);
                }
            });

            /**
             * Callback when user opens cookie preferences/info modal
             */
            lnk_consent('on', 'open_preferences', function() {
                // trackConsent(window.linkfire.consent.tracking.openPref);
            });

            /**
             * Callback when user closes cookie preferences/info modal
             */
            lnk_consent('on', 'close_preferences', function() {
                // trackConsent(window.linkfire.consent.tracking.closePref);
            });

            /**
             * Callback when user dismiss the initial dialog
             */
            lnk_consent('on', 'dismiss', function() {
                trackConsent(window.linkfire.consent.tracking.dismiss);
            });
        </script>
