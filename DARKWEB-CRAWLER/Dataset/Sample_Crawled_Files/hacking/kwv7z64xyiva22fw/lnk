var trackedPlay = false;
function trackPlay(){
    if(trackedPlay){
        return;
    }
    trackedPlay = true;
    $('#trackingcontainer').append($('<img>', {
                    src : "https://st.linkfire.com/Api/Click/app/f893ae211178877fdcf319835d89387b/578068?boardUuid=476519a6-6ef0-4295-93f8-dcd09e24f749&linkUuid=d474202c-519b-4ee1-ac83-8d0f166d129c&ouuid=93babeae-9602-409a-8936-13b6e8cfedd1&tlouuid=93babeae-9602-409a-8936-13b6e8cfedd1&country=Unknown&visitorToken=f893ae211178877fdcf319835d89387b&boardId=17277&uid=5ca590cd107932.28941701&shortlink=colors.lnk.to%2FAMCREAM&referrer=&linkChannel=0&url=https%3A%2F%2Fitunes.apple.com%2Fde%2Fplaylist%2Fcream-rhymes-beats%2Fpl.1dd0b69802f546e09f08c6dd985e942e%3Fmt%3D1%26app%3Dmusic%26at%3D1001lNZj%26ct%3DLFV_f893ae211178877fdcf319835d89387b&artists=&tags=&orgId=11968&browser=Python-urllib&device=crawler&os=unknown&querystring=&city=Unknown&activity=&channelType=Original&channelId=&organisation=COLORS+Media&artist=&album=&track=&label=&board=COLORS&genre=&countryName=Unknown&puuid=850ed79a-df27-46a2-b629-c281d2bab6bb&sourcetype=landingpage&linkId=578068&channel=linkfireplayer&apptype=lfplayer",
                    width : 1,
                    height : 1,
                }));    if(typeof(trackEvent) == 'function'){
        trackEvent('musicplayer', 'play');
    }
    if(typeof(fireEvent) == 'function'){
        fireEvent('assetInteraction', 'Landing page', 'Interaction', 'Started music player');
