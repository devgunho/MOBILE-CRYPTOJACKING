<script type="text/javascript">
   function getQueryStringValue (key) {  
   
   return decodeURIComponent(window.location.search.replace(new RegExp("^(?:.*[&\\?]" + encodeURIComponent(key).replace(/[\.\+\*]/g, "\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1"));  
   }
   
   var MCAFEE = MCAFEE || {};
   
   var URLQueryParams =  MCAFEE.PromotionUtil.getQueryString(window.location.href);
   
   var referrer = document.referrer;
   
   (function getPageTypeFromURL() {
   if(MCAFEE.pagetype) {
       return MCAFEE.pagetype;
   }
   var locPath;
   if (location.hash && location.hash.indexOf('#/content/') > -1) {
       locPath = location.hash;
   }else{
       locPath = location.pathname;
   }
   var locPathArr = locPath.split('/');
   if(locPath.indexOf('/content/') > -1) {
       MCAFEE.pagetype = (locPathArr.length >=6) ? locPathArr[5] : "";
   } else {
       MCAFEE.pagetype = (locPathArr.length >=4) ? locPathArr[3] : "";
   }
   })();
   
   
   if(typeof utag_data != 'undefined'){

       /********* geo code *******/

       if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_geo = getQueryStringValue("");
   
       		}

       }
	   
	   /********** culture code *******/
	  
       if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_culture_code = getQueryStringValue("");
   
       		}

       }
	   
	   /************** country ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");


            if(queryValue){
   
   				utag_data.tm_global_country = getQueryStringValue("");
   
       		}

       }

	   /************** Business Unit ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");



            if(queryValue){
   
   				utag_data.tm_global_businessunit = getQueryStringValue("");
   
       		}

       }
	   
	   /************** Affiliate Channel ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_channel = getQueryStringValue("");
   
       		}

       }
	   
	   /************** Affiliate Id ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_affiliate_id = getQueryStringValue("");
   
       		}

       }

       /************** Affiliate Name ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_affiliate_name = getQueryStringValue("");
   
       		}

       }

        /************** Affiliate Code ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_affiliate_code = getQueryStringValue("");
   
       		}

       }

        /************** Campaign PU Name ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_cms_campaign_pu_name = getQueryStringValue("");
   
       		}

       }
        
        if(""){
        	utag_data.tm_global_cms_campaign_pu_name = "";
        }

        /************** Target User Profile ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_cms_targeted_user_profile = getQueryStringValue("");
   
       		}

       }

         /************** Publish Date ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_page_publish_date = getQueryStringValue("");
   
       		}

       }


         /************** Campaign P Name ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_cms_campaign_p_name = getQueryStringValue("");
   
       		}

       }
         
	   if(""){
       	utag_data.tm_global_cms_campaign_p_name = "";
        utag_data.tm_global_pagename = "";


       }


        /************** Vanity URL ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_vanity_url = getQueryStringValue("");
   
       		}

       }

        /**************Page Version ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){

   				utag_data.tm_global_page_version = getQueryStringValue("");
   
       		}

       }

        /**************Product name ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_product_name = getQueryStringValue("");
   
       		}

       }







       /********** site section **************/

   
     if(""){
   
   
   
       var queryValue = getQueryStringValue("");
   
       if(queryValue){
   
   
           utag_data.tm_global_sitesection = getQueryStringValue("");
   
       }

       else if("homepage"){

   

     utag_data.tm_global_sitesection = "homepage";
   
   
       }
   
   }

   else if("homepage"){

   
     utag_data.tm_global_sitesection = "homepage";


   }
   
   if(""){
   
   
   
       var queryValue = getQueryStringValue("");
   
       if(queryValue){
   
   
           utag_data.tm_global_sitesection = getQueryStringValue("");
   
       }
   
       else if("homepage"){
   
   
   
     utag_data.tm_global_sitesection = "homepage";
   
   
       }
   
   }
   
   else if("homepage"){
   
   
     utag_data.tm_global_sitesection = "homepage";
   
   
   }
   
   
   
    /******** campaign type variable ********/

   if(""){
   
   
   
       var queryValue = getQueryStringValue("");
   
       if(queryValue){
   
   
           utag_data.tm_global_cms_campaign_type = getQueryStringValue("");

       }

       else if("seo"){

   

     utag_data.tm_global_cms_campaign_type = "seo";
   

       }
   
   }

   else if("seo"){


     utag_data.tm_global_cms_campaign_type = "seo";


   }
   

 /******** campaign sub type variable ********/

   if(""){
   
   
   
       var queryValue = getQueryStringValue("");
   
       if(queryValue){
   
   
           utag_data.tm_global_cms_campaign_sub_type = getQueryStringValue("");

       }

       else if("others"){

   

     utag_data.tm_global_cms_campaign_sub_type = "others";
   
   
       }
   
   }

   else if("others"){


     utag_data.tm_global_cms_campaign_sub_type = "others";


   }
   

   
    if(""){
   
   
   
       var queryValue = getQueryStringValue("");
   
       if(queryValue){
   
   
           utag_data.tm_global_traffic_source_type = queryValue;
   
       }
   
       else if(""){
   
   
   
     utag_data.tm_global_traffic_source_type = "";
   
   
       }

   }
   
   else if(""){
   
   
     utag_data.tm_global_traffic_source_type = "";
   
   
   }
   
   else{
   
      
   
   
       if (MCAFEE.pagetype == 'store') {
   
           
   
       if (referrer != '' && referrer.indexOf('http://strongmail.mcafee.com') == 0) {
   
           utag_data.tm_global_traffic_source_type = 'email';
       } else if (URLQueryParams.source && URLQueryParams.source == 'lqs') {
   
           utag_data.tm_global_traffic_source_type = 'sem';
       } else if (URLQueryParams.source && URLQueryParams.source == 'lqr') {
   
           utag_data.tm_global_traffic_source_type = 'retargeting';
       } else if (URLQueryParams.source && URLQueryParams.source == 'lqa') {
   
           utag_data.tm_global_traffic_source_type = 'affiliate';
       } else if (URLQueryParams.kw) {
   
           utag_data.tm_global_traffic_source_type = 'seo';
       } else {
   
           
   
           utag_data.tm_global_traffic_source_type = 'others';
       }
   }
   
   
   }
   
         if(document.getElementById("trafficSourceType")){    
   
   document.getElementById("trafficSourceType").innerHTML = utag_data.tm_global_traffic_source_type; 
       }

   /********* site section level 2 *********/

	 if(""){
   
   
   
       var queryValue = getQueryStringValue("");
   
       if(queryValue){
   
   
           utag_data.tm_global_sitesection_level2 = getQueryStringValue("");

       }

       else if("homepage"){

   

     utag_data.tm_global_sitesection_level2 = "homepage";
   
   
       }
   
   }

   else if("homepage"){


     utag_data.tm_global_sitesection_level2 = "homepage";


   }
   



   /******** platform variable ********/

   if(""){
   
   
   
       var queryValue = getQueryStringValue("");
   
       if(queryValue){
   
   
           utag_data.tm_global_platform = getQueryStringValue("");

       }

       else if("web"){

   

     utag_data.tm_global_platform = "web";
   
   
       }
   
   }

   else if("web"){


     utag_data.tm_global_platform = "web";


   }
       /*******  Campaign Id ********/ 

        if(""){

           var queryValue = getQueryStringValue("");

			 if(queryValue){
   
   				utag_data.tm_global_campaign_id = getQueryStringValue("");

       		}

       }

       /******* MSS+  Product Code ********/ 

        if(""){

           var queryValue = getQueryStringValue("");

			 if(queryValue){
   
   				utag_data.tm_global_antivirus_brand_code = getQueryStringValue("");
   
       		}

       }

       /******  MSS+ Antivirus Status    ******/
  	 if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_antivirus_status = getQueryStringValue("");
   
       		}

       }

      /******  MSS+ Client Version    ******/
   	if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_client_version = getQueryStringValue("");
   
       		}

       } 

     /******  MSS+ Firewall Brand Code    ******/
   	if(""){



           var queryValue = getQueryStringValue("");


            if(queryValue){
   
   				utag_data.tm_global_firewall_brand_code =  getQueryStringValue("");
   
       		}

       }

       /******  MSS+ Firewall Status    ******/
   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_firewall_status = getQueryStringValue("");
   
       		}

       }

      /******  MSS+ Unwanted Prog Count    ******/
   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_unwanted_prog_count = getQueryStringValue("");
   
       		}

   		}

       /******  MSS+ Site Advisor    ******/
   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_site_advisor = getQueryStringValue("");
   
       		}

       }

     /******  MSS+ Security Status    ******/
   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_security_status = getQueryStringValue("");
   
       		}

       }  

      /******  MSS+ Suspicious Site Count    ******/
   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_suspicious_site_count = getQueryStringValue("");
   
       		}

       } 

      /******  MSS+ Dangerous Site Count    ******/
   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_dangerous_site_count = getQueryStringValue("");
   
       		}

       } 

   }




</script>
</div>

	</div>
