<script type="text/javascript">
   function getQueryStringValue (key) {  
   
   return decodeURIComponent(window.location.search.replace(new RegExp("^(?:.*[&\\?]" + encodeURIComponent(key).replace(/[\.\+\*]/g, "\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1"));  
   }
   
   var MCAFEE = MCAFEE || {};
   
   var URLQueryParams =  MCAFEE.PromotionUtil.getQueryString(window.location.href);
   
   var referrer = document.referrer;
   
   (function getPageTypeFromURL() {
   if(MCAFEE.pagetype) {
       return MCAFEE.pagetype;
   }
   var locPath;
   if (location.hash && location.hash.indexOf('#/content/') > -1) {
       locPath = location.hash;
   }else{
       locPath = location.pathname;
   }
   var locPathArr = locPath.split('/');
   if(locPath.indexOf('/content/') > -1) {
       MCAFEE.pagetype = (locPathArr.length >=6) ? locPathArr[5] : "";
   } else {
       MCAFEE.pagetype = (locPathArr.length >=4) ? locPathArr[3] : "";
   }
   })();
   
   
   if(typeof utag_data != 'undefined'){

       /********* geo code *******/

       if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_geo = getQueryStringValue("");
   
       		}

       }
	   
	   /********** culture code *******/
	  
       if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_culture_code = getQueryStringValue("");
   
       		}

       }
	   
	   /************** country ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");


            if(queryValue){
   
   				utag_data.tm_global_country = getQueryStringValue("");
   
       		}

       }

	   /************** Business Unit ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");



            if(queryValue){
   
   				utag_data.tm_global_businessunit = getQueryStringValue("");
   
       		}

       }
	   
	   /************** Affiliate Channel ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_channel = getQueryStringValue("");
   
       		}

       }
	   
	   /************** Affiliate Id ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_affiliate_id = getQueryStringValue("");
   
       		}

       }

       /************** Affiliate Name ********/
	   
	   if(""){

           var queryValue = getQueryStringValue("");

            if(queryValue){
   
   				utag_data.tm_global_affiliate_name = getQueryStringValue("");
   
       		}

       }

