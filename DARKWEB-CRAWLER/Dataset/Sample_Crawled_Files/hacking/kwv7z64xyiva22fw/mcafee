<script src="/enterprise/www/js/v1/ui.js"></script>

<script type="text/javascript">
$.ajax({
	url: "/enterprise/admin/rss-feeds/press-releases/59F8354E-4378-4ECD-9985-FADDB0FE9752.xml?"+ new Date().getTime(),
	type: 'get',
	dataType: 'xml',
	success: function(data) {
		var xml_cn_results = data;
		populateCyberNews(xml_cn_results);
   },
	error: function() {
		alert("An error occurred while processing XML file.");
	}
});

function populateCyberNews(xmlData){
	var pressReleaseData = $(xmlData).find("press_release");
	var cyberNewsText = "";
	for(var j = 0; j < 6 ; j++){
		var crrntPressReleaseTitle = $(pressReleaseData[j]).find("title").text();
		var crrntPressReleaseId = $(pressReleaseData[j]).find("News_Item_ID").text();
		var dateText = $(pressReleaseData[j]).find("Release_Date").text();
		var dateObj = new Date(dateText);
		var monthArr = ["January","February","March","April","May","June","July","August","September","October","November","December"];
		var cyberSecNewsDate = monthArr[dateObj.getUTCMonth()] +" "+ dateObj.getUTCDate() + ", " + dateObj.getUTCFullYear();

		cyberNewsText += "<div class='slide'>";
        cyberNewsText += "	<a href='/enterprise/en-us/about/newsroom/press-releases/press-release.html?news_id="+crrntPressReleaseId+"' class='image-component'>";
        cyberNewsText += "		<div class='date'>"+cyberSecNewsDate+"</div>";
		cyberNewsText += "		<div class='award-block'>";
		cyberNewsText += "			<h4>"+crrntPressReleaseTitle+"</h4>";
		cyberNewsText += "		</div>";
		cyberNewsText += "	</a>";
        cyberNewsText += "</div>"
	}
	$('.logo-carousel-3#cybersecurity_news').slick('unslick');
	$('.logo-carousel-3#cybersecurity_news').hide();
	$("#cybersecurity_news").append(cyberNewsText);
	$('.logo-carousel-3#cybersecurity_news').slick({
		dots: false,
		infinite: true,
		slidesToShow: 3,
		slidesToScroll: 1,
		responsive: [{
			breakpoint: 1023,
			settings: {
				slidesToShow: 3
			}
		},{
			breakpoint: 767,
			settings: {
				slidesToShow: 2
			}
		},{
			breakpoint: 480,
			settings: {
				slidesToShow: 1
			}
		}]
	});	
	$('.logo-carousel-3#cybersecurity_news').show();
}

</script>
