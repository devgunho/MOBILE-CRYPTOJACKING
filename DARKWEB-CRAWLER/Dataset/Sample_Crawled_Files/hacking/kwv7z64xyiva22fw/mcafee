			<script type="text/javascript">
(function(){
	var webStorageFlag= false;
	if (typeof(Storage) !== "undefined") {  
		webStorageFlag = true
	}
	var usr_selected_theme = "";
	//check of usertheme is stored in session or cookie
	if(webStorageFlag && localStorage.getItem("mc_pgTheme") !== ""){
		 usr_selected_theme = localStorage.getItem("mc_pgTheme");
		
	}else{
		usr_selected_theme = getCookieVal(document.cookie, "mc_pgTheme");
	}
	resetPageTheme(usr_selected_theme);
	
	function getCookieVal(cookieVal, paramVal) {
        var testPatt = new RegExp(paramVal);
        var result = "";
        if (testPatt.test(cookieVal)) {
            result = cookieVal.split(paramVal + "=")[1];
            result = result.length > 0 ? result.split(";")[0] : ""
        }
        return result
    }
})();

function resetPageTheme(userTheme){
	if(userTheme == "dark" && $("body").hasClass("brighter")){
		$("body").removeClass("brighter");
	}else if(userTheme == "light" && !($("body").hasClass("brighter"))){
		$("body").addClass("brighter");
	}
	
	$("a.theme-toggle").text($("body").hasClass("brighter")?"Theme Dark": "Theme Light");
}
</script>
<script>
jQuery(function(){
	if ($(window).width() < 1024) {
		$("#nav-secondary").hide();
		$("#nav-secondary").clone().appendTo("#nav");
		$("#nav #nav-secondary").addClass("nav-second-open").show();
		$('#nav-secondary ul > li > a').click(function(e){
			//e.preventDefault();
			var $this = $(this).parent().find('.mini-menu');
			$(".mini-menu").not($this).hide();
			$this.toggle();
		});
	}
});
</script>
<script>
		$(function() {
			$(".title").click(function() {
				if(jQuery(window).width()<767){
					$(this).next('ul').toggle(300);
					$(this).addClass('active');
					return false;
				} else {
					return false;
				}
			});
			$(window).resize(function() {
			  if ( $(window).width() > 1024) {
				  jQuery('body').removeClass('nav-open');
				  jQuery('#nav-secondary').removeClass('nav-second-open');
			  }
			});
		});
	</script>
<script>
  function targetPageParams() {
    return {
      "at_property": "dc18e61f-c3c8-179f-5cdd-32a252c2fdb5"
    };
  }
</script>
<script>
var wrap = jQuery('#wrapper'),
			navOpener = jQuery('.hamburger-btn');
		jQuery(document).on('click', function(e) {
			if (!jQuery(e.target).closest(navOpener).length && jQuery('body').hasClass('nav-open')) {
				if (!jQuery(e.target).closest(wrap).length) {
					jQuery('body').removeClass('nav-open');
				}
			}
		});
$( document ).ready(function() {
    jQuery("#header.new").prependTo( "#wrapper" );
    jQuery('#header:not([class])').remove();

   $(".theme-toggle").click(function(){
		$("body").toggleClass("brighter");
		var usrpageTheme = $("body").hasClass("brighter")?"light":"dark";
		var themeTextVal = {"light":"Theme Dark","dark":"Theme Light"};
		$("a.theme-toggle").text(themeTextVal[usrpageTheme]);
		var webStorageFlag= false;
		if (typeof(Storage) !== "undefined") {  
			webStorageFlag = true
		}
		if(webStorageFlag && localStorage.getItem("mc_pgTheme") !== ""){
			localStorage.setItem("mc_pgTheme",usrpageTheme);
		}else{
			var pgThemeCookieExpiryDate =  new Date("'"+new Date().getUTCMonth()+"/"+new Date().getUTCDate() +"/"+ (new Date().getUTCFullYear()+2)+"'");
			document.cookie = "mc_pgTheme="+usrpageTheme+";"+pgThemeCookieExpiryDate+"; path=/";
		}
		return false;
   });
});
</script>
