        <script>
          // Created by Jerliyah
          let overlay = document.querySelector('.video-ctn .video-overlay')
          let video = document.querySelector('.video-ctn video')
          
          
          overlay.addEventListener('click', toggle_play)
          
          function toggle_play() {
          	
            if( video.classList.contains('active') ) {
            	video.pause()
            	video.classList.remove('active')
            	overlay.classList.add('active')
            }
            else {
            	video.classList.add('active')
            	overlay.classList.remove('active')
              video.play()
            }
          }
        </script>
      </div> <!-- End of div.video-ctn -->
