    $(document).ready(function () {

        // Parallax header
        var titleImage = $('.home-title .standard_content_content_inner img').attr('src');
        $('.home-title .standard_content_content_inner img').closest('p').remove();
        if (titleImage) {
            $('.home-title').parallax({ imageSrc: titleImage });
        }

        // Hero carousel
        $('.home-title .standard_content_content_inner').slick({
            autoplay: true,
            autoplaySpeed: 5000,
        });

        // Set images as backgrounds for masonry
        $('.mason-group__image').each(function () {
            var imgSrc = $(this).find('img').attr('src');
            var cleanImg = imgSrc.replace(/ /g, '%20');
            $(this).find('.mason-group__image-cont').css('background-image', 'url(' + cleanImg + ')');
        });

        // Make links in mason grid
        $('.mason-group').each(function () {
            var linkHref = $(this).find('a').attr('href');
            var link = $('<a />');
            link.attr('href', linkHref);

            $(this).find($('.mason-group__image')).wrapInner(link);
            $(this).find($('.mason-group__text')).wrapInner(link);
        });

        // Background Image
        $('.bg--image').each(function () {
            if ($(this).find('img').length) {
                var imgSrc = $(this).find('img').attr('src');
                var cleanImg = imgSrc.replace(/ /g, '%20');
                $(this).find('img').hide();
                $(this).css('background-image', 'url(' + cleanImg + ')');
            }
        });

        // Wrap blocks with their link
        $('.wrap-link').each(function () {
            var linkHref = $(this).find('a').attr('href');
            var link = $('<a />');
            link.attr('href', linkHref);
            $(this).find('a').remove();
            $(this).find('.dynamic_callout_container').wrapInner(link);
        });

        // Make svg cirlces correct height in ie
        if (/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
            var svgWidth = $('.selector-icons').outerWidth();
            var svgHeight = (svgWidth / 100) * 36;
            $('.selector-block__icons').height(svgHeight);
        }
		
		//Hide Official Partner when row is empty
		
		if(!$('.partners-block__image').children().length && !$('.partners-block__text').children().length){
			$('.partners-block').hide();
		}

        // Supporters carousel
        $('.supporters-carousel .dynamic_callout_container').slick({
            variableWidth: true,
            swipeToSlide: true,
            autoplay: true,
            autoplaySpeed: 2000,
            responsive: [
                {
                    breakpoint: 1025,
                    settings: {
                        variableWidth: false,
                        slidesToShow: 3
                    }
                },
                {
                    breakpoint: 769,
                    settings: {
                        variableWidth: false,
                        centerMode: false,
                        slidesToShow: 1
                    }
                }
            ]
        });

    });

    // Change text when clicking a selector svg
    var selector = '';
    function selectorShow(link, el) {
        //e = e || window.event;
        //e.preventDefault();

        // If clicking the same icon twice, set the defaul MIA text to show
        if (selector == link.replace(/#/g, '')) {

            // Reset selector to blank
            selector = '';

            // Remove the selected class from the circle
            $('.selector-icon--selected').remove();

            $('.selector-block__text.open').removeClass('open');
            $('.selector-block__title.open').removeClass('open');
            $('.selector-block__text#mia').addClass('open');
        } else {
            selector = link.replace(/#/g, '');

            // Remove the selected class from the circle
            if ($('.selector-icon--selected').length) {
                $('.selector-icon--selected').remove();
            }

            var svgClasses = $(el).attr('class');

            var topCircle = $(el).clone().appendTo('.selector-icons');
            topCircle.attr('class', svgClasses + ' selector-icon--selected');

            $('.selector-block__text.open').removeClass('open');
            $('.selector-block__title.open').removeClass('open');
            $('.selector-block__text#' + selector).addClass('open');
            $('.selector-block__title--' + selector).addClass('open');
        }
    }

</script>

        </div>

