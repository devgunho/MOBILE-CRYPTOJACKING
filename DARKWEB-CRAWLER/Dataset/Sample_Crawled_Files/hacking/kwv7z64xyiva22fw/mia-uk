<input name="ctl00$ctl00$CorePlaceHolder$SearchBox$txtSearch" type="text" id="ctl00_ctl00_CorePlaceHolder_SearchBox_txtSearch" class="search_autocomplete" placeholder="Search" />


<a onclick="runAdvancedSearchHeader(event);" id="lnkSearchHeader" class="button"></a>

</div>
<script>
    function runAdvancedSearchHeader(e) {
        var asTextVal = document.getElementById('ctl00_ctl00_CorePlaceHolder_SearchBox_txtSearch').value;
        if (asTextVal == "") return false;
        var asFilterVal = "";
        if ($('#ctl00_ctl00_CorePlaceHolder_SearchBox_ddlFilters').length) {
            if ($('#ctl00_ctl00_CorePlaceHolder_SearchBox_ddlFilters').val() != null) {
                var asFilterVal = $('#ctl00_ctl00_CorePlaceHolder_SearchBox_ddlFilters').val();
                if (asFilterVal != "") asFilterVal = "&f=" + asFilterVal
            }
        }
        if (!e)
            e = window.event;
        if (e.stopPropagation) {
            e.stopPropagation();
        }
        else {
            e.cancelBubble = true;
        }
        window.location = '/search?q=' + asTextVal + asFilterVal;
    }
    document.getElementById('ctl00_ctl00_CorePlaceHolder_SearchBox_txtSearch')
    .addEventListener("keyup", function (event) {
        event.preventDefault();
        if (event.keyCode == 13) {
            runAdvancedSearchHeader(event)
        }
    });
    if ($('#ctl00_ctl00_CorePlaceHolder_SearchBox_ddlFilters').length) {
        document.getElementById('ctl00_ctl00_CorePlaceHolder_SearchBox_ddlFilters')
        .addEventListener("keyup", function (event) {
            event.preventDefault();
            if (event.keyCode == 13) {
                runAdvancedSearchHeader(event)
            }
        });
    }
</script>


                                    
