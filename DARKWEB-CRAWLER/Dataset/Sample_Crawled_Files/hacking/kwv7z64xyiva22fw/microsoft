            <virtual-agent-entry ng-if="hasSkylightEntry && !noChrome && !$root.popupIsDelayed"
                                 data-view="skylightEntryView"
                                 data-name="{{ entryName }}"
                                 data-flow="{{ $root.vaFlowId || calculateEntryFlow() }}"
                                 data-user-input="{{ $root.vaUserInput || undefined }}"
                                 data-message-delay="isMobile ? '' : 2000"
                                 data-message-duration="isMobile ? '' : 10000"
                                 data-on-seen="onEntrySeen(name)"
                                 data-on-clicked="onEntryClicked()">
            </virtual-agent-entry>
        </div>

            <noscript>
        <div class="error-content text-center">
            <div><span class="win-icon warning-symbol win-icon-EmojiNeutral"></span></div>
            <h2> Javascript is disabled </h2>
            <p> Please enable javascript and refresh the page </p>
        </div>
    </noscript>
    <div ng-controller="404PageController">
        <div class="ng-hide" ng-show="errorStatus" ng-if="errorStatus" ng-switch on="errorStatus" data-grid="col-12">
            <div ng-switch-when="500" id="error500" class="error-content text-center">
                <div><span class="win-icon warning-symbol win-icon-EmojiNeutral"></span></div>
                <h2 class="c-heading-2" ng-bind="(errorContent && errorContent.header) ? errorContent.header : 'An error occurred'"></h2>
                <p ng-bind="(errorContent && errorContent.caption) ? errorContent.caption : 'Please try your request again later '"></p>
            </div>
            <div ng-switch-when="404" class="text-center" ng-cloak="">
                <div class="m-hero">
                    <div class="hero-container-404" role="region" aria-label="{{search404Captions.pageNotFoundContainer}}">
                        <section class="m-hero-item f-x-left f-y-center context-accessory">
                            <picture>
                                <img ng-src="{{backgroundUrl}}" alt="{{search404Captions.backgroundAltText}}">
                            </picture>
                            <div>
                                <div class="content-404">
                                    <h1 class="c-heading">{{search404Captions.content404Title}}</h1>
                                    <h4 class="c-paragraph-4">{{search404Captions.content404Description}}</h4>
