                                        <input aria-label="{{search404Captions.searchPrompt}}" aria-controls="page-404-autosuggest" aria-autocomplete="list" type="search" name="search-field"
                                               role="textbox" placeholder="{{search404Captions.searchPrompt | compassFormat: {'sapName': sapName} }}" ng-model="formData.userQuery" ng-init="initAutosuggest()">
                                        <button class="c-glyph" name="search-button" ng-click="primarySearchClicked()">
                                            <span class="x-screen-reader">{{search404Captions.searchIcon}}</span>
                                        </button>
                                        <div class="m-auto-suggest" id="page-404-autosuggest" role="group" data-f-loc-menu-open="results are available, use up and down arrow keys to navigate.">
                                            <ul id="auto-suggest-results" class="c-menu" aria-hidden="true" data-js-auto-suggest-position="default" tabindex="0" role="listbox"></ul>
                                            <ul id="auto-suggest-no-results" class="c-menu f-auto-suggest-no-results" aria-hidden="true" data-js-auto-suggest-position="default" tabindex="0">
