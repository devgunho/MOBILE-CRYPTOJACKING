<script>
	/*****BEGIN URL PARAMETERS*****/
	function urlParam(name) {
		url = window.location.href;
		name = name.replace(/[\[\]]/g, "\\$&");
		var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),results = regex.exec(url);
		if (!results) {
			return "";
		}
		if (!results[2]) {
			return "";
		}
		return decodeURIComponent(results[2].replace(/\+/g, " "));
	}
	/*****END URL PARAMETERS*****/	
	
	/*****BEGIN SET FORM FIELD VALUES*****/
	if (urlParam("category")!="")	{jQuery(".articleFilterCategory").val(urlParam("category"));}
	if (urlParam("topic")!="")		{jQuery(".articleFilterTopic").val(urlParam("topic"));}
	if (urlParam("section")!="")	{jQuery(".articleFilterSection").val(urlParam("section"));}
	if (urlParam("datefrom")!="")	{jQuery(".articleFilterFromDate").val(urlParam("datefrom"));}
	if (urlParam("dateto")!="")		{jQuery(".articleFilterToDate").val(urlParam("dateto"));} 
	/*****END SET FORM FIELD VALUES*****/
	
	/*****BEGIN PAGINATION*****/
	var itemsOnPage=9;
	jQuery(function() {
		jQuery(".pagination").pagination({
			items: 491,
			itemsOnPage: itemsOnPage,
			cssStyle: 'compact-theme',
			hrefTextPrefix: '#page=',
			onPageClick: function(pageNumber, event) {
				paginationChange(pageNumber, event);
			},
			onInit: function(pageNumber, event) {
				paginationChange(1, event);
			},
			useAnchors: false
		});
		
		var hashNumber = parseInt(window.location.hash.replace(/[^0-9\.]/g, ''), 10);
		if((jQuery.isNumeric(hashNumber))){
			jQuery(".pagination").pagination('selectPage', hashNumber);
		}
		else {
			jQuery(".pagination").pagination('selectPage', 1);
			window.location.hash="page=1";
		}

		
		jQuery(window).on('hashchange', function() {
			var hashNumber = parseInt(window.location.hash.replace(/[^0-9\.]/g, ''), 10);
			if(hashNumber>0){
				jQuery(".pagination").pagination('selectPage', hashNumber)
			}
		});
		
	});
	

	
	function paginationChange(pageNumber){
		
		lowIndex=(pageNumber-1)*itemsOnPage;
		highIndex=(pageNumber*itemsOnPage+1);
		jQuery(".pagination").removeAttr("href");
				if (pageNumber!=1){
			lowIndex=((pageNumber-1)*itemsOnPage)+1;
		}
		window.location.hash="page="+pageNumber+"";
				jQuery(".articles .article").addClass("noShow").removeClass("show 1of2 2of2 1of3 2of3 3of3");
		jQuery(".articles .article").each(function(index){
			if(jQuery(this).index()>=lowIndex && jQuery(this).index()<highIndex){
				jQuery(this).removeClass("noShow").addClass("show");
			}
		});
		jQuery(".articles .article.show").each(function(index){
				jQuery(this).removeClass("noShow").addClass("spot"+(index%2+1)+"of2 spot"+(index%3+1)+"of3");
		});
		jQuery(".paginationCurrentPage").html('Page '+pageNumber+' of '+jQuery(".pagination").pagination('getPagesCount')+'');
		if (pageNumber==1){
			jQuery(".articles").addClass("showFeature").removeClass("noFeature");
		}
		else {
			jQuery(".articles").removeClass("showFeature").addClass("noFeature")
		}
	}

	
	
	function createTopicsAndSections(currentCategory){
		try {
			/*Topics and Sections for Internal Audit Insights - Category 20*/
var topics20=[
	"Audit Committee & Board",
	"Critical Business Skills",
	"Culture & Ethics",
	"Data Analytics",
	"Financial Reporting & Sarbanes-Oxley",
	"Fraud",
	"Information Technology & Security",
	"Regulation & Compliance",
	"Risk Management",
	"Training & Development",
];

var sections20=[
    "Becoming a Better Internal Auditor",
    "Tools & Techniques",
    "Managing Your Team"
];



/*Topics and Sections for Infosec Insider - Category 17*/
var topics17=[
    "Threat/Security Intelligence",
    "Risk & Vulnerability Management",
    "Data Protection",
    "Identity and Access Management",
    "Penetration Testing",
    "Cloud/CASB/Virtualization",
    "Mobile Security",
    "Network Security",
    "Privacy"
];

/*OLD SECTIONS
    "Leadership",
    "Industry Impact",
    "News in a Minute"
*/
	
var sections17=[
	"Tools",
	"Processes",
	"Techniques",
	"People"

];



/*Topics and Sections for Resource Center - Category 38*/
var topics38=[
	"Internal & IT Audit",
	"Infosec"
];

var sections38=[
	"eBook",
	"Webcast",
	"White Paper",
	"Partner Content"
];




/*Topics and Sections for Virtual Events - Category 42*/
var topics42=[
	"Upcoming",
	"On-Demand"
];

var sections42=[
	"eBook",
	"Webcast",
	"White Paper",
	"Partner Content"
];
			for (arrayCount=0;arrayCount<topics17.length;arrayCount++) {
				selectedAttribute="";
				if (''==''+topics17[arrayCount]+''){selectedAttribute=' selected="selected" ';}
				jQuery('.articleFilterTopic').append('<option '+selectedAttribute+' value="'+topics17[arrayCount]+'">'+topics17[arrayCount]+'</option>');
			}
			for (arrayCount=0;arrayCount<sections17.length;arrayCount++) {
				selectedAttribute="";
				if (''==''+sections17[arrayCount]+''){selectedAttribute=' selected="selected" ';}
				jQuery('.articleFilterSection').append('<option '+selectedAttribute+' value="'+sections17[arrayCount]+'">'+sections17[arrayCount]+'</option>');
			}
			/*****BEGIN SET DATES BASED ON URL PARAMETERS*****/
			if ('Jan 01, 2000'!='Jan 01, 2000'){ jQuery('.formFromDate').val('Jan 01, 2000'); }
			if ('Jan 01, 3000'!='Jan 01, 3000'){ jQuery('.formToDate').val('Jan 01, 3000'); }
			/*****END SET DATES BASED ON URL PARAMETERS*****/
		}
		catch(err){}
	}
	
	createTopicsAndSections("17");
	/*****END PAGINATION*****/

