<script src="https://www.mottoamfluss.at/wp-content/themes/MottoAmFluss/js/jquery.timepicker.js"></script>
<script type="text/javascript">
    jQuery(document).ready(function(){
        jQuery('.time-select').hide();

        //var restaurant_Noon_minTime = '11:30';
        var restaurant_Noon_maxTime = '13:45';
        //var restaurant_Evening_minTime = '18:00';
        //var restaurant_Evening_maxTime = '23:00';
        var cafe_Morning_minTime = '08:00';
        var cafe_Morning_maxTime = '11:30';
        var cafe_Noon_minTime = '11:30';
        var cafe_Noon_maxTime = '18:00';
        var cafe_Evening_minTime = '18:00';
        var cafe_Evening_maxTime = '23:00';        
        
        jQuery('#tageszeit').change(function(){
            console.log(jQuery(this).val());
            if ( jQuery( this ).hasClass( "restaurant_form" ) ) {
                cafe_Noon_maxTime = restaurant_Noon_maxTime;
            }
            if (jQuery(this).val()=='Noon' || jQuery(this).val()=='Mittag') {
                jQuery('.timeduration').timepicker('remove');
                jQuery('.time-select').show();
                jQuery('.timeduration').timepicker({
                    'minTime': cafe_Noon_minTime,
                    'maxTime': cafe_Noon_maxTime,
                    'showDuration': false,
                    'timeFormat':'H:i',
                    'step':'15',
                    showOn: ['click', 'focus','keypress']
                });
            }else if(jQuery(this).val()=='Evening'|| jQuery(this).val()=='Abend'){
                
                jQuery('.timeduration').timepicker('remove');
                jQuery('.time-select').show();                
                jQuery('.timeduration').timepicker({
                    'minTime': cafe_Evening_minTime,
                    'maxTime': cafe_Evening_maxTime,
                    'showDuration': false,
                    'timeFormat':'H:i',
                    'step':'15',
                    showOn: ['click', 'focus','keypress']
                });
            }else if(jQuery(this).val()=='Morning'|| jQuery(this).val()=='Vormittag'){
                
                jQuery('.timeduration').timepicker('remove');
                jQuery('.time-select').show();                
                jQuery('.timeduration').timepicker({
                    'minTime': cafe_Morning_minTime,
                    'maxTime': cafe_Morning_maxTime,
                    'showDuration': false,
                    'timeFormat':'H:i',
                    'step':'15',
                    showOn: ['click', 'focus','keypress']
                });
            }else{
                jQuery('.timeduration').timepicker('remove');
                jQuery('.time-select').hide(); 
            }
            jQuery('.date-select input[type="text"]').val('');            
            jQuery('.date-select input[type="text"]').removeAttr('aria-required');
            jQuery('.date-select input[type="text"]').removeClass('wpcf7-validates-as-required mycsclass');
            jQuery(".mycstomerror").remove();
            var myid=jQuery(this).val();
            if (myid.length>0) {
                jQuery('#'+myid+' input[type="text"]').attr('aria-required','true');
                jQuery('#'+myid+' input[type="text"]').addClass('wpcf7-validates-as-required mycsclass');
                jQuery("#"+myid).show();
            }
        });
        jQuery(document).on("click",".wpcf7-submit",function(){
            jQuery(".mycsclass").removeClass("mycustomvalidate");
            jQuery(".mycstomerror").remove();
            jQuery(".mycsclass").each(function(){
               if(jQuery(this).val().length<=0){
                    //alert("Please select "+jQuery(this).attr('placeholder'));
                    jQuery(this).after('<span class="wpcf7-not-valid-tip mycstomerror" role="alert">Please select date</span>');
                    jQuery(this).addClass('mycustomvalidate');
                    jQuery(this).focus();
                    return false;
               }
            });
            if (jQuery(".mycsclass").hasClass("mycustomvalidate")){
                return false;
            }
        });
    });
    
        jQuery(window).load(function(){
        if (jQuery('#tageszeit').length>0 && jQuery('#tageszeit').val().length>0) {
           jQuery('#tageszeit').trigger('change')
        }
    });
        
</script>

