<script>
// image switcher
  window.marina = window.marina || {};

  marina.cacheSelectors = function () {
    marina.cache = {
      // Product Page
      $productImage            : $('#ProductPhotoImg'),
      $thumbImages             : $('#ProductThumbs').find('a.product__thumbnail')
    };
  };

  marina.init = function () {
    marina.cacheSelectors();
    marina.productImageSwitch();
  };

    marina.productImageSwitch = function () {
      if (marina.cache.$thumbImages.length) {
        // Switch the main image with one of the thumbnails
        // Note: this does not change the variant selected, just the image
        marina.cache.$thumbImages.on('click', function(evt) {
          evt.preventDefault();
          var newImage = $(this).attr('href');
          marina.switchImage(newImage, null, marina.cache.$productImage);
        });
      }
    };

    marina.switchImage = function (src, imgObject, el) {
      // Make sure element is a jquery object
      var $el = $(el);
      $el.attr('src', src);
    };

  $(marina.init);
</script>


