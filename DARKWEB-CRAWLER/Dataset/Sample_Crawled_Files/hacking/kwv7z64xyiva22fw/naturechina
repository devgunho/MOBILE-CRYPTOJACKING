    <script>
        var skip = true;
        $('body,html').keydown(function(e){
            if(skip){
                if(e.which === 9){
                    $('.skiplink').css('height','36px');
                    skiplink ();
                    skip = false;
                    /*$('#skiplink').focus(function(event) {
                        $('.server a')[0].focus();
                    });*/

                }
            }
        });
        /*$('header').click(function(){
            skip = true;
        })*/
        $(window).scroll(function(event) {
            if($(window).width()>850){
                scrollFun();
            }
        });
        function scrollFun(){
            var $Top = $(window).scrollTop();
            if($Top>10){
                $('.skiplink').css('height','0px');
                skiplink();
            }else{
                // $('.header2 nav').css('top','185px');
            }
        }
        function skiplink (){
            if($('.skiplink').height()>0){
                $('article').css('marginTop','116px');
                $('.index-head').css('top','36px');
            }else{
                $('article').css('marginTop','80px');
                $('.index-head').css('top','0px');
            }
        }
        if(navigator.appName == "Microsoft Internet Explorer"&&parseInt(navigator.appVersion.split(";")[1].replace(/[ ]/g, "").replace("MSIE",""))<10){
            var slide =$('.swiper-banner .swiper-slide');
            $('.swiper-pagination,.swiper-button-prev,.swiper-button-next').hide();
            for(var i=0; i<slide.length;i++){
                if(i>0){
                    $(slide[i]).hide();
                }
            }
        }else{
            var swiper = new Swiper('.swiper-container.swiper-banner', {
                autoplay:5000,
                autoplayDisableOnInteraction:false,
                spaceBetween: 0,
                paginationClickable :true,
                // preventClicks : false,
                pagination : '.banner .swiper-pagination',
                speed:1500,
                loop:true,
                effect: 'fade'
            });
        }
        var temp = 3;
        if($(window).width()<=850) {
            temp = 4;
        }
        if($(window).width()<=640){
            temp = 3;
        }
        var serverbox = $('.server .server-box');
        var whitebox = $('.white-paper-box li');
        app.lengthShow(whitebox,2);
        for (var i = 0; i<serverbox.length; i++) {
            var serverLi = $(serverbox[i]).find('li');
            app.lengthShow(serverLi,temp);
            if(serverLi.length>temp){
                $(serverbox[i]).css('marginBottom','50px');
            }else{
                $(serverbox[i]).css('marginBottom','30px');
            }
        }
        $('.server-box .more').click(function(){
            var server = $(this).siblings('ul').children('li');
            if($(this).hasClass('active')) {
                $(this).children('span').text('æ¶èµ·');
                server.slideDown();
                $(this).removeClass('active');
            }else{
                $(this).addClass('active');
                $(this).children('span').text('æ´å¤æå¡');
                for(var k=1;k <= server.length;k++){
                    if(k>temp){
                        $(server[k-1]).slideUp();
                    }
                }
            }
        });
        $('.white-paper .more').click(function(){
            if($(this).hasClass('active')) {
                $(this).children('span').text('æ¶èµ·');
                whitebox.slideDown();
                $(this).removeClass('active');
            }else{
                $(this).addClass('active');
                $(this).children('span').text('æ´å¤');
                for(var k=0;k <= whitebox.length;k++){
                    if(k>1){
                        $(whitebox[k]).slideUp();
                    }
                }
            }
        });
        $(window).load(function() {
            if($(window).width()>=640) {
                $('.kv-text').outerHeight($('.banner .swiper-slide .img').height());
                $(window).resize(function(){
                    $('.kv-text').outerHeight($('.banner .swiper-slide .img').height());
                })
            }
            if($(window).width()<=850){
                $('a.more').on('click touchend', function (e) {
                    var el = $(this);
                    var link = el.attr('href');
                    window.location = link;
                })
            }
        })
    </script>


