				{
					if (chunks.length)
					{
						var chunk = chunks.shift(), chunkPush = $.extend({}, impressionData);


						chunkPush['event'] 					= 'impression';
						chunkPush.ecommerce['impressions']  = chunk;
						chunkPush['eventCallback'] 			= (function(chunks)
						{
							return function()
							{
								lazyPush(chunks);
							}
						})(chunks);

						(function(data)
						{
							AEC.Cookie.impressions(data).push(dataLayer);
							
						})(chunkPush);
					}
				};

				/**
				 * Call lazy push
				 */
				lazyPush(chunks);
			});
		}


		
