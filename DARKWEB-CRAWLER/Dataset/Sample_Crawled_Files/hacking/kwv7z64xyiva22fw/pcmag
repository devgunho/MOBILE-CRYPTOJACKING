return "";
}
if (!keyName) {
keyName = "keyname";
}
if (!object) {
return beginning;
}
var appendChar = beginning.indexOf("?") >= 0 ? "&" : "?";
return beginning + appendChar + keyName + "=" + encodeURIComponent(JSON.stringify(object));
};
