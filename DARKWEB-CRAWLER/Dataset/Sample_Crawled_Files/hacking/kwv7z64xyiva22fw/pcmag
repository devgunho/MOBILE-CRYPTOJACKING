var getTimestamp = function() { return Math.floor(Date.now() / 1000); }
var slotRenderEndedCallback = function(ev) { var slot = ev.slot; slot._rendered = getTimestamp(); }
var impressionViewableCallback = function(ev) {
  var slot = ev.slot;
  var elId = slot.getSlotElementId();
  if (!slot._rendered) { slot._rendered = getTimestamp(); }
  if (slot.getOutOfPage() || elId == 'gpt-mpu1' || elId.indexOf('-oop') != -1 || elId == 'sharethrough_footer' || elId == 'sharethrough_sidebar') { return };
  var refresh = 30 - (getTimestamp() - slot._rendered);
  if (refresh < 0) { refresh = 1; }
  if (slot._refreshTimeout) { try { clearTimeout(slot._refreshTimeout); } catch (err) {} }
  if (elId == 'gpt-billboard') {
    slot._refreshTimeout = setTimeout(function() { slot._rendered = null; googletag.pubads().refresh([slot, window.AdSlots['mrec1']]); }, refresh*1000);
  } else {
    slot._refreshTimeout = setTimeout(function() { slot._rendered = null; googletag.pubads().refresh([slot]); }, refresh*1000);
  }

}

