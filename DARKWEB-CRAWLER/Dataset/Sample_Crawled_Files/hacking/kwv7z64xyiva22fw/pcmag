function sendAdserverRequest(e) {
    if (pbjs.adserverRequestSent) { return; }
    pbjs.adserverRequestSent = true;
    if (pbjs.getAdServerTargeting) { pbjs.getAdserverTargeting(); }
    googletag.cmd.push(function() { pbjs.que.push(function() { pbjs.setTargetingForGPTAsync(); googletag.pubads().refresh(null, {changeCorrelator: true}); }); });
}
pbjs.que.push(function() {
  if (window.screen < 800) { return; }

  pbjs.bidderSettings = {
      standard: {
          alwaysUseBid: false,
          adserverTargeting: [
          {
              key: "hb_bidder",
              val: function(bidResponse) {
                  return bidResponse.bidderCode;
              }
          }, {
              key: "hb_adid",
              val: function(bidResponse) {
                  return bidResponse.adId;
              }
          }, {
              key: "hb_pb",
              val: function(bidResponse) {
                if (bidResponse.cpm > 200) { //10$
                  return (Math.floor(Math.min(parseInt(bidResponse.cpm), 500) / 10) * 10).toFixed(2);
                } else if (bidResponse.cpm > 40) { //1$
                  return (Math.floor(bidResponse.cpm)).toFixed(2);
                } else if (bidResponse.cpm > 30) { // 10c
                  return (Math.floor(bidResponse.cpm * 10) / 10).toFixed(2);
                }
                return bidResponse.cpm.toFixed(2);
              }
          }, {
              key: "hb_size",
              val: function(bidResponse) {
                  return bidResponse.size;
              }
          }, {
              key: "hb_cpm",
              val: function(bidResponse) {
                return bidResponse.cpm;
              }
          }, {
              key: "hb_dealid",
              val: function(bidResponse) {
                return (typeof bidResponse.dealId === 'undefined' ? "0" : bidResponse.dealId);
              }
          }
          ]
      }
  };

  
    var adUnits = [{ code: 'gpt-mpu1', sizes: [[300, 250], [300, 600]], bids: [{ bidder: 'rubicon', params: { accountId: '9470', siteId: '57242', zoneId: '518280' } }, { bidder: 'pubmatic', params: { publisherId: '156068', adSlot: '/4585/PCMag.com_International/PCMag_Int_ATF_MPU@300x250' } }, { "bidder": "appnexus", "params": { placementId: "6170554" } }, {"bidder": "kargo", "params": { placementId: "_u33Y3O9QyU" } }, { bidder: 'aol', params: { network: '4900.1', placement: '6536634', server: 'adserver-eu.adtech.advertising.com' } }, {bidder:'nanointeractive',params:{pid:'5b743b070ae89942e2430a23',name:'q'}}] },
