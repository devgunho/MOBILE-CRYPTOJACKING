								var errormsg = '<div class="agendalist">Errors:</div>';
								for(var i = 0; i < errors.length; i++) {
									var error = errors[i];
									errormsg += '<div class="agendalistitem"><li>' + error['message'] + '</li></div>';
								}
								
								$('#errordisplay').html(errormsg);
								$('#errordisplay').show();
							});
