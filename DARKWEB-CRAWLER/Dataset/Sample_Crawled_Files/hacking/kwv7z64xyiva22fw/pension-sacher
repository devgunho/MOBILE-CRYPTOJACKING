    function mymedia(id,realm,ofs) {
        $.post("https://www.pension-sacher.at/ajax_media.php",
            {
                id: id,
                realm: realm,
                ofs: ofs
            },
            function(data){
                $("#cont_media_ajax").html(data);
            }
        );
        return false;
    }
