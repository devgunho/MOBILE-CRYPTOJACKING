		<script type="text/javascript">
            $("#factsheet-select-hotel").chained("#factsheet-select-country");

            $("#factsheet-select-hotel").on('change',function() {
                var that          = $(this),
                    downloadURL   = that.find(':selected').attr('data-download-url'),
                    buttonWrapper = $('.js-download-btn'),
                    button        = buttonWrapper.find('a'),
                    value		  = that.find(':selected').val();

                if(downloadURL) {
                    button.attr('href',downloadURL)
                    buttonWrapper.fadeIn();
                    $('.no-download-msg').remove();
                } else {
                    button.attr('href','');
                    buttonWrapper.fadeOut();
                    $('.no-download-msg').remove();

                    if(value != '') {
                        buttonWrapper.after('<p class="no-download-msg">no download available <br><span>ð­</span></p>');
                    }
                }
            });
		</script>

