<div id="cookie-info">
	<p>Cookie-Info</p>
	<p>This website uses cookies to enhance your experience on pentahotels.com. For more information please see our <a href="/footer-links/cookie-policy">cookie policy</a> and our <a href="/footer-links/privacy-policy">privacy policy</a>.</p>
	<a class="dismiss" href="#">NOT OK!</a>
	<a class="close" href="#">OK</a>
</div>
<script>
	/* Cookie Hinweis */
	var cookieNote = (function () {

		var $cookieContainer = document.querySelector('#cookie-info');
		if ($cookieContainer == null) return;

		if (document.cookie.indexOf('penta_hidecookienote=1') != -1) {
			//console.log('cookie: ' + document.cookie);
			$cookieContainer.style.display = 'none';
		} else {
			$cookieContainer.style.display = 'block';
		}

		var getDomain = function (){
			if(document.domain.length){
				var parts = document.domain.replace(/^(www\.)/,"").split('.');

				//is there a subdomain?
				while(parts.length > 2){
					//removing it from our array
					var subdomain = parts.shift();
				}

				//getting the remaining 2 elements
				var domain = parts.join('.');

				return domain.replace(/(^\.*)|(\.*$)/g, "");
			}
			return '';
		};

		var setHideCookie = function(cookiename){
			var d = new Date();
			d.setTime(d.getTime() + (3600 * 1000 * 24 * 365));
			var expires = d.toUTCString();
			cookiename = cookiename || 'penta_hidecookienote';
			document.cookie = cookiename+'=1;path=/;expires=' + expires + '; secure; domain=.' + getDomain();
			$cookieContainer.style.display = 'none';
		};

		$cookieContainer.querySelector('a.dismiss').addEventListener('click', function (e) {
			e.preventDefault();
			setHideCookie();
		});

		$cookieContainer.querySelector('a.close').addEventListener('click', function (e) {
			e.preventDefault();
			setHideCookie();
			setHideCookie('penta_cookies');
		});

	})();
</script>

