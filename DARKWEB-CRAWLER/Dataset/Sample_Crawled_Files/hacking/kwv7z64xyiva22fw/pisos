<script>
    $(document).ready(function(){
    
        $('body').on('click', function(e){
            var clickInMultilanguageSelector = $(e.target)
                .parents()
                .is('#multilanguageSelector');
            
            if (clickInMultilanguageSelector) {
                //Quitar isComingSoon cuando se habilite idioma english
                var isComingSoon = $(e.target).parents().is('#comingSoon') || $(e.target).is('#comingSoon');

                if (!$('#languagesSelectors').hasClass('hide') && !isComingSoon){
                    $('#languagesSelectors').addClass('hide');
                }else{
                    $('#languagesSelectors').removeClass('hide');
                }
            }else{
                if (!$('#languagesSelectors').hasClass('hide')){
                    $('#languagesSelectors').addClass('hide');
                }
            }
        });
    });
</script>
