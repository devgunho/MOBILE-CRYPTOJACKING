	<div class="cd-overlay"><div class="overlay-inner"></div></div>
		<div id="fullpage"><div class="header-1 header-single-reviews">
    <div class="cover cover--09 animated-gradient">
        <div class="container">
            <div class="cover__content_box cover__content_box--09">
                <h1 class="cover__title cover__title--09">
                    <span class="cover__title_sub_review">Review From:</span>
                    <span class="cover__title_text cover__title_text--09 text-shadow">
                        Adam Scheuer                    </span>
                </h1>
