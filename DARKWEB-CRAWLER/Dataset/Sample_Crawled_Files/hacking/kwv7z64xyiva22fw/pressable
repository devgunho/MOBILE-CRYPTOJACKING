<script>
	// Click statement for Consent Banner
	jQuery(document).on("click", ".cc-dismiss", function(){
		if (setPressableCookieBanner()) {
			// Trigger general consent and fire cookies.
			dataLayer.push({'event': 'haveConsent'});
			console.log("Pressable - Cookies Accepted!");
		} else {
			alert('Please enable cookies in your browser.');	
		}
	});
	// Create Pressable GDPR Cookie and keep it in place for 365 days.
	function setPressableCookieBanner() {
		var d = new Date();
		d.setTime(d.getTime() + (180*24*60*60*1000)); // 365 days
		var expires = 'expires='+ d.toUTCString();
		document.cookie = 'pressable_cookie_banner=AT;' + expires + ";path=/";
		cookieEnabled = document.cookie.indexOf("pressable_cookie_banner")!=-1;
		if (cookieEnabled) {
			console.log("Pressable - GDPR Cookie Enabled");
			return true;	
		}
	}	

	// Click statement for Rejecting Cookies
	jQuery(document).on("click", ".cookie-opt-out", function(){
		if (clearListCookies("all")) {
			// Log cookie rejection.
			console.log("Pressable - Cookies Rejected!");
		}
	});
	// Delete Pressable GDPR Cookie and clear.
	function clearListCookies(type)
	{   
		var cookies = document.cookie.split(";");
		// If the user revokes consent on the cookie policy page, delete all cookies.
		if (type == "all") {
			for (var i = 0; i < cookies.length; i++)
			{   
				var spcook =  cookies[i].split("=");
				console.log(spcook[0]);
				deleteCookie(spcook[0]);
			}
			alert('The cookies have been removed!');
		} else {
		// If the consent cookie expires, delete the Advertising cookies.
			deleteCookie("_gat_UA-45728304-1");
			deleteCookie("_ga");
			deleteCookie("_gid");
			deleteCookie("_hjIncludedInSample");
			//deleteCookie("cookieconsent_status");
		}
		function deleteCookie(cookiename)
		{
			var d = new Date();
			d.setDate(d.getDate() - 1);
			var expires = ";expires="+d;
			var name=cookiename;
			//alert(name);
			var value="";
			document.cookie = name + "=" + value + expires + "; path=/;domain=pressable.com";   
			document.cookie = name + "=" + value + expires + "; path=/";                 
		}
		//window.location = ""; // TO REFRESH THE PAGE
	}

	// Geolocation / show consent banner / set cookies/tags using Google Tag Manager
	jQuery(document).ready(function () {
		// Geolocation
		jQuery.getJSON( "https://public-api.wordpress.com/geo/", function( data ) {
			// Console Log the country
			console.log(data.country_short);
			// Country codes in EU - and test country codes
			var countries = "AT BE BG CY CZ DE DK EE EL GR ES FI FR HR HU IE IT LT LU LV MT NL PL PT RO SE SI SK UK GB CH IS LI NO";
			// var user_country = "AT";
			var user_country = data.country_short;
			if (countries.includes(user_country.toString())) {
				var cookie = getCookie("pressable_cookie_banner");
				if (cookie !== "") {
					console.log("Pressable - GDPR Cookie Set");
					dataLayer.push({'event': 'haveConsent'});
					return true; // Cookie banner is set
					
				} else {
					// Trigger the cookie banner with consent request and always show the banner if the user is from the EU and hasn't accepted it.
					clearListCookies("some"); // just be sure that the advertising cookies are not set.
					dataLayer.push({'event': 'showCookieBanner'});
					console.log("Pressable - GDPR Consent Banner Triggered");
				}
			} else {
				// Trigger general consent and fire cookies.
				//dataLayer.push({'event': 'consentCookies'});
				dataLayer.push({'event': 'haveConsent'});
				console.log('Pressable - Not GDPR');
			}
		});
		function getCookie(cname) {
			var name = cname + "=";
			var ca = document.cookie.split(';');
			for(var i = 0; i < ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0) == ' ') {
					c = c.substring(1);
				}
				if (c.indexOf(name) == 0) {
					return c.substring(name.length, c.length);
				}
			}
			return "";
		}
	});
		
</script>
<script type="text/javascript">
  var name = encodeURIComponent("ajs_user_id").replace(/[\-\.\+\*]/g, "\\$&"),
      pattern = new RegExp("(?:(?:^|.*;)\\s*" + name + "\\s*\\=\\s*([^;]*).*$)|^.*$"),
      ajs_user_id = decodeURIComponent(document.cookie.replace(pattern, "$1")).replace(/"/g, '') || null;

  _tkq = window._tkq || [];

  if(ajs_user_id) {
    _tkq.push(['identifyUser', ajs_user_id, undefined]);
  }
</script>	<div style="display:none">
