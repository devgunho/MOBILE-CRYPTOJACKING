$('#more').click(function () {
if($('button span').hasClass('glyphicon-chevron-down'))
{
    $('#more').html('<span class="glyphicon glyphicon-chevron-up"></span> Close'); 
}
else
{      
    $('#more').html('<span class="glyphicon glyphicon-chevron-down"></span> Open'); 
}
}); 
