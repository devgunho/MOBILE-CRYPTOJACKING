				function cookieIsAllowed(cookieName) {
					return cookieSettings && cookieSettings.areCookiesAccepted() &&
					      		cookieSettings.isAllowedCookie(cookieName);
				}
				function addScriptTag(srcUrl) {
					$('head').append($('<script/>').attr({type: 'text/javascript', src: srcUrl}));
				}
				if (activateCookieSelection) {
					activateCookieSelection(function() {
							if (cookieIsAllowed('googleAnalytics')) {
					     		$('head').append(atob(''));
					    	}
						});
				}
				</script>
    	<script type="text/javascript">
        $(document).ready(function() {
            $(window).on('load resize', function () {
                changepos();
            });
            function changepos() {
                var toppos = ($('.slick-active').find("img").height()/2);
                $('.slick-arrow').css('top',toppos+'px');
            }
            $('.stage-slider').slick({
                slidesToShow: 1,
                slidesToScroll: 1
            }).on('afterChange',function(event){
                var toppos = ($('.slick-active').find(".slider-img-wrap").height()/2);
                $('.slick-arrow').css('top',toppos+'px');
            }).trigger('afterChange');
        });
	    </script>
