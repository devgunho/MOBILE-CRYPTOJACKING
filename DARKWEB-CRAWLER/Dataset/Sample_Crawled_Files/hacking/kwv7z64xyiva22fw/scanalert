<script>
$(document).ready(function() {
   $('.webshot div').click(function() { window.open('http://www.mcafee.com/'); });
   $('.site-logo').click(function() { window.open('http://www.mcafee.com/'); });


   $('.securitydetails .item').click(function() {
      var e = $(this);
      var n = $(this).attr('data-name');
      $('.securitydetails .item-detail[data-name='+n+']').slideToggle(300,function() {
         if($(this).is(':visible')) {
            $(e).find('i.fa').removeClass('fa-angle-down');
            $(e).find('i.fa').addClass('fa-angle-up');
         } else {
            $(e).find('i.fa').removeClass('fa-angle-up');
            $(e).find('i.fa').addClass('fa-angle-down');
         }
      });
   });
});
function recordEvent(category,action,label) {
   try { ga('send', 'event', category,action, label); } catch(e) { console.log(e); }
}
</script>

