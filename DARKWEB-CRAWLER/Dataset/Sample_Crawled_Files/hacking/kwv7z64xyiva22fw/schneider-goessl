	$('form select').change(function() {
		$(this).addClass('selected');
  	});
	
	$('form input').focus(function() {
		$(this).removeClass('error');
  	});
	
	$('.close-reveal-modal').click(function() {
		$('.reveal-modal').fadeOut(200);
  	});

	
	
	
	$( "#contactform" ).submit(function( event ) {
		event.preventDefault();
		
		// Validation
		errorcounter = 0;
		
		$('form').find('input').each(function(){
		    if($(this).prop('required')){
			    if ($(this).val()=="" || $(this).val()==null) { 
					//alert("Please enter only alphanumeric values for your advertisement title");
					$(this).addClass('error');
					errorcounter++;
				} else {
					//$(this).prop('disabled', true);
					//alert ('Danke!');
					
			    }
		    }
		});
		
		
		// E-Mail ï¿½berprï¿½fen
		var email = $('input[type=email]').val();
		if ($('input[type=email]').length > 0){
			var pattern = new RegExp(/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/);
				if (!pattern.test(email)) {
				//alert ('Email ist nicht korrekt!');
					$('input[type=email]').addClass('error');
					errorcounter++;
			}
		}
		//alert (errorcounter);
		
		if(errorcounter){
			//alert("Bitte fï¿½llen Sie alle Pflichtfelder aus!");
		} else {
			$.post("goldrausch/scripts/ajax_form_mailer.php", $("#contactform").serialize());
			fbq('track', 'Lead');
			$('form input').prop('disabled', true);
			$('form textarea').prop('disabled', true);
			$('form checkbox').prop('disabled', true);
			$('form radio').prop('disabled', true);
			$('form select').prop('disabled', true);
			$('.reveal-modal').fadeIn(200);
		}
		
		
		
	});
	
	// Accordion
	var acc = document.getElementsByClassName("accor");
	var i; var y;
	for (i = 0; i < acc.length; i++) {
	   acc[i].onclick = function(){
		   for (y = 0; y<acc.length; y++){
			 if(this != acc[y]){
				acc[y].classList.remove("active");
				acc[y].nextElementSibling.classList.remove("show");
			 }
		   }
		   this.classList.toggle("active");
		   this.nextElementSibling.classList.toggle("show");
	 }
	}
	
	
	
	
	$('input, textarea').placeholder();
	</script>
