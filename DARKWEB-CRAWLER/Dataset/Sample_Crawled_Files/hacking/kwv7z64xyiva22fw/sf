.08);color:#22211d}body.body--loading.body--loading .teaser__medium-wrapper{flex-grow:0;flex-shrink:0;margin:0;position:relative}body.body--loading.body--loading .teaser__medium-wrapper:after{position:absolute;pointer-events:none;content:"";width:100%;height:100%;background-color:#dadad2;top:0;left:0;box-sizing:border-box}body.body--loading.body--loading .teaser__medium-wrapper img{visibility:hidden;width:0}body.body--loading.body--loading .teaser__main{display:flex;flex-direction:column;flex-grow:1}body.body--loading.body--loading .teaser__content,body.body--loading.body--loading .teaser__main-outer-wrapper{display:none}body.body--loading.body--loading .teaser__content-shimmer{flex-grow:1;flex-shrink:1;margin:16px 12px 16px 16px}@media (min-width:1280px){body.body--loading.body--loading .teaser__content-shimmer{margin:24px}}body.body--loading.body--loading .media-still__indicator,body.body--loading.body--loading .teaser__links,body.body--loading.body--loading .teaser__opinion,body.body--loading.body--loading .teaser__scoreboard{display:none}body.body--loading.body--loading .ratio:before{content:"";width:1px;margin-left:-1px;float:left;height:0}body.body--loading.body--loading .ratio:after{content:"";display:table;clear:both}body.body--loading.body--loading .ratio--16-9:before{padding-top:56.25%}body.body--loading.body--loading .teaser--static{width:100%}@media (min-width:720px){body.body--loading.body--loading .teaser--size-xl .teaser__main{flex-direction:row}body.body--loading.body--loading .teaser--size-xl .teaser__medium-wrapper{flex-basis:65.75%}}body.body--loading.body--loading .teaser--longform{flex-direction:column}body.body--loading.body--loading .teaser__longform-medium{position:relative;background-color:#dadad2}body.body--loading.body--loading .teaser__longform-medium>.image{visibility:hidden}body.body--loading.body--loading .teaser__longform-medium>.image:before{content:"";width:1px;margin-left:-1px;float:left;height:0;padding-top:56.25%}@media (min-widt