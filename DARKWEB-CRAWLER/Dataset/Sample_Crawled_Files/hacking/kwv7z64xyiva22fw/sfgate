<div class="article-title">
<!-- src/business/templates/hearst/article/headline.tpl -->
    <h1 class="headline entry-title">Hackers infiltrate Google searches</h1>
            <h5 class="deck">Altered sites show up on Google results, snaring the unaware who click on sites</h5>
    <!-- e src/business/templates/hearst/article/headline.tpl --><!-- hearst/common/author_name.tpl -->
<p class="byline">Deborah Gage, Chronicle Staff Writer</p>
<!-- e hearst/common/author_name.tpl -->    <span class="timestamp">Published
        <time itemprop="datePublished"
            datetime="2008-04-02T11:00:00Z">
            4:00&nbsp;am PDT, Wednesday, April 2, 2008
        </time>
    </span>
</div>
<!-- e hearst/article/types/standard_title.tpl -->
    <div class="article-share">
