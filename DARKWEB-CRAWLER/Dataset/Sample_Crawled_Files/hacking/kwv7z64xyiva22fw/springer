<script type="text/javascript">
    function deepDyveResponse(data) {
        if(data.status === "ok") {
            document.getElementById("deepdyve").style.display = "inline-block";
            document.getElementById("deepdyve-link").setAttribute("href", data.url);
        }
    }
    var script = document.createElement('script');
    script.src = "//www.deepdyve.com/rental-link?docId=10.1007/s13389-013-0068-0&journal=2190-8516&fieldName=journal_doi&affiliateId=springer&format=jsonp&callback=deepDyveResponse";
    document.body.appendChild(script);
</script>
