<script type="text/javascript">sharedDataLayer = [{"session":{"authentication":{"loginStatus":"logged out"}},"customEvents":[],"page":{"sharedDataLayer":"present","category":{"pageType":"book-page"}},"user":{"segment":{},"profile":{}}}]
function sprMerge(current, update) {
  Object.keys(update).forEach(function(key) {
    if (current.hasOwnProperty(key)
            && typeof current[key] === 'object'
            && !(current[key] instanceof Array)) {
      sprMerge(current[key], update[key]);
    } else {
      current[key] = update[key];
    }
  });
  return current;
}
    </script>

