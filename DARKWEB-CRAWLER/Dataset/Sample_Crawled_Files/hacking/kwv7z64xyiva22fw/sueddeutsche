    <script id="creInit">
        (function(jq){
            var downloadScript = function(url, callback) {
                jq.ajax({
                    url: url,
                    dataType: 'script',
                    cache: true, // otherwise will get fresh copy every page load
                    success: callback
                })
            }

            var renderSwoosh = function(opcUrl, meteredCount, maxCount) {
                var ajaxUrl = opcUrl + '/assets/js/swoosh.js';
                var mode = 'WWW';
                downloadScript(ajaxUrl, function() {
                    // Wait for the document to be ready before the swoosh is rendered
                    jq(document).ready(function() {
                        opcSwoosh(jq, jq("#sueddeutsche"), meteredCount, maxCount, opcUrl, mode);
                    });
                });
            }

            var creListener = function(response){
                if (typeof(dataLayer) !== 'undefined') {
                    dataLayer.push({'event': 'c1TrackingResponse', 'c1TrackingResponse': response});
                }

                                    var data = response.data;

                                            // Render Swoosh
                        if (data.type == 'reminder') {
                            var read = Math.min(data.max_count, data.count);
                            renderSwoosh("https://offers.sueddeutsche.de", read, data.max_count);
                        }
                    
                    if (data.user_group_actions && data.user_group_actions.cms_snippet && data.user_group_actions.cms_snippet.parameter) {
                        var snippet = data.user_group_actions.cms_snippet.parameter;
                        if (SDE.C1 && SDE.C1.UserGroupActionProcessor) {
                            SDE.C1.UserGroupActionProcessor.processSnippet(snippet);
                        }
                    }
                            };

            downloadScript('https://www.sueddeutsche.de/cre-1.0/tracking/tracking.js', function() {
                if (typeof(cre_client) != "undefined") {
                    cre_client.set_page_view();
                                            cre_client.set_site('sz.de');
                                            cre_client.set_cms_id('1.4160039');
                                            cre_client.set_content_id('\/politik\/verschwundener-journalist-die-spur-des-kritikers-endet-im-konsulat-1.4160039');
                                            cre_client.set_heading('Die Spur des Kritikers endet im Konsulat');
                                            cre_client.set_entitlement_id('live_szde');
                                            cre_client.set_origin('web');
                                            cre_client.set_service_id('szde');
                                            cre_client.set_channel('2.236');
                                            cre_client.set_entitlement('metered');
                                            cre_client.set_doc_type('article');
