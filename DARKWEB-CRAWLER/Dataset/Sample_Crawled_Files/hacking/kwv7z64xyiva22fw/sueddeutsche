                                            cre_client.set_kicker('Verschwundener Journalist');
                                        cre_client.addListener(creListener);
                    cre_client.request();
                }
            });
        })(SDE.$);
    </script>
  
  
      

      
  <script>
    /**
     * Object holding information regarding user settings on SZ.de
     */
    SdeSettings =  {};

    /** Bit mask to signal that ad tracking has been disabled */
    SdeSettings._AD_TRACKING_DISABLED_MASK = 1;
    /** Bit mask to signal that video tracking has been disabled */
    SdeSettings._VIDEO_TRACKING_DISABLED_MASK = 2;

    /**
     * Private method reading the value of the cookie 'SdeDisabled'
     */
    SdeSettings._getSdeDisabledValue = function () {
        var value = document.cookie.replace(/(?:(?:^|.*;\s*)SdeDisabled\s*\=\s*([^;]*).*$)|^.*$/, "$1");
        value = parseInt(value, 10);
        return isNaN(value) ? 0 : value;
    };

    /**
     * Private method setting the value of the cookie 'SdeDisabled'
     */
    SdeSettings._setSdeDisabledValue = function (value) {
        var expiryDate = new Date();
        expiryDate.setFullYear(expiryDate.getFullYear() + 10);
        document.cookie = "SdeDisabled=" + value + ";expires=" + expiryDate + ";domain=.sueddeutsche.de;path=/";
    };

    /**
     * Private method checking the value of the cookie 'SdeDisabled' against the given mask.
     */
    SdeSettings._isDisabled = function (mask) {
        return Boolean(SdeSettings._getSdeDisabledValue() & mask);
    };

    /**
     * Private method adding the given mask to the value of the 'SdeDisabled'
     */
    SdeSettings._disable = function (mask) {
        if (!SdeSettings._isDisabled(mask)) {
            SdeSettings._setSdeDisabledValue(SdeSettings._getSdeDisabledValue() + mask);
        }
    };

    /**
     * Private method removing the given mask from the value of the 'SdeDisabled'
     */
    SdeSettings._enable = function (mask) {
        if (SdeSettings._isDisabled(mask)) {
            SdeSettings._setSdeDisabledValue(SdeSettings._getSdeDisabledValue() - mask);
        }
    };

    /**
     * Returns true if ad tracking has been disabled by the user
     */
    SdeSettings.isAdTrackingDisabled = function () {
        return SdeSettings._isDisabled(SdeSettings._AD_TRACKING_DISABLED_MASK);
    };

    /**
     * Returns true if video tracking has been disabled by the user
     */
    SdeSettings.isVideoTrackingDisabled = function () {
        return SdeSettings._isDisabled(SdeSettings._VIDEO_TRACKING_DISABLED_MASK);
    };
</script>

                    


