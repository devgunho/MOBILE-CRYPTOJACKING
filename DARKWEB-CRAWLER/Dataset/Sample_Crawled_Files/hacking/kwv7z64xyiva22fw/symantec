        var posterType = 'none';
        var posterPath = '';
        if( posterPath !== ''){
            var extension = posterPath.substring(posterPath.lastIndexOf('.') + 1);
            if(typeof extension!== 'undefined' && extension.length > 2){
                posterType = extension;
            }
        }
        var loop = true;
        var loopText = 'true';
        if(loopText === 'false'){
            loop = false;
        }
        
        var mute = true;
        var muteText = 'true';
        if(muteText === 'false'){
            mute = false;
