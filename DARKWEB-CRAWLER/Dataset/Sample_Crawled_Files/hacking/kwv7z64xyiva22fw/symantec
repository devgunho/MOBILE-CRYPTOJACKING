<span class="notDevkit loginFrameParent" style="display:none">
<script type="text/javascript">
	var checkfName = 0;
     var loadLoginFrame = function() {
    	 if(self === top) {
     	$.getJSON('/bin/symantec/displaystatus?SDE=Y&SSOURL=Y', function(data){
     	  var loginFrame = document.createElement('iframe');
          loginFrame.name = "naAutoLogin";
          loginFrame.title = "AutoLogin";
          loginFrame.style= "overflow-x:hidden;overflow-y:hidden;display:none;";
          loginFrame.frameborder="0";
     	  var samlURL = data.samlXmlStringDeflatedEncoded; 
          //console.log("samurl"+samlURL);
          loginFrame.src=data.SSOURL+"?SAMLRequest="+samlURL;
          loginFrame.sandbox="allow-forms allow-pointer-lock allow-popups allow-same-origin allow-scripts";

            loginFrame.onload = function() {
             // console.log('headercontainer.html: frame is loaded');

              if(checkfName < 1){
			  	setTimeout(updateLoginInfo , 500);
                checkfName ++;
                console.log("checkfName1"+checkfName);
              }

          };

          if($('#fn').text()===''){         
			$('.loginFrameParent').append(loginFrame);
		  }
     	});


    	 }
     };

     var updateLoginInfo = function() {
     	$.getJSON('/bin/symantec/displaystatus?fname=Y&UID=Y', function(data){
     		var fName = data.firstName ? data.firstName : '';
            var UID = data.UID ? data.UID : null;
     		console.log("headercontainer.html: test UID =" + UID);
     		if(typeof UID != undefined && UID != null){
     			$(".user-logged-in").css('display','block');
                $("#fn").text(fName); 
     			$(".not-logged-in").hide();
     			var samlUrl = '/bin/symantec/nsllogout?action=logout';
    			var logoutredirectURL = '';
    			var segmentString = '\/etc\/segmentation';
    			var ccPath = '\/etc\/clientcontext\/symantec';
    			var pagePath = '\/content\/symantec\/english\/en\/security\u002Dcenter\/threat\u002Dreport'
    			//signoutContainer.ClientContext(samlUrl, logoutredirectURL);
     			console.log("headercontainer.html: show and hide");
     			if(!ClientContext.get('profile').getInitProperty('isLoggedIn') && (UID !== ClientContext.get('profile').getInitProperty('authorizableId'))) {
     				$CQ(function() {
						CQ_Analytics.SegmentMgr.loadSegments(segmentString);
						CQ_Analytics.ClientContextUtils.init(ccPath, pagePath);
					});
     			}
     		} else {
               if(checkfName < 2 ){
               	setTimeout(updateLoginInfo , 1000);
                checkfName ++;
                console.log("checkfName2"+checkfName);
               }
            }
     	});  
     }
</script>
</span>


