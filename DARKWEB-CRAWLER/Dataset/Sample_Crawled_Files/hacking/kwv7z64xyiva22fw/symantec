	<script>
    	function getCookie(name) {
  			var value = "; " + document.cookie;
  			var parts = value.split("; " + name + "=");
  			if (parts.length == 2) return parts.pop().split(";").shift();
		}
		var pageTraversed = true;
	    if(((getCookie('pageTraversed')!=='true') || (sessionStorage.getItem('pageTraversed') !== null)) && 
        	((sessionStorage.getItem('pageTraversed') === null) || (JSON.parse(sessionStorage.getItem('pageTraversed')).indexOf(window.location.href) === -1))) {
			pageTraversed = false;
        }
		if(/.*Version.*Safari.*/.test(navigator.userAgent)){
            if(getCookie('pageTraversed')!=='true') {
				document.cookie = 'pageTraversed=true;max-age=300';
            }
	        if(sessionStorage.getItem('pageTraversed') === null) {
				sessionStorage.setItem('pageTraversed', JSON.stringify([window.location.href]));
			} else {
				var traversedArray = JSON.parse(sessionStorage.getItem('pageTraversed'));
	        	if(traversedArray.indexOf(window.location.href) === -1) {
					traversedArray.push(window.location.href);
       				sessionStorage.setItem('pageTraversed', JSON.stringify(traversedArray));
				} 
			}
		}
	</script>
