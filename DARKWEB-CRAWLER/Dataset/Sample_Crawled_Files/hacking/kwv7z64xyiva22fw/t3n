    function getSiteScrollDeep() {
        if (document.body !== null) {
            return Math.round(document.body.offsetHeight / window.innerHeight).toString();
        }

        return '';
    }

    function getDfptest() {
        if (typeof URLSearchParams !== 'function') {
            return '';
        }

        var urlParams = new URLSearchParams(window.location.search);
        if (urlParams.has('dfptest')) {
            return urlParams.get('dfptest');
        }

        return '';
    }

    function getUserSource() {
        if (typeof(Storage) === "undefined") {
            return '';
        }

        if (sessionStorage.getItem('user_source') !== null) {
            return sessionStorage.getItem('user_source');
        }

        var user_source = null;
        var referrer = document.referrer;

        if (isReferrerSearch(referrer)) {
            user_source = 'search';
        } else if (isUtmMediumEquals(referrer, 'social')) {
            user_source = 'social';
        } else if (isReferrerAmp(referrer)) {
            user_source = 'amp';
        } else if (isUtmMediumEquals(referrer, 'newsletter')) {
            user_source = 'newsletter';
        } else if (isReferrerOrganic(referrer)) {
            user_source = 'organic';
        }

        sessionStorage.setItem('user_source', user_source);
        return user_source;
    }

    function isReferrerSearch(referrer) {
        var searchEngines = ['google', 'google.de', 'google.com', 'news.google.com', 'news.google.de', 'bing', 'bing.de', 'bing.com', 'ecosia.org', 'duckduckgo.com'];

        searchEngines.forEach(function(searchEngine) {
            if ('https://' + searchEngine === referrer) {
                return 'search'
            }
        });

        return false;
    }

    function isReferrerAmp(referrer) {
        return referrer.indexOf('/amp/') !== -1;
    }

    function isReferrerOrganic(referrer) {
        return referrer === "";
    }

    function isUtmMediumEquals(referrer, expected) {
        if (typeof URLSearchParams !== 'function') {
            return false;
        }

        var urlParams = new URLSearchParams(window.location.search);

        return urlParams.has('utm_medium') && urlParams.get('utm_medium') === expected;
    }

    var tags = 'pinterest,messenger,snapchat,google,tumblr,twitter,themenwoche marketing,social networks,instagram,seo,crm,facebook,templates';
    var tagsArr = tags.split(',');
    var pageType = '';
                        pageType = 'news-post';
            
    googletag.cmd.push(function () {
        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs(true, true);
        googletag.pubads().setTargeting('hostname', window.location.hostname);
                googletag.pubads().setTargeting('pathname', window.location.pathname.substr(0,40));
        googletag.pubads().setTargeting('pagetype', pageType);
        googletag.pubads().setTargeting('login_status', 'nologin');
        googletag.pubads().setTargeting('dfptest', getDfptest());
        googletag.pubads().setTargeting('ressort', 'marketing');
        googletag.pubads().setTargeting('tags', tagsArr);
                googletag.pubads().setTargeting('identifier', 'news-article-378413');
        googletag.pubads().setTargeting('user_source', getUserSource());

        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
            googletag.pubads().setTargeting('scroll_deep', getSiteScrollDeep());
        });

        googletag.enableServices();
    });

