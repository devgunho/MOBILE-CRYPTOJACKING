    <script>

        $(function () {

            $("#home-slider").carousel();

            $('#home-slider .carousel-inner .item').each(function () {
                var imgUrl = $(this).find('img:first').attr('href');
                $(this).css({
                    'background-image': 'url(' + $(this).find('img:first').attr('src') + ')',
                    'height': $(window).height()
                });
                $(this).find('.container').css({ 'height': $(window).height() });//, .container .carousel-caption
                $(this).find('h1').addClass('font-effect-destruction');
                $(this).find('img:first').remove();
            });

            //$(' #home-slider .carousel-inner .item .container .carousel-caption').each(function () {
            //    $(this).append('<span>Caption: ' + $(this).height() + '</span>');
            //});

            $("#home-slider").swiperight(function () {
                $(this).carousel('prev');
            });

            $("#home-slider").swipeleft(function () {
                $(this).carousel('next');
            });

            var copyright_date = $('.copyright-date').html();
            var this_year = new Date().getFullYear();
            if(copyright_date!=this_year){
                $('.copyright-date').html(this_year);
            }

        });


        var CookieGet = $.cookie('incident-notification');
        if (CookieGet != null) {
            $('.notification-incident').remove()
        };

        $(function(){
            $('.notification-incident a').click(function(){ 
                setIncidentCookie();
                $('.notification-incident').remove(); //or hide
            });
        });

        function setIncidentCookie(cname, cvalue) {
            $.cookie('incident-notification', 'dismiss', { path: '/', domain: location.hostname });
        };   

    </script>

