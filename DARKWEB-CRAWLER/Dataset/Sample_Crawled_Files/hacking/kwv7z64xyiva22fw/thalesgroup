<script>
	

$(window).load(function(){
	var line = 1;



  $('.link-load-more').on( 'click', function(e) {

   

		e.preventDefault();
		
		
		 $.ajax({
        url: "functions.cfc?method=loadMore&topicId=63521,63541,63542&maxrows=15&line="+line,

        type: "GET",
        dataType: "html",
        success: function (data) {
			if (data != "") {
				
				result = $.parseHTML(data);
				
				
				$(result).find(".bloc").each( function( index, value ) {
				
				  if( index %3 == 0 ){
				  	$(".col1").append( value );
				  }else if(index %3 == 1 ){
						$(".col2").append( value );
				  
				  }else{
						$(".col3").append( value );
				  }
				
				
				});
			 //	$(".tweets").append( data );
				line++;
				
		
			}
			else{
				$(".link-load-more").hide();
			}
	  
	  
        },
        error: function (xhr, status) {
            alert("Sorry, there was a problem!");
        },
        complete: function (xhr, status) {
           
        }
    });
	
  });


});

</script>




