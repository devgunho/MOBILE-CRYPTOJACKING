    <script>
        var page_infinite=1;
        var processing=false;

        jQuery(document).ready(function() {

            jQuery("#infinite-morebutton").click( function(e){
                if (processing) return false;
                processing = true;

                jQuery.ajax({
                    url: "/wp-content/themes/tomshw/frontend-requests.php",
                    quietMillis: 100,
                    dataType: 'json',
                    type: "GET",
                    data: {
                        Action: "newsnextpage",
                        vertical: 'home',
                        page: page_infinite
                    }
                }).done( function(dataval){
                    if (dataval.status=="success")
                    {
                        jQuery('#infinite-divsection').append(dataval.htmlResult);
                        myLazyLoadBackground.update();
                        page_infinite++;
                    }
                    else if (dataval.status=="error")
                    {
                        console.log(dataval.message);
                    }
                }).fail(function( jqXHR, textStatus ) {
                    console.log( "Request failed: " + textStatus );
                });
                processing = false;
                return false;
            });
        });
    </script>
    <div class="container content_container infinitescroll">
