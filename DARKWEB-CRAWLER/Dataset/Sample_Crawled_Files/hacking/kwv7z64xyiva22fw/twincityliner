    var TwinCitySearch = function () {
        var iframe;
        function resize(e){
            if(e.origin == "https://ticket.twincityliner.com:443" || e.origin == "https://ticket.twincityliner.com" || e.origin == "http://ticket.twincityliner.com") {
                iframe.height = e.data;	
