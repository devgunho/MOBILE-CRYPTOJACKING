        <script type="text/javascript">
    function fetch_locations(){

        var selectedRegion = [];
        function checkedRegion() {
            var selectedRegion = [];
            jQuery('input:checkbox[id^="sort-checkbox-grid_"]').each(function() {
                if( jQuery(this).is(':checked')){
                    selectedRegion.push(jQuery(this).val());
                }
            });
            return selectedRegion;
        }

        jQuery.ajax({
            url: 'https://www.umww.com/wp-admin/admin-ajax.php',
            type: 'post',
            beforeSend: function () {
                jQuery('.location-load').css('display','inline-block');
            },
            data: { action: 'data_fetch_loc', location_name: jQuery('#location_name').val(), region: checkedRegion()},
            success: function(data) {
                jQuery('.location-load').css('display','none');
                jQuery('#location_grid').html( data );
                sr.reveal('.box', { reset: true, delay: 20, scale: 0.1, easing: 'ease-in-out', distance: '0'}, 100);
            }
        });

    }
    jQuery(document).ready( function () {
        fetch_locations();
    });
</script>

    <script type="text/javascript">
        function fetch_locations_map(){

            var selectedRegion = [];
            function checkedRegion() {
                var selectedRegion = [];
                jQuery('input:checkbox[id^="sort-checkbox_"]').each(function() {
                    if( jQuery(this).is(':checked')){
                        selectedRegion.push(jQuery(this).val());
                    }
                });
                return selectedRegion;
            }

            jQuery.ajax({
                url: 'https://www.umww.com/wp-admin/admin-ajax.php',
                type: 'post',
                beforeSend: function () {
                    jQuery('.location-map-load').css('display','inline-block');
                },
                data: { action: 'data_fetch_loc_map',  location_name_map: jQuery('#location_name_map').val(), region_map: checkedRegion() },
                success: function(data) {
                    jQuery('.location-map-load').css('display','none');
                    jQuery('#location_grid_map').html( data );
                    setTimeout(function(){ jQuery(".active-1").trigger('click'); }, 2000);
                }
            });



        }
    </script>

        <script type="text/javascript">
        jQuery(document).ready(function($){
            jQuery(".ajax-search_text").keyup( function () {
                var keyValue = jQuery(this).val();
                if(keyValue){
                    jQuery.ajax({
                        url: 'https://www.umww.com/wp-admin/admin-ajax.php',
                        type: 'post',
                        beforeSend: function () {
                            jQuery('i.loader-text').css('display','inline-block');
                            jQuery('i.search-text').css('display', 'none');
                        },
                        data: { action: 'get_search_list_via_ajax',  text: keyValue },
                        success: function(data) {
                            jQuery('i.loader-text').css('display','none');
                            jQuery('i.search-text').css('display', 'inline-block');
                            jQuery('.um-ajax-result').html(data);
                        }
                    });

                }else{
                    jQuery(".um-ajax-result").html("");
                }
            });
        });
    </script>

