        function alertsHidden() {
            if (getAlertSessionStorage() != null) {
                alertBox = document.getElementsByClassName("alert");
                alertBox[0].className = "alert hide";
                alertBoxElem = alertBox[0];
                alertBoxElem.setAttribute("aria-hidden", "true");
            }
        }

