					}				
							
				function writeReprintLink(){
					document.write('<a href="javaScript:RightslinkPopUp()">Reprints &amp; Permissions</a> | ');
				}
				function writeSubscribeToLink(){	
				var url = document.location.toString();
				var urlArray = url.split("/")
				var nurl = "";
				for (i = 3; i < urlArray.length - 1; i++)
				{				
					if(i<urlArray.length-2){
						nurl += urlArray[i] + "|";
					}
					else
					{
						nurl += urlArray[i];
					}				
				}				
				document.write('<a href="http://asp.usatoday.com/marketing/rss/rsstrans.aspx?ssts=' + escape(nurl) + '"><img src="http://images.usatoday.com/marketing/_images/rssbox.gif" width="36" height="14" border="0" align="absmiddle" alt="Subscribe to stories like this" style="border:0; margin:0; padding:2px 0 0 0"/></a>');					
				}
//-->					
</script></SPAN><!-- /EdSysObj -->
 </td>
</tr>
</table><!--endclickprintexclude-->
<!-- /EdSysObj -->
									<!--endclickprintinclude--><DIV align="right"></DIV><DIV id="relatedNews"></DIV></TD>
</TR>
