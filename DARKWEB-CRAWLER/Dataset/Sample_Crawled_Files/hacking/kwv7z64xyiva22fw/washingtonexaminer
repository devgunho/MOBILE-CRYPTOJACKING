        function throttled(delay, fn) {
            let lastCall = 0
            return function (...args
        )
            {
                const now = (new Date()).getTime()
                if (now - lastCall < delay) {
                    return
                }
                lastCall = now
                return fn(...args
            )
            }
        }

        var handler = function () {
            var windowViewBottom = window.scrollY + window.innerHeight
            var adViewTop = document.getElementById("ga_post_roll_1").offsetTop
            if (windowViewBottom >= adViewTop) {
                var prURL = "/trump-signs-fisa-reauthorization-says-different-from-law-that-was-so-wrongly-abused-during-the-election/post_roll_1"
                var prTitle = "Trump signs FISA reauthorization, says different from law that &#x27;was so wrongly abused during the election&#x27; | Post Roll 1"
                window.removeEventListener('scroll', throttler_ga_post_roll_1)
                console.log('POSTROLL REGISTERED -- pageview sent');
                console.log('-- url: ' + prURL)
                console.log('-- title: ' + prTitle)
                ga('send', 'pageview', {
                    'page': prURL,
                    'title': prTitle,
                });
            }
        }
        var throttler_ga_post_roll_1 = throttled(50, handler)
        window.addEventListener('scroll', throttler_ga_post_roll_1)
    </script> 
