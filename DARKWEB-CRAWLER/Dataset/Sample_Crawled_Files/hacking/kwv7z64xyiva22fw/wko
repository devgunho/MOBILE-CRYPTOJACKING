<script src="https://www.wko.at/Content.Node/kampagnen/Karriereportal-WKOE/js/jquery.min.js"></script>
<script>
	jQuery(function (){
		// No-Script-Fallback
		jQuery('.map-kontakt:not(".bezirkskarte-oe")').addClass('hidden'); // hide contacts except OE
		jQuery('.map-container').removeClass('hidden'); // show Map + Combo
		/* CONTACT MAP */
		jQuery.getScript("https://www.wko.at/Content.Node/kampagnen/Karriereportal-WKOE/js/jquery.maphilight.min.js", function () {
			// map
			var initMaphilight = function(selector) {
				// show activated map and init maphilight
				jQuery(selector).find('img').maphilight({
					// variables from jquery.maphilight.min.js
					stroke: true,
					strokeColor:'ff2600',
					strokeOpacity:1,
					strokeWidth:1,
					fillColor: 'ff2600',
					fillOpacity:1
				});
			};
			initMaphilight('.bezirkskarteoe');
			jQuery('.bezirkskarteoe area').on('click', function(e) {
				jQuerythis = jQuery(this);
				// area stays active
				jQuery('.bezirkskarteoe area').each(function() {
					hData = jQuery(this).data('maphilight') || {}; // get
					hData.alwaysOn = jQuery(this).is(jQuerythis); // modify
					jQuery(this).data('maphilight', hData ).trigger('alwaysOn.maphilight'); // set
				});
				var activatedMapSelektor = jQuerythis.data('activate');
				// change value of select
				jQuery('.choose-state').val(jQuerythis.attr('title'));
				// display contact
				$('.map-kontakt').addClass('hidden').parent().find(activatedMapSelektor).removeClass('hidden');
			});
			// selectboxes 
			jQuery('select.bezirkskarteoe').on('change', function(e) {
				jQuerythis = jQuery(this).find(':selected');
				var activatedMapSelektor = jQuerythis.data('activate');
				$('.bezirkskarteoe area[data-activate="' + activatedMapSelektor + '"]').click();
				// display contact
				$('.map-kontakt').addClass('hidden').parent().find(activatedMapSelektor).removeClass('hidden');
			}); 
		});
		/* END CONTACT MAP */
	});
</script>
