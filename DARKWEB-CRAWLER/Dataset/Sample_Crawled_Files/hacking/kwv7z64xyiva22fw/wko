<script type="text/javascript" src="/assets/a23987d0/c_727255b15bc82751818f6fcef32a29cc.js"></script>
<script type="text/javascript">
/*<![CDATA[*/
var wkoLazyLoad = new LazyLoad();
//counts the words in main and pushes the count to the google tag manager datalayer

var debug = false;

var temp = $('main').clone();

// add selectors to the blacklist to exclude them from word counting
// please note that this is a simple string with ccs selectors not an array/object!
var blacklist = 'script, style';
blacklist += ', img, svg, .photoswipe-gallery, .init-gallery';
blacklist += ', video, .video, .video-js';
blacklist += ', audio';
blacklist += ', form, .fg-form, .faz-suche';
blacklist += ', .event-search__form, .search-no-results, .l-previews, .search-categories';
blacklist += ', .social-links';
blacklist += ', .overview, .content-overview';
blacklist += ', .ticker, .countdown, .marktplatzticker, .epu, .woman2business, .votingtool';
temp.find(blacklist).remove(); // video-js vjs-default-skin news-item--video__player vjs-big-play-centered vjs-paused vjs_video_3-dimensions vjs-fluid vjs-controls-enabled vjs-workinghover vjs-v6 vjs-youtube vjs-user-inactive

// append whitespace to make sure there is at least one whitespace after every element
// we want to exclude inline-tags though. 
// the not-selectors can/should be extened as needed. 
// Otherwise there would be instances where a punctutation character after an inline tag is seperated with a whitespace
if(false && debug){
    temp.find('*:not(span):not(strong):not(a)').append('-x-X ').prepend(' X-x-');
}else{
    temp.find('*:not(span):not(strong):not(a)').append(' ').prepend(' ');
}

// get text-only representation and remove/reduce duplicate whitespaces
var textContent = temp.text().trim().replace(/\s+/g, " "); 

if(debug){
    textContent = textContent.replace(/-x-X/g,'\n---|\n');  
    textContent = textContent.replace(/X-x-/g,'\n|---\n\t');  
}

wordCount = countWords(textContent);

function countWords(str) {
    return str.split(' ')
    .filter(function(n) { return n != '' })//we could maybe extend the filter function to not count punctuation characters etc.
    .length;
}

if(debug){
    console.log('this text contains '+wordCount+' words \n\n-['+textContent+']-');
}


dataLayer.push({'pageWordcount': wordCount.toString()});
/*]]>*/
</script>
</body>
</html>


