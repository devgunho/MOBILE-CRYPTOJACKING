        <script>
        var currentActiveIndex = -1;
        var carousel = {};

        $(document).ready(function() {
            carousel = $("#carousel");

            carousel.Cloud9Carousel({
                yRadius: 20,
                xRadius: carousel.width() / 2.8,
                yOrigin: 20,
                xOrigin: carousel.width() / 2,
                itemClass: 'carousel-item',
                buttonLeft: $("#carousel-buttons > .left"),
                buttonRight: $("#carousel-buttons > .right"),
                autoPlay: 1,
                autoPlayDelay: 2500,
                bringToFront: true,
                onRendered: function(carousel) {
                    var index = carousel.nearestIndex();
                    if (index !== currentActiveIndex) {
                        //remove the class from the current active item
                        if (currentActiveIndex > -1) {
                            $(carousel.items[currentActiveIndex].element).removeClass('active');
                        }
                        //and make this one current
                        $(carousel.items[index].element).addClass('active');
                        currentActiveIndex = index;
                    }
                },
                onLoaded: function() {
                    carousel.css('visibility', 'visible');
                }
            });
        });
        
        $(window).resize(function() {
            console.log('resize');
            console.log(carousel.width() / 2);
            carousel.data("carousel").xOrigin = carousel.width() / 2;
        });

    </script>

    
