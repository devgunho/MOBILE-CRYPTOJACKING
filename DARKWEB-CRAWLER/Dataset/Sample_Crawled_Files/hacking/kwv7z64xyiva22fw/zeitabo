    <script>
        $(document).ready(function () {

            $("#tabs-invoice").hide();
            $("#tabs-direct-debit").hide();
            $("#tabs-credit-card").hide();

            setTimeout(function () {
                $("#payment-methods input:checkbox:first").trigger('click');
            }, 500);

        })
    </script>

    <script>
        $(document).ready(function () {

            
                $('#GiftSku').val('1130975');
                $('#ProductVariantSku').val('1830545-1130975');
            

        $('body').prepend($('<div class="modal-overlay hidden" />'));

        $('.modal-close-button').on('click', function() {
            closeModal();

        });
        $('.gift-selector a').on('click', function(e) {
            var sku = $(this).data('selected-sku');
            var productVariantSku = $(this).data('selected-product-variant-sku');
            onGiftSelected(sku, productVariantSku);
            closeModal();
            e.preventDefault();
        });

        var options = {
            itemContainer: '.slider',
            itemSelector: '.product-gift-image',
            start: 6,
            scrollwheel: false,
            loop: true,
            style: 'flat',
            spacing: 0.2,
            buttons: 'custom',
            buttonPrev: '<i class="fa fa-arrow-circle-left fa-2x after"></i>',
            buttonNext: '<i class="fa fa-arrow-circle-right fa-2x after"></i>',
            onItemSwitch: onItemSwitch
        };

        var optionspopup = {
            itemContainer: '.slider',
            itemSelector: '.product-gift-image',
            start: 6,
            scrollwheel: false,
            loop: true,
            style: 'flat',
            spacing: 0,
            buttons: 'custom',
            buttonPrev: '<i class="fa fa-arrow-circle-left fa-2x after"></i>',
            buttonNext: '<i class="fa fa-arrow-circle-right fa-2x after"></i>',
            onItemSwitch: onItemSwitchPopup
        };

            var pageCarousel = $('div.flipster-carousel').flipster(options);
            var popupCarousel = $('div.flipster-carousel-popup').flipster(optionspopup);

            $.subscribe('onPopupGiftChange', function(e, data) {
                setActiveItemBySku(pageCarousel, data);
            });
            $.subscribe('onPageGiftChange', function(e, data) {
                setActiveItemBySku(popupCarousel, data);
            });

        $('.open-modal').on('click', function(e) {
            var sku = $(this).data('option-sku');
            var modal = $('#gift-details-desktop');
            $('body').prepend(modal); //simulate moving the element to the top of the DOM
            openModal(sku, modal);
            e.preventDefault();
        });

        function closeModal() {
            $('.modal-overlay').hide();
            $('.modal-box').hide();
            $('.modal-box').find('.gift-item').hide();
        }

        function onItemSwitch(currentItem, previousItem) {
            var sku = $(currentItem).data('option-sku');
            var productVariantSku = $(currentItem).data('product-variant-sku');
            $('#GiftSku').val(sku);
            $('#ProductVariantSku').val(productVariantSku);
            $.publish('onPageGiftChange', sku);
        };

        function onItemSwitchPopup(currentItem, previousItem) {
            var sku = $(currentItem).data('option-sku');
            var modal = $('#gift-details-desktop');
            changeActiveImage(sku, modal);
            $.publish('onPopupGiftChange', sku);
        };

        })
    </script>

    

<script src="https://scripts.zeit.de/static/js/webtrekk/webtrekk_v4.4.1.js"></script>

<script>

    $(document).ready(function () {

        if (typeof webtrekkV3 === "function") {
            var webtrekk = {
                linkTrack: "standard",
                linkTrackAttribute: "data-wt-click",
                heatmap: "0",
                form: "0"
            };

            var wt = new webtrekkV3(webtrekk);
            wt.trackId = "981949533494636";
            wt.contentId = "verlag.aboshop.ZEIT.Vorteilsabo.aboshop.productExtentionWithGift.zabo|abo.zeit.de/zeit-leo-magazin-vorteilsabo/";
            wt.orderValue = "4.60";
            wt.orderId = "";
            wt.product = "ZEIT LEO â Das Magazin f&#252;r Kinder";
            wt.productQuantity = "1";
            wt.productCost = "4.60";
            wt.productStatus = "conf";
            wt.productCategory = { 1: "aboshop" }
            wt.customEcommerceParameter = {
                1: "12",
                2: "1830545",
                5: "",
                6: "rechnung"
            };

            wt.customParameter = {
                2: "abo_angebot"
            };

            $.subscribe("onSubmit", function () {
                     wt.sendinfo(); 
            });

            $.subscribe("onSalutationChange", function (e, data) {
                wt.customEcommerceParameter["5"] = data;


            });

            $.subscribe("onPaymentMethodChange", function (e, data) {
                wt.customEcommerceParameter["6"] = data;
            });

            $.subscribe("onProductChange", function (e, product) {
                wt.product = product.Name;
                wt.productCost = product.Price;
                wt.orderValue = product.Price;
                wt.customEcommerceParameter["2"] = product.Sku;
            });
        }
    });

</script>

