var owa_baseUrl = '//track.qcri.org/';
var owa_cmds = owa_cmds || [];
owa_cmds.push(['setSiteId', 'www.aljazeera.com']);
owa_cmds.push(['setCustomVar', 1, 'sampleRate', owa_sampleRate, 'page']);
owa_cmds.push(['trackPageView']);
 
(function() {
	function load_owa() {	
		var s = document.createElement('script');
		s.type = 'text/javascript'; s.async = true;
		s.src = owa_baseUrl + 'v2-track-min.js';
		var x = document.getElementsByTagName('script')[0];
