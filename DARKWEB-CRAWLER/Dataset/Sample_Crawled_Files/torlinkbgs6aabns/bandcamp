        <div id="discover" class="bd-section discover scroll-trigger">
            <div class="row filter-types">
    <h2 class="section-title">Discover
        <em data-bind="visible: locations.selectorVisible() || formats.selectorVisible() || dates.selectorVisible()" >filter by:
        <a data-bind="css: { 'selected': locations.selected }, click: locations.toggleSelected, animateVisible: locations.selectorVisible"><span class="filter-icon icon-loc"></span>location</a>
        <a data-bind="css: { 'selected': formats.selected }, click: formats.toggleSelected, animateVisible: formats.selectorVisible"><span class="filter-icon icon-format"></span>format</a>
        <a data-bind="css: { 'selected': dates.selected }, click: dates.toggleSelected, animateVisible: dates.selectorVisible" ><span class="filter-icon icon-time"></span>time</a>
        </em>
        <a href="/tags" class="alltags">browse by tags</a>
    </h2>
</div>

<div class="discover-bar genre-bar" data-bind="
    style: { 'background-color': genres.backgroundColor },
    css: { 
        'overflow-left': genres.scrollLeftVisible,
        'overflow-right': genres.scrollRightVisible,
    },
    with: genres
" style="background-color: hsla(194, 48%, 50%, 1)">
    <div class="row">

        <!-- client rendered -->
        <div class="discover-pills discover-genres client-rendered" data-bind="
            foreach: pills
        ">
