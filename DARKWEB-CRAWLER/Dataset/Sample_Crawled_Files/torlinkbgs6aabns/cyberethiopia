 <script>
        var modeValue = "amharic";

        $(document).ready(function () {
            //it also possible to switch from amharic to english anytime by setting the mode property to "english" or "amharic"
            //see melastudios.com for example
          var textarea =  $("#username").amharicKeyboard(); //to initialise with english: $("#target").amharicKeyboard({mode:"english"});
            $("#password").amharicKeyboard();
			$(".mode").click(function () {
                var $this = $(this);
                //var textarea = $("#target").amharicKeyboard();
                $(".mode").removeClass("selected");
                $this.addClass("selected");

                if ($this.val() === "English")
                    modeValue = "english";
                else
                    modeValue ="amharic";

                textarea.changeMode(modeValue);


            });
        });
    </script>
    
