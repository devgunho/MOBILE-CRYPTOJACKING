    	}                

    	function initialValidation(fieldInput, label, array) {
    		var inputValue = fieldInput.value;
        	var inputLength = inputValue.length;
        	var arrayLength = array.length;
      
        	if(fieldInput != PhoneInput && inputLength < 2){
           		return false;
        	}
        	else if(fieldInput != PhoneInput && fieldInput != CompanyInput && /^[A-Za-z\-. ]+$/.test(inputValue)==false){
          		return false;
        	}
        	else if(fieldInput != CompanyInput){
        		for (var i = 0; i < arrayLength; i++) {
      				if(inputValue == array[i]){
              			return false;
            		}
    			}
        	}
        	return true;
  		}
    
    	function fieldValidation(fieldInput, label, array) {
    		var inputValue = fieldInput.value;
        	var inputLength = inputValue.length;
        	var arrayLength = array.length;
      
        	if(fieldInput != PhoneInput && inputLength < 2){
           		setTimeout(function(){ form.showErrorMessage("Must be at least 2 characters in length", label); }, 100);
           		return false;
        	}
        	else if(fieldInput != PhoneInput && fieldInput != CompanyInput && /^[A-Za-z\-. ]+$/.test(inputValue)==false){
          		setTimeout(function(){ form.showErrorMessage("No numbers or special characters", label); }, 100);
          		return false;
        	}
        	else if(fieldInput != CompanyInput){
        		for (var i = 0; i < arrayLength; i++) {
      				if(inputValue == array[i]){
      					setTimeout(function(){ form.showErrorMessage("This does not appear to be a valid entry", label); }, 100);
              			return false;
            		}
    			}
        	}
        	return true;
  		}

    	//Initial Validation for pre-filled form
    	if(initialValidation(FirstNameInput, FirstNameLabel, blacklist)){
      		FirstNameValid = true;
      		if(FirstNameValid && LastNameValid && PhoneValid && CompanyValid){
        		form.submittable(true);
        		submitButtonWrap.innerHTML = oldHTML;
      		}
    	}   
    	else{
      		FirstNameValid = false;
      		form.submittable(false);
      		submitButtonWrap.innerHTML = newHTML;
    	}
   
    	if(initialValidation(LastNameInput, LastNameLabel, blacklist)){
      		LastNameValid = true;
      		if(FirstNameValid && LastNameValid && PhoneValid && CompanyValid){
        		form.submittable(true);
        		submitButtonWrap.innerHTML = oldHTML;
      		}
    	}   
    	else{
      		LastNameValid = false;
      		form.submittable(false);
      		submitButtonWrap.innerHTML = newHTML;
    	}
    
    	if(initialValidation(PhoneInput, PhoneLabel, blacklist)){
      		PhoneValid = true;
      		if(FirstNameValid && LastNameValid && PhoneValid && CompanyValid){
        		form.submittable(true);
        		submitButtonWrap.innerHTML = oldHTML;
      		}
    	}   
    	else{
      		PhoneValid = false;
      		form.submittable(false);
      		submitButtonWrap.innerHTML = newHTML;
    	}
  
    	if(initialValidation(CompanyInput, CompanyLabel, blacklist)){
      		CompanyValid = true;
      		if(FirstNameValid && LastNameValid && PhoneValid && CompanyValid){
        		form.submittable(true);
        		submitButtonWrap.innerHTML = oldHTML;
      		}
    	}   
    	else{
      		CompanyValid = false;
      		form.submittable(false);
      		submitButtonWrap.innerHTML = newHTML;
    	} 
    
    	//Validation on blur - non-pre-filled forms
    	FirstNameInput.onblur = function() {
      		if(fieldValidation(FirstNameInput, FirstNameLabel, blacklist)){
        		FirstNameValid = true;
        		if(FirstNameValid && LastNameValid && PhoneValid && CompanyValid){
          			form.submittable(true);
          			submitButtonWrap.innerHTML = oldHTML;
        		}
      		}   
      		else{
        		FirstNameValid = false;
        		form.submittable(false);
        		submitButtonWrap.innerHTML = newHTML;
      		}
    	} 
    	LastNameInput.onblur = function() {
      		if(fieldValidation(LastNameInput, LastNameLabel, blacklist)){
        		LastNameValid = true;
        		if(FirstNameValid && LastNameValid && PhoneValid && CompanyValid){
          			form.submittable(true);
          			submitButtonWrap.innerHTML = oldHTML;
        		}
      		}   
      		else{
        		LastNameValid = false;
        		form.submittable(false);
        		submitButtonWrap.innerHTML = newHTML;
      		}
    	}
    	PhoneInput.onblur = function() {
      		if(fieldValidation(PhoneInput, PhoneLabel, blacklist)){
        		PhoneValid = true;
        		if(FirstNameValid && LastNameValid && PhoneValid && CompanyValid){
          			form.submittable(true);
          			submitButtonWrap.innerHTML = oldHTML;
        		}
      		}   
      		else{
        		PhoneValid = false;
        		form.submittable(false);
        		submitButtonWrap.innerHTML = newHTML;
      		}
    	}
    	CompanyInput.onblur = function() {
      		if(fieldValidation(CompanyInput, CompanyLabel, blacklist)){
        		CompanyValid = true;
        		if(FirstNameValid && LastNameValid && PhoneValid && CompanyValid){
          			form.submittable(true);
          			submitButtonWrap.innerHTML = oldHTML;
        		}
      		}   
      		else{
        		CompanyValid = false;
        		form.submittable(false);
        		submitButtonWrap.innerHTML = newHTML;
      		} 
    	}
      
  	}
    
  });    
  
</script> 
  
  
