<script>
  const $LvYibH1usScqA8OugGWk6 = $('#carousel__LvYibH1usScqA8OugGWk6');
  const $LvYibH1usScqA8OugGWk6__content = $('#carousel__LvYibH1usScqA8OugGWk6 .carousel__content');
  const $LvYibH1usScqA8OugGWk6__navigation = $('#carousel__LvYibH1usScqA8OugGWk6 .carousel__navigation');
  const $LvYibH1usScqA8OugGWk6__navigation__links = $('#carousel__LvYibH1usScqA8OugGWk6 .carousel__navigation > a[data-stat]');
  const $LvYibH1usScqA8OugGWk6__navigation__links__all = $('#carousel__LvYibH1usScqA8OugGWk6 .carousel__navigation > a');
  const $LvYibH1usScqA8OugGWk6__navigation__active__link = $('#carousel__LvYibH1usScqA8OugGWk6 .carousel__navigation > a.active');
  let carouselAutoPlay__LvYibH1usScqA8OugGWk6 = null;

  $(window).resize(function(){ updateCarouselHeight__LvYibH1usScqA8OugGWk6(); });
  $(window).load(function(){ updateCarouselHeight__LvYibH1usScqA8OugGWk6(); });

  $(document).ready(function(){
    $LvYibH1usScqA8OugGWk6__content.first().addClass('active');
    $LvYibH1usScqA8OugGWk6__navigation__links.first().addClass('active');
    $('[data-arrow="0"]').attr('data-arrow', $LvYibH1usScqA8OugGWk6__navigation__links.length);

    if($LvYibH1usScqA8OugGWk6.hasClass('autoplay')){
      carouselAutoPlay__LvYibH1usScqA8OugGWk6 = setInterval ( function(){ carouselRotate__LvYibH1usScqA8OugGWk6();}, 5000 );
    }

    $LvYibH1usScqA8OugGWk6__navigation__links__all.click(function(){
      if($LvYibH1usScqA8OugGWk6.hasClass('autoplay')){
        clearInterval(carouselAutoPlay__LvYibH1usScqA8OugGWk6); // Stop autoplay on click
      }
      $LvYibH1usScqA8OugGWk6__navigation__links.removeClass('active');
      $LvYibH1usScqA8OugGWk6__content.removeClass('active');

      const $totalSlides = $LvYibH1usScqA8OugGWk6__navigation__links.length;

      if( $(this).attr('data-stat') ) {
        $slideNumber = $(this).attr('data-stat');
      } else {
        $slideNumber = $(this).attr('data-arrow');
      }

      $('.carousel__navigation--LvYibH1usScqA8OugGWk6 a[data-stat="' + $slideNumber + '"]').addClass('active');
      $(".carousel__LvYibH1usScqA8OugGWk6--"+$slideNumber).addClass('active');

      arrowIncrement("#carousel__LvYibH1usScqA8OugGWk6", $slideNumber, $totalSlides);

      return false;
    });
  });

  function arrowIncrement (carouselContainer, slideCurrent, slideTotal) {
    if( slideCurrent == 1 ) {
        $(carouselContainer + ' .nav-next').attr('data-arrow', 2);
        $(carouselContainer + ' .nav-prev').attr('data-arrow', slideTotal);
      } else if ( slideCurrent == slideTotal ) {
        $(carouselContainer + ' .nav-next').attr('data-arrow', 1);
        $(carouselContainer + ' .nav-prev').attr('data-arrow', slideTotal - 1);
      } else {
        $(carouselContainer + ' .nav-next').attr('data-arrow', parseFloat(slideCurrent) + 1);
        $(carouselContainer + ' .nav-prev').attr('data-arrow', parseFloat(slideCurrent) - 1);
      }
  }

  // Auto rotate carousel
  function carouselRotate__LvYibH1usScqA8OugGWk6(){
    let _nextSlide = false;
    const $totalSlides = $LvYibH1usScqA8OugGWk6__navigation__links.length;

    $LvYibH1usScqA8OugGWk6__navigation__links.each(function(i) {
      if(_nextSlide){
        $(this).addClass('active');
        $(".carousel__LvYibH1usScqA8OugGWk6--"+ (i + 1)).addClass('active');
        _nextSlide = false;
      } else if($(this).hasClass('active')){
        _nextSlide = true;
        $(this).removeClass('active');
        $LvYibH1usScqA8OugGWk6__content.removeClass('active');
      }
    });

    if(_nextSlide){
      $LvYibH1usScqA8OugGWk6__navigation__links.eq(0).addClass("active");
    }

    let activeSlide = parseInt($('#carousel__LvYibH1usScqA8OugGWk6 .carousel__navigation > a.active').attr('data-stat'));
    $(".carousel__LvYibH1usScqA8OugGWk6--" + activeSlide).addClass("active");
    arrowIncrement("#carousel__LvYibH1usScqA8OugGWk6", activeSlide, $totalSlides);
  }

  // Set carousel children to height of the tallest child
  function updateCarouselHeight__LvYibH1usScqA8OugGWk6(){
    let maxHeight = Math.max.apply(null, $("#carousel__LvYibH1usScqA8OugGWk6 .carousel__height").map(function (){
      return $(this).outerHeight();
    }).get());

    $("#carousel__LvYibH1usScqA8OugGWk6").css('height', maxHeight);
  }
</script>

      
    </div>
    
  </div>
</section>

  

  
    
