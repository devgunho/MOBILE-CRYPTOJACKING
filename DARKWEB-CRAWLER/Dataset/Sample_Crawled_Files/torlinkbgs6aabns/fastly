<script>
  const $4kH3RVHPhes8OEieCOmmCK = $('#carousel__4kH3RVHPhes8OEieCOmmCK');
  const $4kH3RVHPhes8OEieCOmmCK__content = $('#carousel__4kH3RVHPhes8OEieCOmmCK .carousel__content');
  const $4kH3RVHPhes8OEieCOmmCK__navigation = $('#carousel__4kH3RVHPhes8OEieCOmmCK .carousel__navigation');
  const $4kH3RVHPhes8OEieCOmmCK__navigation__links = $('#carousel__4kH3RVHPhes8OEieCOmmCK .carousel__navigation > a[data-stat]');
  const $4kH3RVHPhes8OEieCOmmCK__navigation__links__all = $('#carousel__4kH3RVHPhes8OEieCOmmCK .carousel__navigation > a');
  const $4kH3RVHPhes8OEieCOmmCK__navigation__active__link = $('#carousel__4kH3RVHPhes8OEieCOmmCK .carousel__navigation > a.active');
  let carouselAutoPlay__4kH3RVHPhes8OEieCOmmCK = null;

  $(window).resize(function(){ updateCarouselHeight__4kH3RVHPhes8OEieCOmmCK(); });
  $(window).load(function(){ updateCarouselHeight__4kH3RVHPhes8OEieCOmmCK(); });

  $(document).ready(function(){
    $4kH3RVHPhes8OEieCOmmCK__content.first().addClass('active');
    $4kH3RVHPhes8OEieCOmmCK__navigation__links.first().addClass('active');
    $('[data-arrow="0"]').attr('data-arrow', $4kH3RVHPhes8OEieCOmmCK__navigation__links.length);

    if($4kH3RVHPhes8OEieCOmmCK.hasClass('autoplay')){
      carouselAutoPlay__4kH3RVHPhes8OEieCOmmCK = setInterval ( function(){ carouselRotate__4kH3RVHPhes8OEieCOmmCK();}, 5000 );
    }

    $4kH3RVHPhes8OEieCOmmCK__navigation__links__all.click(function(){
      if($4kH3RVHPhes8OEieCOmmCK.hasClass('autoplay')){
        clearInterval(carouselAutoPlay__4kH3RVHPhes8OEieCOmmCK); // Stop autoplay on click
      }
      $4kH3RVHPhes8OEieCOmmCK__navigation__links.removeClass('active');
      $4kH3RVHPhes8OEieCOmmCK__content.removeClass('active');

      const $totalSlides = $4kH3RVHPhes8OEieCOmmCK__navigation__links.length;

      if( $(this).attr('data-stat') ) {
        $slideNumber = $(this).attr('data-stat');
      } else {
        $slideNumber = $(this).attr('data-arrow');
      }

      $('.carousel__navigation--4kH3RVHPhes8OEieCOmmCK a[data-stat="' + $slideNumber + '"]').addClass('active');
      $(".carousel__4kH3RVHPhes8OEieCOmmCK--"+$slideNumber).addClass('active');

      arrowIncrement("#carousel__4kH3RVHPhes8OEieCOmmCK", $slideNumber, $totalSlides);

      return false;
    });
  });

  function arrowIncrement (carouselContainer, slideCurrent, slideTotal) {
    if( slideCurrent == 1 ) {
        $(carouselContainer + ' .nav-next').attr('data-arrow', 2);
        $(carouselContainer + ' .nav-prev').attr('data-arrow', slideTotal);
      } else if ( slideCurrent == slideTotal ) {
        $(carouselContainer + ' .nav-next').attr('data-arrow', 1);
        $(carouselContainer + ' .nav-prev').attr('data-arrow', slideTotal - 1);
      } else {
        $(carouselContainer + ' .nav-next').attr('data-arrow', parseFloat(slideCurrent) + 1);
        $(carouselContainer + ' .nav-prev').attr('data-arrow', parseFloat(slideCurrent) - 1);
      }
  }

  // Auto rotate carousel
  function carouselRotate__4kH3RVHPhes8OEieCOmmCK(){
    let _nextSlide = false;
    const $totalSlides = $4kH3RVHPhes8OEieCOmmCK__navigation__links.length;

    $4kH3RVHPhes8OEieCOmmCK__navigation__links.each(function(i) {
      if(_nextSlide){
        $(this).addClass('active');
        $(".carousel__4kH3RVHPhes8OEieCOmmCK--"+ (i + 1)).addClass('active');
        _nextSlide = false;
      } else if($(this).hasClass('active')){
        _nextSlide = true;
        $(this).removeClass('active');
        $4kH3RVHPhes8OEieCOmmCK__content.removeClass('active');
      }
    });

    if(_nextSlide){
      $4kH3RVHPhes8OEieCOmmCK__navigation__links.eq(0).addClass("active");
    }

    let activeSlide = parseInt($('#carousel__4kH3RVHPhes8OEieCOmmCK .carousel__navigation > a.active').attr('data-stat'));
    $(".carousel__4kH3RVHPhes8OEieCOmmCK--" + activeSlide).addClass("active");
    arrowIncrement("#carousel__4kH3RVHPhes8OEieCOmmCK", activeSlide, $totalSlides);
  }

  // Set carousel children to height of the tallest child
  function updateCarouselHeight__4kH3RVHPhes8OEieCOmmCK(){
    let maxHeight = Math.max.apply(null, $("#carousel__4kH3RVHPhes8OEieCOmmCK .carousel__height").map(function (){
      return $(this).outerHeight();
    }).get());

    $("#carousel__4kH3RVHPhes8OEieCOmmCK").css('height', maxHeight);
  }
</script>

      
    </div>
    
  </div>
