    var dirty = false;
    var isPostback = false;

    function setDirty(value) {
        dirty = value;
        var hDirty = document.getElementById("Dirty");
        if (hDirty != null) {
            hDirty.value = value.toString();
        }
    }

    function saveOnUnload() {
        if (dirty && !isPostback) {
            dirty = false; // avoid twice confirm boxes
            var hSkipSave = document.getElementById("SkipSave");
            var skipSave = false;
            try {
                skipSave = (hSkipSave.value == true.toString);
            }
            catch (e) {
            }
            if (!skipSave) {
                //var objconfirm = confirm("Ãnderungen speichern?");
                //if (objconfirm == true) {
                try {
                    initiatePostback();
                }
                catch (e) {
                }
                //}
            }
        }
        isPostback = false;
    }
    function initiatePostback() {
        document.getElementById("form1").submit();
    }
