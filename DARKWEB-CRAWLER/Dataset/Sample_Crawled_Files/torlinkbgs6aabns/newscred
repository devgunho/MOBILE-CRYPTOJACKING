<script type="text/javascript">
    jQuery(document).ready(function ($) {

        if($('body').hasClass('page-id-1831')){
            pricingHeaderHandler();
        }

        $(document).scroll( function () {
            if($('body').hasClass('page-id-1831')){
                pricingHeaderHandler();
            }
        });

        function pricingHeaderHandler() {

            var hasCookie = $('.cookie-popup').css('display') == 'none' ? false : true;

            var screen = window.innerWidth
                || document.documentElement.clientWidth
                || document.body.clientWidth;

            if(screen > 980){
                //nodes
                var th1 = $('.ft-title'),
                    th2 = $('.heading-row th:nth-child(2)'),
                    th3 = $('.heading-row th:nth-child(3)');

                th2.find('img').hide();
                th2.find('h3').hide();
                th2.find('p').hide();
                th3.find('img').hide();
                th3.find('h3').hide();
                th3.find('p').hide();

                th2.css({
                    'box-shadow': '0 4px 2px -2px rgba(0,0,0,.15)'
                });
                th3.css({
                    'box-shadow': '0 4px 2px -2px rgba(0,0,0,.15)'
                });

                $('.pricing-table .heading-row th:nth-child(2)').css({
                    'background-color':'rgba(255,255,255, .9)',
                });
                $('.pricing-table .feature-row td:nth-child(2)').css({
                    'background-color':'rgba(255,255,255, .9)',
                });
                $('.pricing-table .heading-row th:nth-child(3)').css({
                    'background-color':'rgba(255,255,255, .9)',
                });
                $('.pricing-table .feature-row td:nth-child(3)').css({
                    'background-color':'rgba(255,255,255, .9)',
                });

                var scroll = $(window).scrollTop();

                if (scroll > 490 && scroll < 2001) {
                    th2.css({
                        'opacity': 1,
                    });
                    th3.css({
                        'opacity': 1,
                    });

                    th1.css({
                        'opacity': 0,
                        'transition': 'all .s',
                    });

                    th2.css({
                        'position': 'fixed',
                        'max-width': '360px',
                        'top': hasCookie ? '97px' : '57px',
                    });

                    th3.css({
                        'position': 'fixed',
                        'max-width': '359px',
                        'margin-left': '361px',
                        'top': hasCookie ? '96px' : '56px',
                    });
                } else if(scroll > 2000) {
                    th2.css({
                        'opacity': 0,
                        'transition': 'all .1s',
                    });
                    th3.css({
                        'opacity': 0,
                        'transition': 'all .1s',
                    });
                } else{
                    th1.attr('style', '');
                    th2.attr('style', '');
                    th3.attr('style', '');

                    th2.find('img').show();
                    th2.find('h3').show();
                    th2.find('p').show();
                    th3.find('img').show();
                    th3.find('h3').show();
                    th3.find('p').show();
                    $('.pricing-table .heading-row th:nth-child(2)').css({
                        'background-color':'#fff',
                    });
                    $('.pricing-table .feature-row td:nth-child(2)').css({
                        'background-color':'#fff',
                    });
                    $('.pricing-table .heading-row th:nth-child(3)').css({
                        'background-color':'#fff',
                    });
                    $('.pricing-table .feature-row td:nth-child(3)').css({
                        'background-color':'#fff',
                    });

                }
            }

        }
    });
</script>
