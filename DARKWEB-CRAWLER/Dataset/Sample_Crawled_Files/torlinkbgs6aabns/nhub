<script>
(function (H) {
    H.wrap(H.Tooltip.prototype, 'hide', function () {});
}(Highcharts));
jQuery(function () {

// marker show on load (fix)
Highcharts.Series.prototype.drawPoints = function() {};

function drawTopWords() {
    var timeLineContainer = '#t-w-b-container';
    var topWordsContainer = '#t_w_container2';
    var accordion = '';
    
    jQuery(topWordsContainer).html('');
    jQuery(timeLineContainer).find('#spinner').show();

    var drawTopWordsAPIUrl = getApiUrlByOptions();
    drawTopWordsAPIUrl = drawTopWordsAPIUrl
      .replace('graph_api.php', 'top_words_api.php')
      .replace('timelineGraphAPI', 'topWordsAPI1')
      //.replace('method%255D%3Ddata', 'method%255D%3DgetData2')
      .replace('%5Bmethod%5D%3Ddata', '%5Bmethod%5D%3DgetData2');
    console.info(drawTopWordsAPIUrl);
    jQuery.getJSON(drawTopWordsAPIUrl/*decodeURIComponent('http://newshub.tagsreaper.com:8080/graph_api.php?params[method]=data&params[type]=1&params[count]=7&params[date][fromModify]=-1days')*/, function (data) {
        var response = data;
        if (response === undefined) {
          jQuery('.t-w-main-container').remove();
        }
        accordion += '<div id="t-w-accordion-container" class="accordion-container">';
        var partsNum;
        var wordsNum = 1;
        var maxWords = 7;
        var maxParts = 10;
        jQuery.each(response.data, function (word, parts) {
            partsNum = 1;
            if (wordsNum <= maxWords) {
                accordion += '<div class="t-w-set">';
                accordion+="<span><b>"+parts.nameFormated+"</b></span>";
                accordion += '<div class="t-w-content">';
                accordion += '<p><ul class="list-group">';
                jQuery.each(parts.fragmentsFormated, function (n, parts) {    
                    jQuery.each(parts, function (n, part) {
                      if (partsNum <= maxParts) {
                          accordion += '<li class="list-group-item"><a href="'+part[1]+'" target="_blank">'+part[0]+'</a></li>';
                          partsNum++;
                      } else {
                          return false;
                      }
                    });
                });
                accordion += '</ul></p>';
                accordion += '</div>';
                accordion += '</div>';
                wordsNum++;
            } else {
                return false;
            }
        });
        accordion += '</div>';
        jQuery(timeLineContainer).find('.spinner').hide();
        //jQuery('#t-w-container').append(accordion);
        jQuery(topWordsContainer).html(accordion);
        jQuery(".t-w-set > span").on("click", function(){
            if(jQuery(this).hasClass('active')) {
                jQuery(this).removeClass("active");
                jQuery(this).siblings('.t-w-content').slideUp(200);
                jQuery(".t-w-set > span i").removeClass("fa-minus").addClass("fa-plus");
            } else {
                jQuery(".t-w-set > span i").removeClass("fa-minus").addClass("fa-plus");
                jQuery(this).find("i").removeClass("fa-plus").addClass("fa-minus");
                jQuery(".t-w-set > span").removeClass("active");
                jQuery(this).addClass("active");
                jQuery('.t-w-content').slideUp(200);
                jQuery(this).siblings('.t-w-content').slideDown(200);
            }
        });
    });    
};

    //var chart;
    function getQueryParams(qs) {
        qs = qs.split('+').join(' ');
        var params = {},
            tokens,
            re = /[?&]?([^=]+)=([^&]*)/g;
        while (tokens = re.exec(qs)) {
            params[decodeURIComponent(tokens[1])] = decodeURIComponent(tokens[2]);
        }
        return params;
    }
