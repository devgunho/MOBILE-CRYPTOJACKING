            <script>
        var language = 'de';
        $('.counter').each(function() {
            var $this = $(this),
                countTo = $this.attr('data-count');

            $({ countNum: $this.text()}).animate({
                    countNum: countTo
                },
                {
                    duration: 2000,
                    easing:'swing',
                    step: function() {
                        $this.text(
                            parseInt(Math.floor(this.countNum)).toLocaleString(language)

                        );
                    },
                    complete: function() {
                        $this.text(
                            parseInt(this.countNum).toLocaleString(language)
                        );
                    }
                });
        });
    </script>
                                
