<!-- jQuery for triggering mobile menu -->
<script language="javascript"> 
function togglemobilemenu() {
	var ele = document.getElementById("simple-siteheadermobile-menu");
	if(ele.style.display == "block") {
		$('#simple-siteheadermobile-menu').slideToggle('fast');
  	}
	else {
		$('#simple-siteheadermobile-menu').slideToggle('fast');
	}
} 
</script>


<!-- jQuery for expanding search field in header -->
<script language="javascript"> 
function togglesearchbar() {
	$(".hiddensearchbar").css({display: "inline-block"});
	$(".hiddensearchbar-text").focus();
} 
</script>

