<script language="javascript"> 
function togglemobilemenu() {
	var ele = document.getElementById("simple-siteheadermobile-menu");
	if(ele.style.display == "block") {
		$('#simple-siteheadermobile-menu').slideToggle('fast');
  	}
	else {
		$('#simple-siteheadermobile-menu').slideToggle('fast');
	}
} 
</script>


