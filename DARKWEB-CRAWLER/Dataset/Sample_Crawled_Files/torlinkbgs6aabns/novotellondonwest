	<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" /><script src="/App_Script/Primary/bundle.name?v=_ZhTTqJPKP_a9KGYz-tz-BlR0f1X4tTIfQBKvg73_D81"></script>

	
	<script type="text/javascript">

		var pageFixes = [];
		var pageFixDefaultBreakPoint = 768;

		function registerPageFix(dynamicClass, containerClass, containerWidth) {
			pageFixes.push({
				dynamicClass: dynamicClass,
				containerClass: containerClass,
				containerWidth: parseInt(containerWidth) > 0 ? containerWidth : pageFixDefaultBreakPoint
			});
		}

		var pageFixResizeTimeout = null;

		function fix() {

			if (pageFixResizeTimeout) {
				clearTimeout(pageFixResizeTimeout);
				pageFixResizeTimeout = null;
			}

			for (var i = 0; i < pageFixes.length; i++) {

				var pageFix = pageFixes[i];
				var maxHeight = 0;
				var dynamic = $(pageFix.dynamicClass).css({ "height": "auto" });
				var containerClass = pageFix.containerClass;
				var containerWidth = pageFix.containerWidth;

				// if we have a specified container check its width
				if (containerClass && $(containerClass).outerWidth() < containerWidth) {
					continue;
				}

				// measure heights
				dynamic.each(function () {
					maxHeight = $(this).outerHeight() > maxHeight ? $(this).outerHeight() : maxHeight;
				});

				// set heights
				dynamic.css({
					"height": maxHeight + "px"
				});
			}
		}

		$(document).ready(function () {

			$(window).on("resize", function () {

				if (pageFixResizeTimeout) {
					clearTimeout(pageFixResizeTimeout);
					pageFixResizeTimeout = null;
				}

				pageFixResizeTimeout = setTimeout(function () {
					fix();
				}, 500);
			});

			registerPageFix(".offerPanels .offerPanel .content", "#Content");

			setTimeout(fix, 300);
		});

	</script>
    
	<script type="text/javascript">
		$(document).ready(function () {
		});
	</script>




