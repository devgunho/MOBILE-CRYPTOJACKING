<script type="text/javascript">

var selectedTab = 'school';


if (!selectedTab) {
    $('#mainNav li.home').addClass('active');
} else {
    $('#mainNav li').each(function (index, item) {
        var identificators = $(item).data('identificators');

        if (identificators && $.inArray(selectedTab, identificators.split(',')) !== -1) {
            $(item).addClass('active');
        }
    });
}

$(function() {
    var searchBar = $('.search-main-nav').children('input[maxlength="255"]');
    if(searchBar.attr("placeholder") != "Szukaj") {
        searchBar.attr("placeholder", "Szukaj").css('text-indent', 0);
    } else {
        setTimeout(function(){
            searchBar.attr("placeholder", "Szukaj").css('text-indent', 0);
        },1000);
    }
});

</script>

