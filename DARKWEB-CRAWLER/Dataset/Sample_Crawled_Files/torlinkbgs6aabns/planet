  <script type="text/javascript" charset="utf-8">
    $(document).ready(function()
		{  			
   		var start = 10;
   		var limit = 10;
   		var wait = false;
   		$(window).scroll( function() { 
        			//	alert("nachladen!");
   				reloadq();

   		} );  
		function reloadq() {

			
			var position        = $("#more").offset().top;  
			var scllwidth        = $(window).scrollTop() + $(window).height();
//If (scllwidth) {
//alert ("scllwidth "+scllwidth+" > position " +position);
			if (scllwidth >= position && wait == false) {
				wait = true;
				//alert (start);
				//alert ("scllwidth "+scllwidth+" > position " +position);
				last_msg_funtion();
			}
//}
		}   		 		
   		function last_msg_funtion() 
			{ 
			
			$('div#last_msg_loader').html('<img src="files/bigLoader.gif">');	
			var am = $("#alleVeranstaltungen").attr('act_main');
			
			$("#nachgeladenPositionHinten").load("./index.php?article_id=173&start=" + start + "&limit=" + limit + "&locat=Home .VeranstaltungsCluster", 
				{act_main:am},	
					function(){
						$('div#last_msg_loader').empty();
						$("#"+ start + "-" + limit).insertBefore(this);
						$("#nachgeladenPositionHinten").empty();
						//$("#"+ start + "-" + limit).getScript("./files/js/veranstaltungsmodul.js");
						$.getScript("./files/js/veranstaltungsmodul.js", function() {
   							start = start + limit;
							wait = false;
						});
						
					}
				);			
			};		
   	});
  </script>
  
