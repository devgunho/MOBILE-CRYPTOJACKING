(function ($)
{
    $(document).ready( function()
    {
        $(window).scroll( function()
        {
            if ( $(window).scrollTop() > 750 )
            {
               $("#about").css("position","fixed");
               $("#about").css("top","0px");
            }
            else if ( $(window).scrollTop() < 750 )
            {
               $("#about").css("position","relative");
            }
        });
    });
})(jQuery);
