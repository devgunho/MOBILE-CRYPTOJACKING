<script>
    $(document).ready(function () {
        $("#upper_btn_ini").click(function () {
            $("#upper_btn").slideDown(1000);
            $("#zwijacz_header").show();
        });
        $("#zwijacz_header").click(function () {
            $("#upper_btn").slideUp(1000);
            $("#zwijacz_header").hide();
        });
    });
    $(document).ready(function () {
        $("#navToggler").click(function () {
            $(".second-menu").slideToggle(1000);
        });
    });

</script>


