        <script>
            function showCloud(id) {
                let nodeListAll = document.querySelectorAll('.item.active');
                let nodeListButtonAll = document.querySelectorAll('.map button');
                let nodeList = document.querySelectorAll('.item[data-cloud="' + id + '"]');
                for (let node of nodeListAll) node.style.display = 'none';
                for (let node of nodeList) node.style.display = 'block';
                for (let node of nodeListButtonAll) node.classList.remove('active');
                document.querySelector('.map button.cloud' + id).classList.add('active');
            }
        </script>
