        <div class="container-fluid stage" id="app">
                <script>
        /**
         * IMPORTANT: This belongs in content. If you put this at the end of the body,
         * then the top navigation is only update after all other scripts are loaded,
         * resulting in a delayed background change if the user already scrolled down
         * and just reloaded the page.
         *
         * Script handles navigation background. If user scrolls down, navigation
         * transparent is removed
         **/
        let jsIsScrolledToTop = document.body.scrollTop === 0 || document.documentElement.scrollTop === 0;
        let jsElementNavbar = document.getElementById("navigation");

        window.addEventListener("scroll", checkScrollNavigation);
        checkScrollNavigation(null, true);

        function checkScrollNavigation(event, force) {
            if ((document.body.scrollTop > 0 || document.documentElement.scrollTop > 0)) {
                if (force || jsIsScrolledToTop) {
                    jsElementNavbar.classList.remove('transparent');
                    jsIsScrolledToTop = false;
                }
            } else {
                if (force || !jsIsScrolledToTop) {
                    jsElementNavbar.classList.add('transparent');
                    jsIsScrolledToTop = true;
                }
            }
        }
    </script>

