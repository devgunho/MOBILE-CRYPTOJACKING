			document.addEventListener('DOMContentLoaded', function() {
				(function($) {
					/**
					 * Function to get our cookie
					 * @param  {string} 	name 	The name of the cookie
					 * @return {boolean}      		true|false
					 */
					function getCookie(name) {
						var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
						return v ? v[2] : null;
					}

					/**
					 * Function to set our cookie
					 * @param {string} 		name  	The name of the cookie
					 * @param {integer} 	value 	The value of the cookie
					 * @param {integer} 	days  	Optional expiration timeframe of the cookie (in days)
					 */
					function setCookie(name, value, days) {
						var d = new Date;
						d.setTime(d.getTime() + 24*60*60*1000*days);

						// special handling to either set or not set a time with the cookie
						if (typeof days === 'undefined' || days == null) {
							// don't set a time
							document.cookie = name + '=' + value + ';path=/;';
						} else {
							// set a time
							document.cookie = name + '=' + value + ';path=/;expires=' + d.toGMTString();
						}
					}

					/**
					 * Function to delete our cookie
					 * @param  {string} 	name 	The name of the cookie
					 * @return void
					 */
					function deleteCookie(name) {
						setCookie(name, '', -1);
					}

					function add_on_scroll_event() {
						if( $('.gravitate-popups.hellobar').length ) {
							$(window).scroll(function () {
								if ($(window).scrollTop() > 0) {
									// if cookie doesn't exist, allow showing
									if (!getCookie('gravitate_ignore_hellobar_26477_1553790493') && !$(body).hasClass('show-hellobar')) {
										$('.gravitate-popups.hellobar').css('visibility', 'visible');
										$( 'body' ).addClass( 'show-hellobar' );

										$(window).trigger('gravitate-popups-show', true);
									}
								}
							});
						}
					}

					// primary ajax call for popups plugin
					function popupsAjaxCall(lat, lon, geo, timeout) {
						if (!timeout) {
							timeout = 0;
						}

						setTimeout(function() {
							// ajax call
							$.ajax({
								url : 'https://www.skyhighnetworks.com/wp-admin/admin-ajax.php',
								type : 'post',
								data : {
									action : 'gravitate_popups_get_popup',
									page_id : '14625',
									user_lat : lat,
									user_lon : lon,
									geo_alert : geo,
								}
							}).done(function(popup_html) {
								if (popup_html) {
									$('body').append(popup_html);
									$('.gravitate-popups.hellobar').css('visibility', 'hidden');
									add_on_scroll_event();
									if( $('.gravitate-popups.hellobar').length ) {
										if ($(window).scrollTop() >= 0) {
											// if cookie doesn't exist, allow showing
											if (!getCookie('gravitate_ignore_hellobar_26477_1553790493') && !$(body).hasClass('show-hellobar')) {
												$('.gravitate-popups.hellobar').css('visibility', 'visible');
												$( 'body' ).addClass( 'show-hellobar' );

												$(window).trigger('gravitate-popups-show', true);
											}
										}
									}
								}
							}).fail(function() {
								// ...
							});
						}, timeout);
					}

					// hellobar logic
					$(document).ready(function() {

						// get geo location
						var lat, lon, popups_position;
						var popups_have_location = false;
						var popups_location_timeout = 0 * 1000;
						var popups_geolocation_enabled = 0;

						if (popups_geolocation_enabled && typeof navigator.geolocation != 'undefined') {
							navigator.geolocation.getCurrentPosition(function(position) {
								// success
								popups_position = position;
								popups_have_location = true;

								popupsAjaxCall(popups_position.coords.latitude, popups_position.coords.longitude, true, popups_location_timeout);

							}, function(positionError) {
								// error handling
								popupsAjaxCall(0, 0, false, popups_location_timeout);
							}, {
					            enableHighAccuracy: true,
					            timeout: popups_location_timeout
							});
						} else {
							popupsAjaxCall(0, 0, false, popups_location_timeout);
						}

						// basic class wrapper at a high level for targeting
						$( 'body' ).addClass( 'gravitate-hellobar-available' );

						// handling upon closure of bar
						$(document).on('click', '.gravitate-popups.hellobar .popup-alert-bar-close', function(event) {
							// hide the bar
							$('.gravitate-popups.hellobar').css('visibility', 'hidden');
							$( 'body' ).removeClass( 'show-hellobar' );

							$(window).trigger('gravitate-popups-show', false);

							// set cookie so it doesn't show anymore
							setCookie('gravitate_ignore_hellobar_26477_1553790493', 1);
						});


					});
				})(jQuery);
			});
			</script>
		
