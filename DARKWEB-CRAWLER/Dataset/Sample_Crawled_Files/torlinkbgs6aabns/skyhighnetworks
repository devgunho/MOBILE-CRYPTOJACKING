                /**** Gartner Widget close button, set and get ignore cookie after being closed ****/
                function gartnerWidgetCloseButton() {
            		document.getElementById('gartner_widget_wrapper').style.display = 'none';

            		setGartnerCookie('ignore_gartner_widget', 1, 365);
            	}

                function setGartnerCookie(cname, cvalue, exdays) {
            		var d = new Date();
            		d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            		var expires = 'expires=' + d.toGMTString();
            		document.cookie = cname + '=' + cvalue + ';' + expires + ';path=/';
            	}

                function getGartnerCookie(cname) {
                    var name = cname + '=';
                    var decodedCookie = decodeURIComponent(document.cookie);
                    var ca = decodedCookie.split(';');
                    for(var i = 0; i < ca.length; i++) {
                        var c = ca[i];
                        while (c.charAt(0) == ' ') {
                            c = c.substring(1);
                        }
                        if (c.indexOf(name) == 0) {
                            return c.substring(name.length, c.length);
                        }
                    }
                    return '';
                }
