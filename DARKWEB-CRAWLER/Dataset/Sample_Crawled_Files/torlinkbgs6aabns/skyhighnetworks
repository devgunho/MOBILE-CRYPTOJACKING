                    window.addEventListener('DOMContentLoaded', function() {
                        //if widget doesn't exist or cookie is defined return as is, don't create widget
                        if((window.GartnerPI_Widget === 'undefined') || (getGartnerCookie('ignore_gartner_widget'))) {
                            return;
