                {{ activity_obj this.obj }}
                {{#if this.target.activity_name}} on {{ activity_obj this.target }}{{/if}}
                </div>
                <div class="summary">
                    {{ this.obj.activity_extras.summary }}
                </div>
            </li>
            {{/each}}
        </ul>
    
    </script>

        
