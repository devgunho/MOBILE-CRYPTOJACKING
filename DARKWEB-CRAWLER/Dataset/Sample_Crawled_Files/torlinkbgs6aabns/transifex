<div class="o-interactive-expand js-social-share">
  <a class="js-share-twitter o-link o-link--lite-onMuted u-color-twitter u-marginLeft-1x" href="#"><svg class="o-icon-lite u-box-2x"><use xlink:href="#icon-twitter"/></svg></a><a class="js-share-facebook o-link o-link--lite-onMuted u-color-facebook u-marginLeft-1x" href="#"><svg class="o-icon-lite u-box-2x"><use xlink:href="#icon-facebook"/></svg></a><a class="js-share-linkedin o-link o-link--lite-onMuted u-color-linkedin u-marginLeft-1x" href="#" ><svg class="o-icon-lite u-box-2x"><use xlink:href="#icon-linkedin"/></svg></a><a class="js-share-google-plus js-secondary o-link o-link--lite-onMuted u-color-googleplus u-marginLeft-1x o-interactive-expand__item" href="#"><svg class="o-icon-lite u-box-2x"><use xlink:href="#icon-google"/></svg></a><a id="txwidget-btn" class="js-secondary o-link o-link--lite-onMuted u-color-brand u-marginLeft-1x tipsy_enable o-interactive-expand__item"
    href="/nkinkade/CC/widgets/"
    original-title="Add a translations widget in your website"><svg class="o-icon-lite u-box-2x"><use xlink:href="#icon-add-outline"/></svg></a><span class="o-link u-color-secondary u-marginLeft-1x js-expand"><svg class="u-box-2x o-icon-lite"><use xlink:href="#icon-menu"/></svg></span>
</div>
<script type="text/javascript">
  $(document).ready(function(){
    var $share = $('.js-social-share'),
        shareLink = 'https://www.transifex.com/nkinkade/CC/',
        shareTitle= 'Check out the Creative Commons translation project on @transifex:';

    $share.find('.js-share-facebook').click(function() {
      window.open('//www.facebook.com/share.php?p[url]=' + encodeURIComponent(shareLink), 'Facebook','menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
      return false;
    });

    $share.find('.js-share-twitter').click(function() {
      window.open('https://twitter.com/intent/tweet?original_referer=' + encodeURIComponent(shareLink) + '&text=' + encodeURIComponent(shareTitle) + '%20' + encodeURIComponent(shareLink)+ '&hashtags=' + encodeURIComponent('I10n'),'Twitter','menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
      return false;
    });

    $share.find('.js-share-google-plus').click(function() {
      window.open('//plus.google.com/share?url=' + encodeURIComponent(shareLink),'GooglePlus','menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
      return false;
    });

    $share.find('.js-share-linkedin').click(function() {
      window.open('//www.linkedin.com/shareArticle?mini=true&url=' + encodeURIComponent(shareLink) + '&title=' + encodeURIComponent(shareTitle) + '&source=' + encodeURIComponent(shareLink),'LinkedIn','menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
      return false;
    });

    $share.find('.js-expand').on('mouseenter', function(){
      $share.addClass('is-focus');
    });

    $share.on('mouseleave', function(){
      $share.removeClass('is-focus');
    });

    $share.find('.js-secondary').tipsy({ 'gravity': 'se' });
  });
</script>


  </div>
</div>

