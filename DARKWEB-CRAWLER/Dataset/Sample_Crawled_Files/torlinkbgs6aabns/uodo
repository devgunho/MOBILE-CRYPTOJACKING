<script type="text/javascript">
jQuery(document).ready(function(){

    $('#image-map area').click(function () {

        var element = $(this).prop("hash").substring(1);

            $("#circle").fadeOut(900,function(){
                $('#'+element).fadeIn(1000);

            });

            $('.show-circle, .pizza-fragment').click(function () {

                var element = $(this).data('section');

                $('#'+element).fadeOut(800,function(){
                    $('#circle').fadeIn(400);
                });
            })
        }
    );

    $('map').imageMapResize();
});
</script>
