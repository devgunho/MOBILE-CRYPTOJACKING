  <div class="u--is-hidden" data-url="/myvienna/@@current-user.json" data-user="null" id="myvienna-user"></div>


  
  <a id="myvienna-flyout-label" href="https://www.wien.info/myvienna" class="icon icon--suitcase-header-white myvienna_flyout-label" data-flyout-label data-default-label="myVienna" data-loadmodule='["flyout", "myvienna-user"]' data-flyout-module="myvienna" ><span data-myvienna-username-logged-out>myVienna</span> <span data-myvienna-username-logged-in class="u--is-hidden"></span> <span class="icon icon--list-down-white"></span></a>

  <!-- this logic needs to be inline to load the myvienna user instantly so the contents do not jump -->
  <script>
    try {
      var username = localStorage.getItem('myviennauser')

      if (username) {
        var loggedInElement = document.querySelector('[data-myvienna-username-logged-in]')
        var loggedOutElement = document.querySelector('[data-myvienna-username-logged-out]')

        loggedInElement.innerHTML = username

        if (loggedInElement.classList) {
          loggedInElement.classList.remove('u--is-hidden')
          loggedOutElement.classList.add('u--is-hidden')
        }
      }
    } catch(e) {

    }
  </script>

  
