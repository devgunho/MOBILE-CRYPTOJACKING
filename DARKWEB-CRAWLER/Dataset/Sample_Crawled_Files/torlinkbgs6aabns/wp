        <script>
          function replaceAction(event) {
            event.preventDefault()
            var parameters = window.location.href.split('?')[1] || ''
            var newUrl = ''
            if (parameters) {
              newUrl = "https://poczta.wp.pl/login/v1/token" + "?" + parameters
            } else {
              newUrl = "https://poczta.wp.pl/login/v1/token"
            }
            document.getElementById('loginForm').action = newUrl
            event.target.submit()
          }

          if (window.location.hash == '#bad_login') {
            document.getElementById('login-error-message').style.display = ''
            document.getElementById('login-intro').style.display = 'none'
          }
        </script>
