	function shuffle(a) {
	  var i = 0, j = 0, temp = null;
	  for (i = a.length-1; i > 0; i--) {
	    j = Math.floor(Math.random() * (i + 1));
	    temp = a[i];
	    a[i] = a[j];
	    a[j] = temp;
	  }
	}

